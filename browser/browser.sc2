	PROCEDURE shpSplitterV.MouseDown
		LPARAMETERS nButton, nShift, nXCoord, nYCoord
		LOCAL lcWindowName,lnScaleMode,lnMinCol,lnMaxCol,lnMCol1,lnMCol2,lnColumns,lnLeft,lnOldLeft
		LOCAL llBrowser,llLockScreen
		LOCAL oLeftPane,oRightPane
		
		thisform.lMouseDown=.T.
		thisform.nMouseButton=IIF(TYPE("nButton")=="N",nButton,0)
		thisform.nShift=nShift
		IF nButton#1
			RETURN
		ENDIF
		lcWindowName=thisform.Name
		lnScaleMode=thisform.ScaleMode
		lnMCol1=MCOL(lcWindowName,lnScaleMode)
		IF TYPE("lnMCol1")#"N" OR lnMCol1<=0
			RETURN
		ENDIF
		llBrowser=thisform.lBrowser
		llLockScreen=thisform.LockScreen
		DO CASE
			CASE NOT thisform.lOutlineOCX
				oLeftPane=thisform.lstClassList
				oRightPane=thisform.lstMembers
			CASE llBrowser
				oLeftPane=thisform.oleClassList
				oRightPane=thisform.oleMembers
			OTHERWISE
				oLeftPane=thisform.oleFolderList
				oRightPane=thisform.oleItems
		ENDCASE
		llVisible1=oLeftPane.Visible
		llVisible2=oRightPane.Visible
		oLeftPane.Visible=.F.
		oRightPane.Visible=.F.
		IF thisform.lWebView
			thisform.oleWebBrowser.Visible=.F.
		ENDIF
		lnLeft=this.Left
		lnOldLeft=oRightPane.Left
		lnMinCol=oLeftPane.Left+(this.Width-1)
		lnMaxCol=oRightPane.Left+oRightPane.Width-4
		lnMCol2=lnMCol1
		this.FillStyle=0
		this.ZOrder(0)
		DO WHILE MDOWN()
			DOEVENTS
			thisform.Draw
			lnMCol2=MCOL(lcWindowName,lnScaleMode)
			IF TYPE("lnMCol2")#"N" OR lnMCol2=0
				LOOP
			ENDIF
			IF lnMCol2<=lnMinCol
				lnMCol2=MCOL("",3)
				IF lnMCol2<=0
					LOOP
				ENDIF
				lnMCol2=IIF(lnMCol2>=thisform.Left+thisform.Width,lnMaxCol,lnMinCol)
			ENDIF
			lnMCol2=MIN(MAX(lnMCol2,lnMinCol),lnMaxCol)
			IF lnMCol2=lnMCol1 OR lnMCol2<0
				LOOP
			ENDIF
			this.Left=lnLeft+(lnMCol2-lnMCol1)
		ENDDO
		this.FillStyle=1
		oLeftPane.Visible=llVisible1
		oRightPane.Visible=llVisible2
		IF thisform.lWebView
			thisform.oleWebBrowser.Visible=.T.
		ENDIF
		IF lnMCol2=lnMCol1
			RETURN
		ENDIF
		IF lnMCol2<0
			lnMol2=lnMCol1
		ENDIF
		lnColumns=lnMCol2-lnMCol1+oRightPane.Left
		lnColumns=MIN(MAX(lnColumns,lnMinCol),lnMaxCol)-oRightPane.Left
		WITH thisform
			.LockScreen=.T.
			oLeftPane.Width=oLeftPane.Width+lnColumns
			.txtClassList3D.Width=oLeftPane.Width
			oRightPane.Left=oRightPane.Left+lnColumns
			oRightPane.Width=oRightPane.Width-lnColumns
			.txtMembers3D.Left=oRightPane.Left
			.txtMembers3D.Width=oRightPane.Width
			DO CASE
				CASE llBrowser
					.edtClassDesc.Width=.txtClassList3D.Width
					.edtMemberDesc.Left=.txtMembers3D.Left
					.edtMemberDesc.Width=.txtMembers3D.Width
				CASE NOT llBrowser AND .lWebBrowser
					.oleWebBrowser.Left=.txtMembers3D.Left
					.oleWebBrowser.Width=.txtMembers3D.Width
			ENDCASE
			this.Left=lnLeft+(oRightPane.Left-lnOldLeft)
			.LockScreen=llLockScreen
			.SavePreferences
		ENDWITH
		
	ENDPROC


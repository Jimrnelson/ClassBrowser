	PROCEDURE getparams
		LPARAMETERS tcCode
		LOCAL lcCode,lcParams,lcCommand,lcMemLine,lnCount,lnAtPos
		LOCAL laLines[1]
		
		IF VARTYPE(tcCode)#"C" OR EMPTY(tcCode)
			RETURN ""
		ENDIF
		lcCode=ALLTRIM(tcCode)
		lcParams=""
		FOR lnCount = 1 TO ALINES(laLines,lcCode)
			lcMemLine=ALLTRIM(STRTRAN(laLines[lnCount],CHR(9)," "))
			IF NOT EMPTY(lcParams)
				lcParams=lcParams+lcMemLine
			ELSE
				IF EMPTY(lcMemLine) OR INLIST(LEFT(lcMemLine,1),"*","&")
					LOOP
				ENDIF
				lcCommand=UPPER(LEFT(lcMemLine,4))
				IF lcCommand=="NOTE"
					LOOP
				ENDIF
				IF NOT INLIST(lcCommand,"LPAR","PARA")
					EXIT
				ENDIF
				lnAtPos=AT(" ",lcMemLine)
				IF lnAtPos=0
					EXIT
				ENDIF
				lcParams=lcParams+ALLTRIM(STRTRAN(SUBSTR(lcMemLine,lnAtPos+1)," ",""))
			ENDIF
			IF NOT RIGHT(lcParams,1)==";"
				EXIT
			ENDIF
			lcParams=ALLTRIM(LEFT(lcParams,LEN(lcParams)-1))
		ENDFOR
		lcParams=ALLTRIM(STRTRAN(lcParams,",",", "))
		RETURN lcParams
	ENDPROC

	PROCEDURE getproperty
		LPARAMETERS tcProperty
		LOCAL lcProperty,lcProperties,lcValue,lnAtPos,lnLastSelect
		LOCAL cProperty
		LOCAL cPropertyName
		LOCAL cPropertyValue
		LOCAL nLen
		LOCAL nEqualPos
		LOCAL nPos
		LOCAL nPos1
		LOCAL nPos2
		
		IF VARTYPE(tcProperty)#"C" OR EMPTY(tcProperty)
			RETURN ""
		ENDIF
		
		lcProperty=LOWER(ALLTRIM(tcProperty))
		lnLastSelect=SELECT()
		this.RefreshRecNo
		SELECT (this.cAlias)
		lcProperties=Properties
		SELECT (lnLastSelect)
		
		m.lcValue = ".F."
		
		* RMK - 05/11/2004 - added support for multi-line properties
		
		m.cProperty = m.lcProperties
		DO WHILE !EMPTY(m.cProperty)
			m.nEqualPos = AT_C(' = ', m.cProperty)
			IF m.nEqualPos > 3
				m.cPropertyName = LEFTC(m.cProperty, m.nEqualPos - 1)
				IF SUBSTR(m.cProperty, m.nEqualPos + 3, 517) == REPLICATE(CHR(1), 517)
					* we have an extended propererty
					m.nLen = VAL(SUBSTR(m.cProperty, m.nEqualPos + 3 + 517, 8))
		
					m.cPropertyValue = SUBSTR(m.cProperty, m.nEqualPos + 3 + 517 + 8, nLen)
		
					m.cProperty = SUBSTR(m.cProperty, m.nEqualPos + 3 + 518 + 8 + nLen + 1)
				ELSE
					nPos1 = AT(CHR(13), m.cProperty)
					nPos2 = AT(CHR(10), m.cProperty)
					DO CASE
					CASE nPos1 == 0 AND nPos2 == 0
						m.nPos = 0
					CASE nPos1 == 0
						m.nPos = m.nPos1 + 1
					CASE nPos2 == 0
						m.nPos = m.nPos2 + 1
					OTHERWISE
						m.nPos = MIN(m.nPos1, m.nPos2) + 1
					ENDCASE
					IF m.nPos > 0
						m.cPropertyValue = SUBSTR(m.cProperty, m.nEqualPos + 3, m.nPos - (m.nEqualPos + 2))
						m.cProperty = SUBSTR(m.cProperty, m.nPos)
					ELSE
						m.cPropertyValue = SUBSTR(m.cProperty, m.nEqualPos + 3)
						m.cProperty = ''
					ENDIF
				ENDIF
				m.cProperty = LTRIM(LTRIM(m.cProperty, 0, CHR(13)), 0, CHR(10))
				
				IF LOWER(ALLTRIM(m.cPropertyName)) == m.lcProperty
					m.lcValue = IIF(LEN(m.cPropertyValue) = 0, "[None]", m.cPropertyValue)
					EXIT
				ENDIF
		
				* m.cProperty = GETWORDNUM(m.lcProperties, 1, CHR(10))
		
			ELSE
				m.cProperty = ''
			ENDIF
		ENDDO
		
		RETURN lcValue
		
	ENDPROC


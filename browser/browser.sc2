	PROCEDURE autorefresh
		LOCAL lcClass,lnRecNo,lnRecCount,lnListIndex,lnCount,lnFileNo,lcAlias
		LOCAL laInstances,lnInstances,llRefresh,lnLastSelect
		
		IF NOT this.lBrowser OR this.lBusyState OR NOT this.lInitialized OR ;
				this.nClassListIndex<0
			RETURN .F.
		ENDIF
		lnLastSelect=SELECT()
		IF USED(this.cAlias)
			IF this.nTimeStamp>0 OR THIS.lPRGMode
				this.RefreshRecNo
				SELECT (this.cAlias)
				llRefresh=(TYPE("Timestamp")=="N" AND TimeStamp#this.nTimeStamp) OR THIS.lPRGMode
				IF NOT llRefresh AND NOT EMPTY(this.cClass) AND ;
						NOT this.lFileMode
					DIMENSION laInstances[1]
					lnInstances=AINSTANCE(laInstances,this.cClass)
					llRefresh=(lnInstances#this.nInstances)
					IF NOT llRefresh AND lnInstances>0 AND this.nInstances>0
						FOR lnCount = 1 TO lnInstances
							IF NOT laInstances[lnCount]==this.aInstances[lnCount]
								llRefresh=.T.
								EXIT
							ENDIF
						ENDFOR
					ENDIF
					RELEASE laInstances
				ENDIF
				IF llRefresh
					SELECT 0
					this.RefreshMembers
					IF NOT this.lOutlineOCX
						IF (this.lstClassList.ListIndex-1)#this.lstClassList.ListIndex
							this.lstClassList.TopIndex=this.lstClassList.ListIndex
						ENDIF
					ENDIF
				ENDIF
			ENDIF
			SELECT (this.cAlias)
			IF this.lSCXMode
				LOCATE FOR LOWER(LEFT(BaseClass,4))=="form"
			ELSE
				lnListIndex=this.nClassListIndex+1
				lcClass=this.aClassList[lnListIndex,1]
				lnRecNo=this.aClassList[lnListIndex,2]
				IF "."$lcClass
					LOCATE
				ELSE
					LOCATE FOR RECNO()=lnRecNo AND LOWER(Reserved1)=="class" AND ;
							LOWER(ALLTRIM(ObjName))==lcClass
				ENDIF
			ENDIF
		ENDIF
		lnRecCount=0
		FOR lnFileNo = 1 TO this.nFileCount
			IF NOT EMPTY(this.aFiles[lnFileNo])
				lcAlias="metadata"+ALLTRIM(STR(lnFileNo))
				IF NOT USED(lcAlias)
					lnRecCount=lnRecCount+1
					LOOP
				ENDIF
				SELECT (lcAlias)
				COUNT TO lnCount
				lnRecCount=lnRecCount+lnCount
			ENDIF
		ENDFOR
		SELECT 0
		IF lnRecCount#this.nRecCount
			this.RefreshBrowser
		ENDIF
		SELECT (lnLastSelect)
		
	ENDPROC

	PROCEDURE bintoint
		LPARAMETERS tcBinary
		LOCAL lcInteger,lnInteger,lnCount,lnStrLen
		
		IF EMPTY(tcBinary)
			RETURN "0"
		ENDIF
		lnStrLen=LEN(tcBinary)
		lnInteger=0
		FOR lnCount = 0 TO (lnStrLen-1)
			IF SUBSTR(tcBinary,lnStrLen-lnCount,1)=="1"
				lnInteger=lnInteger+2^lnCount
			ENDIF
		ENDFOR
		RETURN INT(lnInteger)
		
	ENDPROC


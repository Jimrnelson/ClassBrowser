	PROCEDURE oleFolderList.NodeClick
		*** OLE Control Event ***
		LPARAMETERS node
		LOCAL lnIndex,lnListIndex
		
		IF thisform.lRelease
			RETURN
		ENDIF
		ACTIVATE SCREEN
		lnIndex=Node.Index
		thisform.nLastFolderListIndex=thisform.nFolderListIndex
		IF thisform.nMouseButton=-3
			this.object.Nodes[thisform.nShift].Selected=.T.
			thisform.nMouseButton=-1
			RETURN .F.
		ENDIF
		IF thisform.lMouseDown
			this.object.Nodes[lnIndex].Selected=.T.
		ELSE
			thisform.nMouseButton=-1
		ENDIF
		lnListIndex=thisform.nFolderListIndex-1
		IF thisform.nMouseButton=-3
			this.object.Nodes[thisform.nShift].Selected=.T.
			thisform.nMouseButton=-1
			RETURN .F.
		ENDIF
		IF thisform.nMouseButton=-2
			thisform.nMouseButton=-1
			IF (lnIndex-1)#lnListIndex
				this.object.Nodes[lnListIndex+1].Selected=.T.
			ENDIF
			RETURN .F.
		ENDIF
		DO CASE
			CASE (lnIndex-1)#lnListIndex
				thisform.RefreshItemList
			CASE thisform.nItemListIndex>0
				thisform.oleItems.object.ListItems.Item[thisform.nItemListIndex].Selected=.F.
				thisform.nItemListIndex=-1
				thisform.RefreshItem
		ENDCASE

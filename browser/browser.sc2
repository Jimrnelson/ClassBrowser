	PROCEDURE getwebicon
		LPARAMETERS tcFileName
		LOCAL lcFileName,lcFileExt
		
		IF EMPTY(tcFileName)
			RETURN "folder.ico"
		ENDIF
		lcFileName=LOWER(ALLTRIM(tcFileName))
		lcFileExt=JUSTEXT(lcFileName)
		DO CASE
			CASE LEFT(lcFileName,5)=="file:"
				RETURN "web_file.ico"
			CASE LEFT(lcFileName,5)=="http:" OR LEFT(lcFileName,4)=="www."
				RETURN "web_site.ico"
			CASE EMPTY(lcFileExt)
				RETURN "explorer.ico"
			CASE NOT lcFileExt==CHRTRAN(lcFileExt,"!@#%^&*()+=,./\?","")
				RETURN "web_site.ico"
			CASE INLIST(lcFileExt+" ","htm ","html ","asp ")
				RETURN "web_file.ico"
		ENDCASE
		RETURN "web_doc.ico"
		
	ENDPROC


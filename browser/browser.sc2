		IF ISNULL(oItem) OR ISNULL(oItem.oAction) OR ;
				NOT oItem.oAction.OLEDragDrop(this,@data,@effect,@button,@shift,@x,@y)
			effect=0
			NODEFAULT
			RETURN .F.
		ENDIF
		
	ENDPROC


	PROCEDURE redefineclass
		LPARAMETERS tcAsClass,tcClassLoc
		LOCAL lcAsClass,lcClassLoc,lcClass,lcParentClass,lcBaseClass,lcClassLibrary
		LOCAL oRedefineForm,lnRecNo,lcFilter,lnAtPos,lcAlias,llPrompt
		
		IF NOT this.lBrowser OR NOT this.lVCXSCXMode OR this.lFileMode
			RETURN .F.
		ENDIF
		lcAlias="tempdata0"
		lcClass=this.cClass
		lcParentClass=this.cParentClass
		lcClassLibrary=this.cClassLibrary
		lcBaseClass=this.cBaseClass
		lcClassLoc=IIF(TYPE("tcClassLoc")#"C" OR EMPTY(tcClassLoc),"",tcClassLoc)
		llPrompt=(TYPE("tcAsClass")#"C")
		IF NOT llPrompt
			lcAsClass=LOWER(ALLTRIM(tcAsClass))
			lnAtPos=AT(",",lcAsClass)
			IF lnAtPos>0
				lcClassLoc=LOWER(ALLTRIM(LEFT(lcAsClass,lnAtPos-1)))
				lcAsClass=LOWER(ALLTRIM(SUBSTR(lcAsClass,lnAtPos+1)))
			ENDIF
		ELSE
			lcAsClass=""
			lcClassLoc=""
			this.lModalDialog=.T.
			DO FORM brwredef NAME oRedefineForm WITH (this),(lcClass),(lcParentClass),(lcClassLibrary),(lcBaseClass) ;
					TO lcAsClass
		
			SET DATASESSION TO this.DataSessionID
			lnAtPos=AT(",",lcAsClass)
			IF lnAtPos=0
				lcClassLoc=""
			ELSE
				lcClassLoc=LOWER(ALLTRIM(SUBSTR(lcAsClass,lnAtPos+1)))
				lcAsClass=LOWER(ALLTRIM(LEFT(lcAsClass,lnAtPos-1)))
			ENDIF
		ENDIF
		IF ISNULL(lcAsClass) OR EMPTY(lcAsClass)
			RETURN .F.
		ENDIF
		SET MESSAGE TO M_REDEFINING_CLASS_LOC+" ..."
		lcParentClass=LOWER(ALLTRIM(IIF(EMPTY(lcAsClass),lcBaseClass,lcAsClass)))
		this.RefreshRecNo
		SELECT (this.cAlias)
		IF EMPTY(lcClassLoc)
			lcBaseClass=lcParentClass
		ELSE
			lnRecNo=RECNO()
			lcClassLoc=LOWER(ALLTRIM(lcClassLoc))
			IF EMPTY(JUSTEXT(lcClassLoc))
				lcClassLoc=FORCEEXT(lcClassLoc,"vcx")
			ENDIF
			lcClassLoc=LOWER(FULLPATH(lcClassLoc))
			IF NOT FILE(lcClassLoc)
				lcClassLoc=LOWER(FORCEPATH(lcClassLoc,this.cFileName))
				IF NOT FILE(lcClassLoc)
					SELECT 0
					SET MESSAGE TO
					this.MsgBox(M_FILE_LOC+[ "]+lcClassLoc+[" ]+M_DOES_NOT_EXIST_LOC+[.],16)
					RETURN .F.
				ENDIF
			ENDIF
			this.RefreshRecNo
			SELECT (this.cAlias)
			IF LOWER(ALLTRIM(ObjName))==LOWER(ALLTRIM(lcParentClass))
				SELECT 0
				SET MESSAGE TO
				this.MsgBox(M_CLASS_LOC+[ (]+lcParentClass+[) ]+M_OF_LOC+[ "]+lcClassLoc+ ;
						[" ]+M_HAS_IDENTICAL_CLASS_LOC+[.],16)
				RETURN .F.
			ENDIF
			lcFilter=FILTER()
			IF USED(lcAlias)
				USE IN (lcAlias)
			ENDIF
			SELECT 0
			USE (lcClassLoc) AGAIN SHARED ALIAS (lcAlias)
			IF NOT USED()
				lcClassLoc=LOWER(FULLPATH(lcClassLoc))
				SELECT 0
				SET MESSAGE TO
				this.MsgBox(M_UNABLE_TO_OPEN_LOC+[ "]+lcClassLoc+[".],16)
				RETURN .F.
			ENDIF
			CURSORSETPROP("Buffering",1)
			SET FILTER TO &lcFilter
			LOCATE FOR LOWER(ObjName)==lcParentClass
			IF EOF()
				IF USED(lcAlias)
					USE IN (lcAlias)
				ENDIF
				SELECT 0
				SET MESSAGE TO
				this.MsgBox(M_CLASS_LOC+[ (]+lcParentClass+[) ]+M_OF_LOC+ ;
						[ "]+lcClassLoc+[" ]+M_DOES_NOT_EXIST_LOC+[.],16)
				RETURN .F.
			ENDIF
			lcBaseClass=LOWER(ALLTRIM(BaseClass))
			IF USED(lcAlias)
				USE IN (lcAlias)
			ENDIF
			SELECT (this.cAlias)
			GO lnRecNo
		ENDIF
		IF NOT EMPTY(lcClassLoc)
			lcClassLoc=LOWER(SYS(2014,lcClassLoc,this.cFileName))
		ENDIF
		IF LOWER(Class)==lcParentClass AND LOWER(ClassLoc)==lcClassLoc AND LOWER(BaseClass)==lcBaseClass
			SELECT 0
			SET MESSAGE TO
			RETURN
		ENDIF
		this.ClearClass
		IF NOT LOWER(BaseClass)==lcBaseClass AND llPrompt AND ;
				this.MsgBox(M_CLASS_LOC+[ (]+IIF(EMPTY(lcClassLoc),this.ProperBaseClass(lcParentClass), ;
						lcParentClass)+[) ]+IIF(EMPTY(lcClassLoc),"",M_OF_LOC+ ;
						[ "]+JUSTFNAME(lcClassLoc)+[" ])+M_HAS_DIFFERENT_CLASS_LOC+[.]+CR+CR+ ;
						M_WARNING_REDEFINE_LOC+CR+CR+M_CHANGE_LOC+ ;
						[ ]+this.ProperBaseClass(BaseClass)+[ ]+M_TO_LOC+[ ]+ ;
						this.ProperBaseClass(lcBaseClass)+"?",292)#6
			SELECT 0
			SET MESSAGE TO
			RETURN .F.
		ENDIF
		REPLACE BaseClass WITH lcBaseClass, ;
				Class WITH lcParentClass, ;
				ClassLoc WITH lcClassLoc
		this.cClass=lcClass
		this.RefreshBrowser
		SELECT 0
		SET MESSAGE TO
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		
	ENDPROC


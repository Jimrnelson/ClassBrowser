	PROCEDURE indenttext
		LPARAMETERS tcCode,tnTimes
		LOCAL lnTimes,lcCode,lcCode2,lnCount,lcStrData,lnStrLen
		
		IF EMPTY(tcCode)
			RETURN ""
		ENDIF
		lnTimes=IIF(TYPE("tnTimes")=="N",tnTimes,0)
		IF MEMLINE(tcCode)<=1
			IF LEFT(ALLTRIM(tcCode),1)=="*" AND AT(" ",tcCode)>8 
				lcCode=""
				lcStrData=tcCode
				DO WHILE NOT EMPTY(lcStrData)
					lnStrLen=50-4*(lnTimes+2)
					DO WHILE .T.
						IF lnStrLen=1 OR EMPTY(SUBSTR(lcStrData,lnStrLen,1))
							EXIT
						ENDIF
						lnStrLen=lnStrLen-1
					ENDDO
					lcCode=lcCode+IIF(EMPTY(lcCode),"",MARKER+"*   ")+ ;
							ALLTRIM(LEFT(lcStrData,lnStrLen))
					lcStrData=ALLTRIM(SUBSTR(lcStrData,lnStrLen+1))
				ENDDO
			ELSE
				lcCode=tcCode
			ENDIF
			IF lnTimes=0
				RETURN lcCode
			ENDIF
		ENDIF
		lcCode=TAB+STRTRAN(tcCode,CR_LF,CR_LF+REPLICATE(TAB,lnTimes))
		DO WHILE RIGHT(lcCode,1)==TAB
			lcCode=LEFT(lcCode,LEN(lcCode)-1)
		ENDDO
		lcStrData=TAB+CR_LF
		DO WHILE lcStrData$lcCode
			lcCode=STRTRAN(lcCode,lcStrData,CR_LF)
		ENDDO
		RETURN lcCode
		
	ENDPROC

	PROCEDURE Init
		LPARAMETERS tcFileName,tcDefaultClass,tlListBox,tcClassType,tnWindowState,tlGallery
		LOCAL lcFileName,lcFileName2,lcClass,lcDefaultClass,llListBox,lcFilter,laSelObj
		LOCAL lcName,lcNameExt,lcBrowserDBF,lcBrowserFPT,lcBrowser2DBF,lcBrowser2FPT
		LOCAL lnCount,lnCount2,lcType,lcFileNameNoPath,lnImageCount,oForm,llIngnoreErrors
		LOCAL lcVersion,lnAtPos,lcClass,llCatalog,llAutoDefault
		LOCAL laSelObj[1]
		
		DO CASE
			CASE _windows
				this.cPlatform="WINDOWS"
			CASE _mac
				this.cPlatform="MAC"
			CASE _dos
				this.cPlatform="DOS"
			CASE _unix
				this.cPlatform="UNIX"
			OTHERWISE
				this.MsgBox(M_CRRNT_PLAT_NOT_SPRTD_LOC+".",16)
				this.QueryUnload
				this.Destroy
				RETURN .F.
		ENDCASE
		IF TYPE("this.oleClassList.BaseClass")=="C"
			this.lOutlineOCX=.T.
		ENDIF
		lcDefaultClass=this.tcDefaultClass
		IF NOT this.lOutlineOCX OR TYPE("this.tlListBox")#"L"
			llListBox=.T.
		ELSE
			llListBox=this.tlListBox
		ENDIF
		this.tcClassType=tcClassType
		this.tnWindowState=tnWindowState
		RELEASE tcDefaultClass,tlListBox,tcClassType,tnWindowState,tlGallery
		this.lBrowser=(NOT tlGallery OR llListBox)
		this.nClassLibraryCount=0
		DIMENSION this.aClassLibrary[1,2]
		this.aClassLibrary=""
		IF NOT this.lBrowser
			this.Caption=M_COMPONENT_GALLERY_LOC
		ENDIF
		IF this.lOutlineOCX
			IF TYPE("this.oleImageList.object")#"O"
				this.MsgBox(M_WINDOWS_RECOURCES_LOW_LOC,16)
				this.QueryUnload
				this.Destroy
				RETURN .F.
			ENDIF
			lnImageCount=this.oleImageList.object.ListImages.Count
			DIMENSION this.aImageList[lnImageCount]
			FOR lnCount = 1 TO lnImageCount
				this.aImageList[lnCount]=LOWER(this.oleImageList.object.ListImages[lnCount].Key)+" "
			ENDFOR
			lnImageCount=this.oleSmallImages.object.ListImages.Count
			DIMENSION this.aSmallImages[lnImageCount]
			FOR lnCount = 1 TO lnImageCount
				this.aSmallImages[lnCount]=LOWER(this.oleSmallImages.object.ListImages[lnCount].Key)+" "
			ENDFOR
		ENDIF
		IF NOT EMPTY(this.cBrowserTable)
			lcBrowserDBF=this.cBrowserTable
		ELSE
		*!*	Geändert durch: SF 22.11.2020
		*!*	<pdm>
		*!*	<change date="{^2020-11-22,11:45:00}">Geändert durch: SF<br />
		*!*	Do not store browser.dbf in c:\Programs. use standard path
		*!*	</change>
		*!*	</pdm>
		
		*	lcBrowserDBF=this.cProgramName
			lcBrowserDBF=FORCEPATH(THIS.cProgramName,HOME(7))
		
		*!*	/Geändert durch: SF 22.11.2020
		ENDIF
		lcBrowserDBF=FORCEEXT(lcBrowserDBF,"dbf")
		lcBrowserFPT=FORCEEXT(lcBrowserDBF,"fpt")
		this.cBrowserTable=lcBrowserDBF
		IF NOT FILE(lcBrowserDBF) OR NOT FILE(lcBrowserFPT)
			SELECT 0
			CREATE (lcBrowserDBF) FROM brwstruc
			USE
		ENDIF
		llIgnoreErrors=this.lIgnoreErrors
		this.lIgnoreErrors=.T.
		SELECT 0
		USE (lcBrowserDBF) EXCLUSIVE ALIAS browser
		IF USED()
			PACK MEMO
		ENDIF
		USE (lcBrowserDBF) AGAIN SHARED ALIAS browser
		this.lIgnoreErrors=llIngnoreErrors
		IF NOT USED()
			this.MsgBox(M_UNABLE_TO_OPEN_LOC+[ "]+lcBrowserDBF+[".],16)
			this.QueryUnload
			this.Destroy
			RETURN .F.
		ENDIF
		IF FCOUNT()<40 OR NOT UPPER(FIELD(41))=="USER4" OR TYPE("Name")#"M" OR ;
				TYPE("Platform")#"C" OR TYPE("Script")#"M" OR TYPE("Backup")#"L"
			USE
			lcBrowser2DBF=LOWER(FORCEPATH(FORCEEXT(lcBrowserDBF,"")+"__2",JUSTPATH(lcBrowserDBF)))
			lcBrowser2FPT=lcBrowser2DBF+".fpt"
			lcBrowser2DBF=lcBrowser2DBF+".dbf"
			SELECT * FROM (lcBrowserDBF) INTO TABLE (lcBrowser2DBF)
			USE
			IF USED("browser")
				USE IN browser
			ENDIF

	PROCEDURE refreshbuttons
		LOCAL llExport,llFind,llSubclass,llRename,llRedefine,llRemove,llCleanUp,llFileMode
		LOCAL llBrowser,llSCXMode,llViewButtons,llWebButtons,llUpOneLevel,lcMember,lcMemberChar
		
		WITH this
			IF .nMemberListIndex>0
				lcMember=.aMemberList[.nMemberListIndex,1]
				lcMemberChar=.aMemberList[.nMemberListIndex,2]
			ELSE
				lcMember=""
				lcMemberChar=""
			ENDIF
			llBrowser=(.lBrowser AND .nClassCount>0)
			llFileMode=(llBrowser AND .lFileMode)
			llSCXMode=(llBrowser AND .lSCXMode)
			llExport=(llBrowser AND .lVCXSCXMode) AND NOT .lPRGMode
			llFind=(llBrowser OR (NOT .lBrowser AND .nFolderCount>0))
			llSubclass=(llBrowser AND .lVCXSCXMode AND NOT llSCXMode AND NOT .lPRGMode)
			llRename=(llBrowser AND .lVCXSCXMode AND NOT llFileMode AND NOT llSCXMode AND ;
					NOT .lReadOnly AND (EMPTY(lcMember) OR INLIST(lcMemberChar,"p","m"))) AND NOT .lPRGMode
			llRedefine=(llBrowser AND .lVCXSCXMode AND NOT llFileMode AND NOT .lReadOnly AND ;
					EMPTY(lcMember)) AND NOT .lPRGMode
			llRemove=(llBrowser AND NOT .lReadOnly) AND NOT .lPRGMode
			llCleanUp=(llBrowser AND .lVCXSCXMode AND NOT llSCXMode AND NOT .lReadOnly) AND NOT .lPRGMode
			llUpOneLevel=(NOT .lBrowser AND .nFolderCount>0 AND ;
					NOT ISNULL(.oFolderSource) AND NOT .oFolderSource.lCatalog)
			IF .cmdExport.Enabled#llExport
				.cmdExport.Enabled=llExport
			ENDIF
			IF .cmdFind.Enabled#llFind
				.cmdFind.Enabled=llFind
			ENDIF
			IF .cmdSubclass.Enabled#llSubclass
				.cmdSubclass.Enabled=llSubclass
			ENDIF
			IF .cmdRename.Enabled#llRename
				.cmdRename.Enabled=llRename
			ENDIF
			IF .cmdRedefine.Enabled#llRedefine
				.cmdRedefine.Enabled=llRedefine
			ENDIF
			IF .cmdCleanUp.Enabled#llCleanUp
				.cmdCleanUp.Enabled=llCleanUp
			ENDIF
			IF .cmdUpOneLevel.Enabled#llUpOneLevel
				.cmdUpOneLevel.Enabled=llUpOneLevel
			ENDIF
			IF .lOutlineOCX
				llViewButtons=(NOT .lBrowser AND NOT .lWebView)
				llWebButtons=(NOT .lBrowser AND .lWebView)
				IF .chkLargeIconsView.Visible#llViewButtons
					.chkLargeIconsView.Visible=llViewButtons
					.chkSmallIconsView.Visible=llViewButtons
					.chkListView.Visible=llViewButtons
					.chkDetailsView.Visible=llViewButtons
				ENDIF
				IF .cmdGoBack.Visible#llWebButtons
					.cmdGoBack.Visible=llWebButtons
					.cmdGoForward.Visible=llWebButtons
					.cmdStop.Visible=llWebButtons
					.cmdRefresh.Visible=llWebButtons
				ENDIF
			ENDIF
			
		ENDWITH
		
	ENDPROC

	PROCEDURE refreshcaption
		LOCAL lvResult
		
		this.vResult=.T.
		DO brwRefreshCaption IN browser WITH (this)
		lvResult=this.vResult
		this.vResult=.T.
		RETURN lvResult
		
	ENDPROC

	PROCEDURE refreshclassicon
		LPARAMETERS tnIndex,tcClassIconBMP,tcClass,tcBaseClass,tcFileName
		LOCAL lcClassIconBMP,lcClass,lnParameters,lnImageIndex,llIgnoreErrors
		
		lnParameters=PARAMETERS()
		IF NOT this.lOutlineOCX OR lnParameters<1
			RETURN .F.
		ENDIF
		IF lnParameters<5
			tcClassIconBMP=this.aClassList[tnIndex,7]
			tcClass=this.cClass
			tcBaseClass=this.cBaseClass
			tcFileName=this.cFileName
		ENDIF
		lcClassIconBMP=LOWER(ALLTRIM(tcClassIconBMP))
		lcClass=tcClass
		this.lError=.F.
		llIgnoreErrors=this.lIgnoreErrors
		this.lIgnoreErrors=.T.
		IF RIGHT(lcClassIconBMP,1)=="\" OR RIGHT(lcClassIconBMP,1)=="."
			lcClassIconBMP=""
		ENDIF
		IF EMPTY(lcClassIconBMP)
			IF "."$lcClass
				lcClassIconBMP=LOWER(this.ClassIcon(lcClass))
			ELSE
				lcClassIconBMP=LOWER(this.ClassIcon(tcBaseClass))
			ENDIF
		ELSE
			lcClassIconBMP=LOWER(ALLTRIM(lcClassIconBMP))
			IF ASCAN(this.aImageList,lcClassIconBMP+" ")=0
				lcClassIconBMP=LOWER(FULLPATH(lcClassIconBMP,tcFileName))
				lnImageIndex=this.GetImageIndex(.T.,lcClassIconBMP)
				IF lnImageIndex>0
					lcClassIconBMP=ALLTRIM(this.aImageList[lnImageIndex])
				ELSE
					lcClassIconBMP=LOWER(this.ClassIcon(tcBaseClass))
				ENDIF
			ENDIF
		ENDIF
		IF this.lBrowser
			this.oleClassList.object.Nodes[tnIndex].Image=lcClassIconBMP
		ELSE
			this.oleFolderList.object.Nodes[tnIndex].Image=lcClassIconBMP
		ENDIF
		this.lIgnoreErrors=llIgnoreErrors
		this.lError=.F.
		
	ENDPROC

	PROCEDURE refreshclasslist
		LPARAMETERS tcDefaultClass,tlIgnoreTable
		LOCAL lcDefaultClass,llIgnoreTable,lnDefaultListIndex
		LOCAL lnClassCount,laClassList,lnClassCount2,laClassList2
		LOCAL lcObjName,lcBaseClass,llBaseClassMatch,lcClass,lcClassLoc,lcFilter
		LOCAL lnIndent,lcClassType,lcFileName,lcFileExt,llSCXMode,llVCXSCXMode
		LOCAL lnDisplayMode,llAutoExpand,lcParentClassSymbol,llParentClassSymbol
		LOCAL lnFileNo,llMatch,lnCount,lnCount2,lnAtPos,lnRecCount,lcParentKey,lcAlias
		LOCAL lnFileCount,llActive,oNode,llLockScreen,llHierarchyError,lcKey,lnRecNo
		LOCAL llPRGMode
		
		IF this.lRelease OR NOT this.lBrowser OR (this.lOutlineOCX AND ;
				TYPE("this.oleClassList")#"O") OR (NOT this.lOutlineOCX AND ;
				TYPE("this.lstClassList")#"O")
			RETURN .F.
		ENDIF
		SET DATASESSION TO this.DataSessionID
		lnFileCount=this.nFileCount
		lcFileExt=LOWER(RIGHT(this.cFileName,4))
		IF NOT USED(this.cAlias) AND (lcFileExt==".vcx" OR lcFileExt==".scx" OR lcFileExt==".prg")
			IF lnFileCount=1
				RETURN .F.
			ENDIF
			FOR lnFileNo = 1 TO lnFileCount
				IF NOT EMPTY(this.aFiles[lnFileNo])
					this.cAlias="metadata"+ALLTRIM(STR(lnFileNo))
					IF USED(this.cAlias)
						EXIT
					ENDIF
				ENDIF
			ENDFOR
		ENDIF
		SET MESSAGE TO M_REFRESH_CLASS_LIST_LOC+" ..."
		llActive=this.lActive
		this.lActive=.T.
		llLockScreen=this.LockScreen
		this.LockScreen=.F.
		lnRecCount=0
		llHierarchyError=.F.
		IF NOT this.lRefreshBrowserMode
			IF this.lOutlineOCX
				this.oleClassList.Enabled=.F.
				this.oleClassList.Visible=.F.
			ENDIF
		ENDIF
		lnDisplayMode=this.nDisplayMode
		lcParentClassSymbol=this.cParentClassSymbol
		llAutoExpand=this.lAutoExpand
		llIgnoreTable=tlIgnoreTable
		lcClassType=this.cClassType
		lcDefaultClass=IIF(EMPTY(tcDefaultClass) OR TYPE("tcDefaultClass")#"C", ;
				"",LOWER(tcDefaultClass))
		lnClassCount=this.nClassCount
		IF NOT llIgnoreTable OR lnClassCount<=0
			DIMENSION laClassList[1,9]
			laClassList=""
		ELSE
			DIMENSION laClassList[lnClassCount,9]
			=ACOPY(this.aClassList,laClassList)
			IF EMPTY(laClassList[1,1])
				llIgnoreTable=.F.
			ENDIF
		ENDIF
		IF NOT llIgnoreTable OR lnClassCount<=0
			lnCount=0
			laClassList=""
			FOR lnFileNo = 1 TO lnFileCount
				IF this.lRelease
					this.LockScreen=llLockScreen
					RETURN .F.
				ENDIF
				lcFileName=this.aFiles[lnFileNo]
				IF EMPTY(lcFileName)
					LOOP
				ENDIF
				llSCXMode=(RIGHT(lcFileName,4)==".scx")
				llPRGMode=(RIGHT(lcFileName,4)==".prg")
				llVCXSCXMode=(llSCXMode OR llPRGMode OR RIGHT(lcFileName,4)==".vcx")
				
				lcAlias="metadata"+ALLTRIM(STR(ASCAN(this.aFiles,lcFileName)))
				IF USED(lcAlias)
					SELECT (lcAlias)
					lcFilter=this.cFilter
					SET FILTER TO &lcFilter
					LOCATE
					COUNT TO lnCount2
					LOCATE
				ELSE
					lnCount2=1
				ENDIF
				lnRecCount=lnRecCount+lnCount2
				lnCount=lnCount+1
				DIMENSION laClassList[lnCount,9]
				laClassList[lnCount,1]="("+lcFileName+")"
				laClassList[lnCount,2]=1
				laClassList[lnCount,3]=1
				laClassList[lnCount,4]=""
				laClassList[lnCount,5]=""
				laClassList[lnCount,6]=lcFileName
				laClassList[lnCount,7]=IIF(llVCXSCXMode,ALLTRIM(Reserved5),"c:\dev\browser\ole.ico")
				laClassList[lnCount,8]=""
				laClassList[lnCount,9]=.F.
				DO CASE
					CASE NOT llVCXSCXMode
						LOOP
					CASE RIGHT(lcFileName,4)==".vcx" OR RIGHT(lcFilename,4)==".prg"
						SCAN ALL FOR LOWER(Reserved1)=="class" AND NOT DELETED()
							lcObjName=LOWER(ALLTRIM(ObjName))
							lcBaseClass=LOWER(ALLTRIM(BaseClass))
							llBaseClassMatch=(ATC(lcClassType+",",this.cboClassType.RowSource+",")>0) OR THIS.MatchClass(lcClassType)
							IF EMPTY(lcClassType) OR (llBaseClassMatch AND ;
									this.WildCardMatch(lcClassType,lcBaseClass)) OR ;
									(NOT llBaseClassMatch AND this.WildCardMatch(lcClassType,lcObjName))
								lnCount=lnCount+1
								DIMENSION laClassList[lnCount,9]
								laClassList[lnCount,1]=lcObjName
								laClassList[lnCount,2]=RECNO()
								laClassList[lnCount,3]=0
								IF EMPTY(ClassLoc)
									laClassList[lnCount,4]=""
								ELSE
									laClassList[lnCount,4]=LOWER(FULLPATH(ALLTRIM(ClassLoc),lcFileName))
								ENDIF
								laClassList[lnCount,5]=LOWER(ALLTRIM(Class))
								laClassList[lnCount,6]=lcFileName
								laClassList[lnCount,7]=ALLTRIM(Reserved5)
								laClassList[lnCount,8]=LOWER(ALLTRIM(BaseClass))
								laClassList[lnCount,9]=.F.
							ENDIF
							lnRecNo=RECNO()
							LOCATE FOR UPPER(ALLTRIM(Platform))=="COMMENT" AND ;
									LOWER(ALLTRIM(ObjName))==lcObjName
							IF NOT EOF() AND UPPER(ALLTRIM(MLINE(Reserved2,1)))=="OLEPUBLIC"
								laClassList[lnCount,9]=.T.
							ENDIF
							GO lnRecNo
						ENDSCAN
					CASE RIGHT(lcFileName,4)==".scx"
						LOCATE FOR LOWER(LEFT(BaseClass,4))=="form"
						IF NOT EOF()
							lcObjName=LOWER(ALLTRIM(ObjName))
							lcBaseClass=LOWER(ALLTRIM(BaseClass))
							llBaseClassMatch=(ATC(lcClassType+",",this.cboClassType.RowSource+",")>0) OR THIS.MatchClass(lcClassType)
							IF EMPTY(lcClassType) OR (llBaseClassMatch AND ;
									this.WildCardMatch(lcClassType,lcBaseClass)) OR ;
									(NOT llBaseClassMatch AND this.WildCardMatch(lcClassType,lcObjName))
								lnCount=lnCount+1
								DIMENSION laClassList[lnCount,9]
								laClassList[lnCount,1]=lcObjName
								laClassList[lnCount,2]=RECNO()
								laClassList[lnCount,3]=0
								IF EMPTY(ClassLoc)
									laClassList[lnCount,4]=""
								ELSE
									laClassList[lnCount,4]=LOWER(FULLPATH(ALLTRIM(ClassLoc),lcFileName))
								ENDIF
								laClassList[lnCount,5]=LOWER(ALLTRIM(Class))
								laClassList[lnCount,6]=lcFileName
								laClassList[lnCount,7]=ALLTRIM(Reserved5)
								laClassList[lnCount,8]=LOWER(ALLTRIM(BaseClass))
								laClassList[lnCount,9]=.F.
							ENDIF
						ENDIF
		
		
					OTHERWISE
						lnCount=lnCount+1
						DIMENSION laClassList[lnCount,9]
						laClassList[lnCount,1]=lcFileName
						laClassList[lnCount,2]=0
						laClassList[lnCount,3]=1
						laClassList[lnCount,4]=""
						laClassList[lnCount,5]=lcFileName
						laClassList[lnCount,6]=""
						laClassList[lnCount,7]=""
						laClassList[lnCount,8]=""
						laClassList[lnCount,9]=.F.
				ENDCASE
			ENDFOR
			lnClassCount=lnCount
		ENDIF
		SELECT 0
		IF lnDisplayMode=1 OR lnFileCount=1
			lnCount=lnClassCount+1
			DIMENSION laClassList[lnCount,9]
			laClassList[lnCount,1]=CHR(255)
			laClassList[lnCount,2]=0
			laClassList[lnCount,3]=1
			laClassList[lnCount,4]=""
			laClassList[lnCount,5]=""
			laClassList[lnCount,6]=""
			laClassList[lnCount,7]=""
			laClassList[lnCount,8]=""
			laClassList[lnCount,9]=.F.
			=ASORT(laClassList)
			DIMENSION laClassList[MAX(lnClassCount,1),9]
		ENDIF
		IF lnClassCount>0
			IF lnDisplayMode=1
				lnClassCount2=0
				DIMENSION laClassList2[lnClassCount,9]
				laClassList2=""
				FOR lnCount = 1 TO lnClassCount
					laClassList2[lnCount,1]=""
					laClassList2[lnCount,2]=0
					laClassList2[lnCount,3]=0
					laClassList2[lnCount,4]=""
					laClassList2[lnCount,5]=""
					laClassList2[lnCount,6]=""
					laClassList2[lnCount,8]=""
					laClassList2[lnCount,9]=.F.
				ENDFOR
				FOR lnCount = 1 TO lnClassCount
					lcClassLoc=laClassList[lnCount,4]
					IF laClassList[lnCount,2]=0 OR (NOT EMPTY(lcClassLoc) AND ;
							ASCAN(this.aFiles,lcClassLoc)>0)
						LOOP
					ENDIF
					lnClassCount2=lnClassCount2+1
					laClassList2[lnClassCount2,1]=laClassList[lnCount,1]
					laClassList2[lnClassCount2,2]=laClassList[lnCount,2]
					laClassList2[lnClassCount2,3]=1
					laClassList2[lnClassCount2,4]=lcClassLoc
					laClassList2[lnClassCount2,5]=laClassList[lnCount,5]
					laClassList2[lnClassCount2,6]=laClassList[lnCount,6]
					laClassList2[lnClassCount2,7]=laClassList[lnCount,7]
					laClassList2[lnClassCount2,8]=laClassList[lnCount,8]
					laClassList2[lnClassCount2,9]=laClassList[lnCount,9]
					laClassList[lnCount,2]=0
					lnIndent=1
					IF NOT "."$laClassList[lnCount,1]
						this.RefreshClassListSubclass(@laClassList,@lnClassCount, ;
								@laClassList2,@lnClassCount2,lnCount,lnIndent, ;
								laClassList2[lnClassCount2,6])
						lnCount=0
					ENDIF
				ENDFOR
				IF lnClassCount#lnClassCount2
					FOR lnCount = 1 TO lnClassCount
						IF laClassList[lnCount,3]>0
							LOOP
						ENDIF
						llMatch=.F.
						FOR lnCount2 = 1 TO lnClassCount2
							IF laClassList[lnCount,1]==laClassList2[lnCount2,1]
								llMatch=.T.
								EXIT
							ENDIF
						ENDFOR
						IF NOT llMatch
							lcClass=laClassList[lnCount,1]
							IF lcClass==this.cClass
								this.cClass=""
							ENDIF
							IF this.lDisplayHierarchyError AND NOT llHierarchyError
								this.MsgBox(M_UNABLE_DISP_TREE_LOC+[:]+CR+CR+ ;
										M_CLASS_LOC+[:            (]+lcClass+[) of "]+ ;
										laClassList[lnCount,6]+["]+CR+CR+ ;
										M_PARENT_CLASS_LOC+[:  (]+laClassList[lnCount,5]+[) of "]+ ;
										laClassList[lnCount,4]+["],16,T_CLASS_BROWSER_LOC)
							ENDIF
							llHierarchyError=.T.
						ENDIF
					ENDFOR
				ENDIF
				=ACOPY(laClassList2,laClassList)
			ELSE
				IF lnFileCount>1
					FOR lnCount = 1 TO lnClassCount
						IF NOT "."$laClassList[lnCount,1]
							laClassList[lnCount,1]="("+laClassList[lnCount,6]+")@"+laClassList[lnCount,1]
						ENDIF
					ENDFOR
					lnCount=lnClassCount+1
					DIMENSION laClassList[lnCount,9]
					laClassList[lnCount,1]=CHR(255)
					laClassList[lnCount,2]=0
					laClassList[lnCount,3]=1
					laClassList[lnCount,4]=""
					laClassList[lnCount,5]=""
					laClassList[lnCount,6]=""
					laClassList[lnCount,7]=""
					laClassList[lnCount,8]=""
					laClassList[lnCount,9]=.F.
					=ASORT(laClassList)
					DIMENSION laClassList[lnClassCount,9]
					FOR lnCount = 1 TO lnClassCount
						lnAtPos=AT("@",laClassList[lnCount,1])
						IF lnAtPos>0
							laClassList[lnCount,1]=SUBSTR(laClassList[lnCount,1],lnAtPos+1)
						ENDIF
					ENDFOR
				ENDIF
				lnClassCount2=lnClassCount
				FOR lnCount = 1 TO lnClassCount
					laClassList[lnCount,3]=IIF("."$laClassList[lnCount,1],1,2)
				ENDFOR
			ENDIF
			lnClassCount=lnClassCount2
		ENDIF
		lnDefaultListIndex=0
		IF NOT EMPTY(lcDefaultClass)
			lnDefaultListIndex=-1
			FOR lnCount = 1 TO lnClassCount
				IF laClassList[lnCount,1]==lcDefaultClass AND ;
						laClassList[lnCount,6]==this.cFileName
					lnDefaultListIndex=lnCount-1
					EXIT
				ENDIF
			ENDFOR
			IF lnDefaultListIndex<0
				FOR lnCount = 1 TO lnClassCount
					IF laClassList[lnCount,1]==lcDefaultClass
						lnDefaultListIndex=lnCount-1
						EXIT
					ENDIF
				ENDFOR
			ENDIF
			lnDefaultListIndex=MAX(lnDefaultListIndex,0)
		ENDIF
		IF this.lOutlineOCX
			this.ClearNodes(this.oleClassList)
		ELSE
			this.lstClassList.ListIndex=0
			this.lstClassList.Clear
		ENDIF
		FOR lnCount = 1 TO lnClassCount
			lcFileName=laClassList[lnCount,6]
			lcClass=laClassList[lnCount,1]
			lcClassLoc=laClassList[lnCount,4]
			IF NOT EMPTY(lcClassLoc) AND ASCAN(this.aFiles,lcClassLoc)=0
				llParentClassSymbol=.T.
				lcClass=lcParentClassSymbol+"  "+lcClass
			ELSE
				llParentClassSymbol=.F.
				lcClass="    "+lcClass
			ENDIF
			IF laClassList[lnCount,9]
				lcClass=STUFF(lcClass,2,2,"*")
			ENDIF
			lnIndent=laClassList[lnCount,3]
			IF this.lOutlineOCX
				IF "."$lcClass
					oNode=this.oleClassList.object.Nodes.Add(,4,lcFileName,lcClass)
				ELSE
					IF lnIndent<=lnDisplayMode OR llParentClassSymbol
						lcParentKey=lcFileName
					ELSE
						lcParentKey=lcClassLoc+"_"+laClassList[lnCount,5]
					ENDIF
					lcKey=lcFileName+"_"+laClassList[lnCount,1]
					IF lnIndent=1
						oNode=this.oleClassList.object.Nodes.Add(,4,lcKey,lcClass)
					ELSE
						oNode=this.oleClassList.object.Nodes.Add(lcParentKey,4,lcKey,lcClass,,)
						IF llAutoExpand AND NOT oNode.parent.Expanded
							oNode.parent.Expanded=.T.
						ENDIF
					ENDIF
				ENDIF
				this.RefreshClassIcon(lnCount,laClassList[lnCount,7],lcClass, ;
						laClassList[lnCount,8],lcFileName)
			ELSE
				this.lstClassList.AddItem(lcClass)
				this.lstClassList.Picture[lnCount]="oclosed.bmp"
			ENDIF
			IF "."$lcClass
				IF NOT this.lOutlineOCX
					this.lstClassList.Picture[lnCount]="leaf.bmp"
				ENDIF
				LOOP
			ENDIF
			IF lnIndent=1
				LOOP
			ENDIF
			IF NOT this.lOutlineOCX
				this.lstClassList.List[lnCount]=SPACE(2*lnIndent)+ ;
						this.lstClassList.List[lnCount]
			ENDIF
			IF lnDisplayMode=1
				IF this.lOutlineOCX
					IF llAutoExpand AND lnCount>=3 AND ;
							laClassList[lnCount-1,3]<laClassList[lnCount,3]
						this.oleClassList.object.Nodes[lnCount-1].Expanded=.T.
					ENDIF
				ELSE
					IF llAutoExpand AND lnCount>=3 AND ;
							laClassList[lnCount-1,3]<laClassList[lnCount,3]
						this.lstClassList.Picture[lnCount-1]="oopen.bmp"
					ENDIF
				ENDIF
				LOOP
			ENDIF
		ENDFOR
		IF NOT this.lRefreshBrowserMode
			IF this.lOutlineOCX
				this.oleClassList.Enabled=.T.
				this.oleClassList.Visible=.T.
			ENDIF
		ENDIF
		IF lnClassCount>0
			IF this.lOutlineOCX
				this.oleClassList.object.Nodes[lnDefaultListIndex+1].Selected=.T.
					
				* RMK 11/30/2003 - updated to ensure node is visible
				this.oleclassList.Scroll = .F.
				this.oleClassList.object.Nodes[lnDefaultListIndex+1].EnsureVisible()
				this.oleclassList.Scroll = .T.
		
				this.nClassListIndex=this.oleClassList.object.SelectedItem.Index-1
			ELSE
				this.lstClassList.ListIndex=lnDefaultListIndex+1
				this.lstClassList.TopIndex=this.lstClassList.ListIndex
				this.nClassListIndex=this.lstClassList.ListIndex-1
			ENDIF
		ENDIF
		this.nClassCount=lnClassCount
		IF lnRecCount>0
			this.nRecCount=lnRecCount
		ENDIF
		DIMENSION laClassList[MAX(lnClassCount,1),9]
		DIMENSION this.aClassList[MAX(lnClassCount,1),9]
		ACOPY(laClassList,this.aClassList)
		this.lActive=llActive
		this.LockScreen=llLockScreen
		SELECT 0
		SET MESSAGE TO
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		
	ENDPROC

	PROCEDURE refreshclasslistsubclass
		LPARAMETERS taClassList,tnClassCount,taClassList2,tnClassCount2,tnIndex,tnIndent,tcFileName
		LOCAL lnCount,llMatch
		EXTERNAL ARRAY taClassList,taClassList2
		
		llMatch=.F.
		FOR lnCount = 1 TO tnClassCount
			IF taClassList[lnCount,2]=0 OR ;
					NOT taClassList[lnCount,5]==taClassList[tnIndex,1] OR ;
					EMPTY(taClassList[lnCount,4]) OR NOT taClassList[lnCount,4]==tcFileName OR ;
					ASCAN(this.aFiles,taClassList[lnCount,4])=0
				LOOP
			ENDIF
			llMatch=.T.
			tnClassCount2=tnClassCount2+1
			taClassList2[tnClassCount2,1]=taClassList[lnCount,1]
			taClassList2[tnClassCount2,2]=taClassList[lnCount,2]
			taClassList2[tnClassCount2,3]=tnIndent+1
			taClassList2[tnClassCount2,4]=taClassList[lnCount,4]
			taClassList2[tnClassCount2,5]=taClassList[lnCount,5]
			taClassList2[tnClassCount2,6]=taClassList[lnCount,6]
			taClassList2[tnClassCount2,7]=taClassList[lnCount,7]
			taClassList2[tnClassCount2,8]=taClassList[lnCount,8]
			taClassList2[tnClassCount2,9]=taClassList[lnCount,9]
			taClassList[lnCount,2]=0
			=this.RefreshClassListSubclass(@taClassList,@tnClassCount, ;
					@taClassList2,@tnClassCount2,lnCount,tnIndent+1,taClassList2[tnClassCount2,6])
		ENDFOR
		RETURN llMatch
		
	ENDPROC

	PROCEDURE refreshdescriptions
		LOCAL lnHeight1,lnHeight2
		
		lnHeight1=this.edtClassDesc.Height+2
		lnHeight2=this.edtItemDesc.Height+2
		IF NOT this.lDescriptions
			IF (this.lBrowser AND NOT this.edtClassDesc.Visible) OR ;
					(NOT this.lBrowser AND NOT this.edtItemDesc.Visible AND ;
					this.lInitialized)
				RETURN
			ENDIF
			this.edtClassDesc.Visible=.F.
			this.edtMemberDesc.Visible=.F.
			this.edtItemDesc.Visible=.F.
			IF this.lOutlineOCX
				this.oleClassList.Height=this.oleClassList.Height+lnHeight1
				this.oleFolderList.Height=this.oleFolderList.Height+lnHeight2
				this.oleMembers.Height=this.oleMembers.Height+lnHeight1
				this.oleItems.Height=this.oleItems.Height+lnHeight2
				IF this.lWebBrowser
					this.oleWebBrowser.Height=this.oleItems.Height
				ENDIF
				IF this.lBrowser
					this.txtClassList3D.Height=this.oleClassList.Height
					this.txtMembers3D.Height=this.oleMembers.Height
				ELSE
					this.txtClassList3D.Height=this.oleFolderList.Height
					this.txtMembers3D.Height=this.oleItems.Height
				ENDIF
			ELSE
				this.lstClassList.Height=this.lstClassList.Height+lnHeight1
				this.lstMembers.Height=this.lstMembers.Height+lnHeight1
				this.txtClassList3D.Height=this.lstClassList.Height
				this.txtMembers3D.Height=this.lstMembers.Height
			ENDIF
			this.MinHeight=this.MinHeight-lnHeight1
		ELSE
			IF (this.lBrowser AND this.edtClassDesc.Visible) OR ;
					(NOT this.lBrowser AND this.edtItemDesc.Visible)
				RETURN
			ENDIF
			this.MinHeight=this.MinHeight+lnHeight1
			IF this.Height<this.MinHeight
				this.Height=this.MinHeight
			ENDIF
			IF this.lOutlineOCX
				this.oleClassList.Height=this.oleClassList.Height-lnHeight1
				this.oleFolderList.Height=this.oleFolderList.Height-lnHeight2
				this.oleMembers.Height=this.oleMembers.Height-lnHeight1
				this.oleItems.Height=this.oleItems.Height-lnHeight2
				IF this.lWebBrowser
					this.oleWebBrowser.Height=this.oleItems.Height
				ENDIF
				IF this.lBrowser
					this.txtClassList3D.Height=this.oleClassList.Height
					this.txtMembers3D.Height=this.oleMembers.Height
				ELSE
					this.txtClassList3D.Height=this.oleFolderList.Height
					this.txtMembers3D.Height=this.oleItems.Height
				ENDIF
			ELSE
				this.lstClassList.Height=this.lstClassList.Height-lnHeight1
				this.lstMembers.Height=this.lstMembers.Height-lnHeight1
			ENDIF
			IF this.lBrowser
				this.edtClassDesc.Visible=.T.
				this.edtMemberDesc.Visible=.T.
			ELSE
				this.edtItemDesc.Visible=.T.
			ENDIF
		ENDIF
		this.shpSplitterH.Visible=this.lDescriptions
		
	ENDPROC


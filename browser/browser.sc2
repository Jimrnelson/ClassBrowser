	PROCEDURE setfont
		LPARAMETER tcFontName,tnFontSize,tlFontBold,tlFontItalic
		LOCAL lcFontName,lnFontSize,lnAtPos1,lnAtPos2,llFontBold,llFontItalic
		LOCAL llReset,lnParameters,llLockScreen
		
		lnParameters=PARAMETERS()
		llReset=.F.
		llLockScreen=this.LockScreen
		IF lnParameters=0 OR (NOT ISNULL(tcFontName) AND ","$tcFontName)
			IF lnParameters=0
				lcFontName=GETFONT(this.FontName,this.FontSize, ;
						IIF(this.FontBold,"B","")+IIF(this.FontItalic,"I",""))
			ELSE
				lcFontName=IIF(EMPTY(tcFontName),"",ALLTRIM(tcFontName))
			ENDIF
			IF EMPTY(lcFontName)
				RETURN .F.
			ENDIF
			lnAtPos1=AT(",",lcFontName)
			lnAtPos2=AT(",",lcFontName,2)
			lnFontSize=VAL(SUBSTR(lcFontName,lnAtPos1+1,lnAtPos2-1))
			llFontBold=(RIGHT(lcFontName,1)=="B" OR RIGHT(lcFontName,2)=="BI")
			llFontItalic=(RIGHT(lcFontName,1)=="I")
			lcFontName=LEFT(lcFontName,lnAtPos1-1)
		ELSE
			IF ISNULL(tcFontName)
				llReset=.T.
				IF FONTMETRIC(1,F_MS_SANS_SERIF_LOC,8,"")#13 OR ;
						FONTMETRIC(4,F_MS_SANS_SERIF_LOC,8,"")#2 OR ;
						FONTMETRIC(6,F_MS_SANS_SERIF_LOC,8,"")#5 OR ;
						FONTMETRIC(7,F_MS_SANS_SERIF_LOC,8,"")#11
					this.FontName=F_ARIAL_LOC
				ELSE
					this.FontName=F_MS_SANS_SERIF_LOC
				ENDIF
				this.FontSize=8
				this.FontBold=.F.
				this.FontItalic=.F.
				lcFontName=this.FontName
				lnFontSize=this.FontSize
				llFontBold=this.FontBold
				llFontItalic=this.FontItalic
			ELSE
				lcFontName=IIF(EMPTY(tcFontName),"",ALLTRIM(tcFontName))
				IF EMPTY(lcFontName) AND NOT this.FontName==F_ARIAL_LOC AND ;
						(FONTMETRIC(1,F_MS_SANS_SERIF_LOC,8,"")#13 OR ;
						FONTMETRIC(4,F_MS_SANS_SERIF_LOC,8,"")#2 OR ;
						FONTMETRIC(6,F_MS_SANS_SERIF_LOC,8,"")#5 OR ;
						FONTMETRIC(7,F_MS_SANS_SERIF_LOC,8,"")#11)
					lcFontName=F_ARIAL_LOC
				ENDIF
				lnFontSize=IIF(EMPTY(tnFontSize),0,tnFontSize)
				llFontBold=IIF(lnParameters>=3,tlFontBold,this.FontBold)
				llFontItalic=IIF(lnParameters>=4,tlFontItalic,this.FontItalic)
			ENDIF
		ENDIF
		this.LockScreen=.T.
		IF (llReset OR NOT this.FontName==lcFontName) AND NOT EMPTY(lcFontName)
			this.FontName=lcFontName
			IF this.lOutlineOCX
				this.oleClassList.object.Font.Name=lcFontName
				this.oleFolderList.object.Font.Name=lcFontName
				this.oleMembers.object.Font.Name=lcFontName
				this.oleItems.object.Font.Name=lcFontName
			ELSE
				this.lstClassList.FontName=lcFontName
				this.lstMembers.FontName=lcFontName
			ENDIF
			this.edtClassDesc.FontName=lcFontName
			this.edtMemberDesc.FontName=lcFontName
			this.edtItemDesc.FontName=lcFontName
		ENDIF
		IF (llReset OR this.FontSize#lnFontSize) AND NOT EMPTY(lnFontSize)
			this.FontSize=lnFontSize
			IF this.lOutlineOCX
				this.oleClassList.object.Font.Size=lnFontSize
				this.oleFolderList.object.Font.Size=lnFontSize
				this.oleMembers.object.Font.Size=lnFontSize
				this.oleItems.object.Font.Size=lnFontSize
			ELSE
				this.lstClassList.FontSize=lnFontSize
				this.lstMembers.FontSize=lnFontSize
			ENDIF
			this.edtClassDesc.FontSize=lnFontSize
			this.edtMemberDesc.FontSize=lnFontSize
			this.edtItemDesc.FontSize=lnFontSize
		ENDIF
		IF llReset OR this.FontBold#llFontBold
			this.FontBold=llFontBold
			IF this.lOutlineOCX
				this.oleClassList.object.Font.Bold=llFontBold
				this.oleFolderList.object.Font.Bold=llFontBold
				this.oleMembers.object.Font.Bold=llFontBold
				this.oleItems.object.Font.Bold=llFontBold
			ELSE
				this.lstClassList.FontBold=llFontBold
				this.lstMembers.FontBold=llFontBold
			ENDIF
			this.edtClassDesc.FontBold=llFontBold
			this.edtMemberDesc.FontBold=llFontBold
			this.edtItemDesc.FontBold=llFontBold
		ENDIF
		IF llReset OR this.FontItalic#llFontItalic
			this.FontItalic=llFontItalic
			IF this.lOutlineOCX
				this.oleClassList.object.Font.Italic=llFontItalic
				this.oleFolderList.object.Font.Italic=llFontItalic
				this.oleMembers.object.Font.Italic=llFontItalic
				this.oleItems.object.Font.Italic=llFontItalic
			ELSE
				this.lstClassList.FontItalic=llFontItalic
				this.lstMembers.FontItalic=llFontItalic
			ENDIF
			this.edtClassDesc.FontItalic=llFontItalic
			this.edtMemberDesc.FontItalic=llFontItalic
			this.edtItemDesc.FontItalic=llFontItalic
		ENDIF
		IF VARTYPE(this.oViewCode)=="O"
			this.oViewCode.SetFont
		ENDIF
		this.LockScreen=llLockScreen
		
	ENDPROC

	PROCEDURE setimageview
		LPARAMETERS tnView
		LOCAL lnView,llVisible
		
		IF TYPE("tnView")#"N" OR TYPE("this.oleItems")#"O"
			RETURN .F.
		ENDIF
		lnView=INT(tnView)
		IF NOT BETWEEN(lnView,0,3)
			RETURN .F.
		ENDIF
		DO CASE
			CASE lnView=0
				this.chkLargeIconsView.Value=.T.
				this.chkSmallIconsView.Value=.F.
				this.chkListView.Value=.F.
				this.chkDetailsView.Value=.F.
			CASE lnView=1
				this.chkLargeIconsView.Value=.F.
				this.chkSmallIconsView.Value=.T.
				this.chkListView.Value=.F.
				this.chkDetailsView.Value=.F.
			CASE lnView=2
				this.chkLargeIconsView.Value=.F.
				this.chkSmallIconsView.Value=.F.
				this.chkListView.Value=.T.
				this.chkDetailsView.Value=.F.
			CASE lnView=3
				this.chkLargeIconsView.Value=.F.
				this.chkSmallIconsView.Value=.F.
				this.chkListView.Value=.F.
				this.chkDetailsView.Value=.T.
		ENDCASE
		this.nImageView=lnView
		WITH this.oleItems
			llVisible=.Visible
			.Visible=.F.
			.View=lnView
			.Visible=llVisible
		ENDWITH
		
	ENDPROC

	PROCEDURE setwebbrowser
		LPARAMETERS tlWebBrowserMode,tlRemove
		
		IF this.lBrowser
			RETURN .F.
		ENDIF
		IF tlRemove
			this.lWebBrowser=.F.
			this.lWebView=.F.
			IF TYPE("this.oleWebBrowser")=="O"
				this.RemoveObject("oleWebBrowser")
			ENDIF
			this.oleItems.Visible=.T.
			RETURN
		ENDIF
		IF TYPE("this.oleWebBrowser")#"O"
			this.NewObject("oleWebBrowser",this.cWebBrowserClass,this.cWebBrowserClassLibrary)
			IF TYPE("this.oleWebBrowser")#"O"
				this.lWebBrowser=.F.
				this.lWebView=.F.
				RETURN .F.
			ENDIF
			this.lWebBrowser=.T.
			WITH this.oleWebBrowser
				.Top=this.oleItems.Top
				.Left=this.oleItems.Left
				.Height=this.oleItems.Height
				.Width=this.oleItems.Width
				.lParseSource=.T.
				.lRefresh=.T.
				.Navigate(this.cBlankHTMLFile)
			ENDWITH
		ENDIF
		IF tlWebBrowserMode=this.lWebView
			RETURN
		ENDIF
		this.lWebView=tlWebBrowserMode
		this.RefreshButtons
		IF tlWebBrowserMode
			this.oleWebBrowser.Visible=.T.
			this.oleItems.Visible=.F.
		ELSE
			this.oleItems.Visible=.T.
			this.oleWebBrowser.Stop
			this.oleWebBrowser.Visible=.F.
		ENDIF
		
	ENDPROC

	PROCEDURE shellexecute
		* WinApi :: ShellExecute
		**  Function: Opens a file in the application that it's
		**            associated with.
		**      Pass: lcFileName -  Name of the file to open
		**   
		**  Return:   2  - Bad Association (e.g., invalid URL)
		**            31 - No application association
		**            29 - Failure to load application
		**            30 - Application is busy 
		**
		**            Values over 32 indicate success
		**            and return an instance handle for
		**            the application started (the browser) 
		LPARAMETERS tcFileName,tcWorkDir,tcOperation,tcParams
		LOCAL lcFileName,lcWorkDir,lcOperation,lcParams
		
		IF EMPTY(tcFileName)
			RETURN -1
		ENDIF
		lcFileName=ALLTRIM(tcFileName)
		lcWorkDir=IIF(TYPE("tcWorkDir")="C",ALLTRIM(tcWorkDir),"")
		lcOperation=IIF(TYPE("tcOperation")="C" AND NOT EMPTY(tcOperation),ALLTRIM(tcOperation),"Open")
		lcParams=IIF(TYPE("tcParams")="C" AND NOT EMPTY(tcParams),ALLTRIM(tcParams),"")
		
		*-* HINSTANCE ShellExecute(hwnd, lpszOp, lpszFile, lpszParams, lpszDir, wShowCmd)
		*-* 
		*-* HWND hwnd;	/* handle of parent window	*/
		*-* LPCTSTR lpszOp;	/* address of string for operation to perform	*/
		*-* LPCTSTR lpszFile;	/* address of string for filename	*/
		*-* LPTSTR lpszParams;	/* address of string for executable-file parameters	*/
		*-* LPCTSTR lpszDir;	/* address of string for default directory	*/
		*-* INT wShowCmd;	/* whether file is shown when opened	*/
		DECLARE INTEGER ShellExecute ;
			    IN SHELL32.DLL ;
			    INTEGER nWinHandle,;
			    STRING cOperation,;   
			    STRING cFileName,;
			    STRING cParameters,;
			    STRING cDirectory,;
			    INTEGER nShowWindow
		RETURN ShellExecute(0,lcOperation,lcFileName,lcParams,lcWorkDir,1)
		
	ENDPROC

	PROCEDURE Show
		LPARAMETERS nStyle
		
		IF this.lRelease OR this.lBusyState
			NODEFAULT
			RETURN .F.
		ENDIF
		IF this.lBrowser AND NOT EMPTY(this.cFileName) AND USED(this.cAlias)
			IF this.lOutlineOCX
				IF (this.oleClassList.object.SelectedItem.Index-1)#this.nClassListIndex
					this.RefreshMembers
				ENDIF
			ELSE
				IF (this.lstClassList.ListIndex-1)#this.nClassListIndex
					this.RefreshMembers
				ENDIF
			ENDIF
		ENDIF
		IF this.AddInMethod(PROGRAM())
			NODEFAULT
			RETURN
		ENDIF
		this.Resize
		
	ENDPROC


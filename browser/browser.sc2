				=.F.
			CASE lcType=="L"
				IF TYPE("_screen.ActiveForm.ActiveControl")=="O" AND ;
						NOT LEFT(LOWER(_screen.ActiveForm.Name),12)=="classbrowser"
					lcFileName=LOWER(_screen.ActiveForm.ActiveControl.ClassLibrary)
					IF TYPE("lcDefaultClass")#"C"
						lcDefaultClass=LOWER(_screen.ActiveForm.ActiveControl.Class)
					ENDIF
				ELSE
					RELEASE laSelObj
					DIMENSION laSelObj[1]
					lnCount=ASELOBJ(laSelObj)
					IF lnCount=1 OR (lnCount=0 AND ASELOBJ(laSelObj,1)>0)
						lcFileName=LOWER(laSelObj[1].ClassLibrary)
						IF TYPE("lcDefaultClass")#"C"
							lcName=LOWER(laSelObj[1].Name)
							lcClass=LOWER(laSelObj[1].Class)
							DIMENSION laSelObj[1]
							laSelObj=.NULL.
							IF ASELOBJ(laSelObj,1)=1 AND LOWER(laSelObj[1].Name)==lcName AND ;
									NOT LOWER(JUSTEXT(SYS(1271,laSelObj[1])))=="scx"
								lcDefaultClass=lcName
							ELSE
								lcDefaultClass=lcClass
							ENDIF
							DIMENSION laSelObj[1]
							laSelObj=.NULL.
						ENDIF
					ENDIF
					RELEASE laSelObj
				ENDIF
				IF EMPTY(lcFileName) OR NOT FILE(lcFileName)
					lcFileName=""
					lcDefaultClass=""
				ENDIF
			CASE lcType=="O"
				lcFileName=LOWER(tcFileName.ClassLibrary)
				IF EMPTY(lcFileName)
					lcFileName=""
				ELSE
					IF TYPE("lcDefaultClass")#"C"
						lcDefaultClass=LOWER(tcFileName.Class)
					ENDIF
				ENDIF
				RELEASE tcFileName
			CASE lcType=="C"
				lcFileName=LOWER(ALLTRIM(lcFileName))
				IF NOT EMPTY(lcFilename)
					lnAtPos=AT(",",lcFileName)
					IF lnAtPos>0
						lcFileName=ALLTRIM(LEFT(lcFileName,lnAtPos-1))
					ENDIF
					IF NOT "."$lcFileName
						lcFileName=lcFileName+IIF(this.lBrowser,".vcx",".dbf")
					ENDIF
				ENDIF
			OTHERWISE
				this.MsgBox(M_FUNCTION_ARG_INVALID_LOC+".",16)
				this.QueryUnload
				this.Destroy
				RETURN .F.
		ENDCASE
		llAutoDefault=(TYPE("lcFileName")#"C" OR EMPTY(lcFileName))
		IF llAutoDefault
			lcFileName=""
		ENDIF
		SELECT browser
		SCAN ALL FOR UPPER(ALLTRIM(Platform))==this.cPlatform AND ;
				UPPER(ALLTRIM(Type))=="PREFW" AND ;
				UPPER(ALLTRIM(ID))=="FORMINFO" AND NOT EMPTY(Name)
			lcName=LOWER(ALLTRIM(MLINE(Name,1)))
			IF NOT ":"$lcName
				lcName=LOWER(FULLPATH(lcName,this.cProgramPath))
				REPLACE Name WITH lcName

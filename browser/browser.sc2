	PROCEDURE updateformcount
		LPARAMETERS tnValue,tcFileName
		LOCAL lnValue,lcFileName,lnLastSelect,lnRecNo
		
		IF EMPTY(tnValue) OR NOT USED("browser")
			RETURN .F.
		ENDIF
		lcFileName=IIF(ISNULL(tcFileName) OR EMPTY(tcFileName),"",LOWER(ALLTRIM(tcFileName)))
		lnLastSelect=SELECT()
		lnRecNo=IIF(LOWER(ALIAS())=="browser",RECNO(),0)
		SELECT browser
		IF ISNULL(tcFileName)
			this.RefreshRecNo
		ELSE
			LOCATE FOR UPPER(ALLTRIM(Platform))==this.cPlatform AND ;
					UPPER(ALLTRIM(Type))=="PREFW" AND ;
					UPPER(ALLTRIM(ID))=="FORMINFO" AND ;
					LOWER(ALLTRIM(Name))==lcFileName
		ENDIF
		IF NOT EOF()
			lnValue=MAX(FormCount+tnValue,0)
			IF lnValue<=0
				lnValue=0
			ENDIF
			REPLACE FormCount WITH lnValue
		ENDIF
		SELECT (lnLastSelect)
		IF lnRecNo>0
			GO lnRecNo
		ELSE
			IF EOF()
				LOCATE
			ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE updatereferences
		LPARAMETERS tcOldClassLoc,tcOldClass,tcNewClassLoc,tcNewClass,tlAllInstances
		LOCAL lcOldClassLoc,lcOldClass,lcNewClassLoc,lcNewClass,lcClassLoc,lcClass
		LOCAL lcAlias,lnLastSelect,lnRecNo,lnFileNo,lnCount,oForm,llUpdated
		
		IF NOT this.lBrowser OR PARAMETERS()<4
			RETURN .F.
		ENDIF
		llUpdated=.F.
		lcOldClassLoc=LOWER(FULLPATH(ALLTRIM(tcOldClassLoc)))
		lcOldClass=LOWER(ALLTRIM(tcOldClass))
		lcNewClassLoc=LOWER(FULLPATH(ALLTRIM(tcNewClassLoc)))
		lcNewClass=LOWER(ALLTRIM(tcNewClass))
		lnLastSelect=SELECT()
		lnRecNo=IIF(EOF() OR RECNO()>RECCOUNT(),1,RECNO())
		FOR lnFileNo = 1 TO this.nFileCount
			lcFileName=this.aFiles[lnFileNo]
			IF EMPTY(lcFileName)
				LOOP
			ENDIF
			lcAlias="metadata"+ALLTRIM(STR(lnFileNo))
			IF NOT USED(lcAlias)
				LOOP
			ENDIF
			SET MESSAGE TO M_SEARCHING_LOC+" ("+lcFileName+") ..."
			SELECT (lcAlias)
			SCAN ALL FOR NOT EMPTY(ClassLoc)
				lcClassLoc=LOWER(FULLPATH(ALLTRIM(MLINE(ClassLoc,1)),lcFileName))
				lcClass=LOWER(ALLTRIM(MLINE(Class,1)))
				IF NOT lcClassLoc==lcOldClassLoc OR NOT lcClass==lcOldClass
					LOOP
				ENDIF
				lcNewClassLoc=LOWER(ALLTRIM(SYS(2014,lcNewClassLoc,lcFileName)))
				CLEAR CLASS (lcClass)
				CLEAR CLASS (lcNewClass)
				REPLACE ClassLoc WITH lcNewClassLoc, Class WITH lcNewClass
				llUpdated=.T.
			ENDSCAN
		ENDFOR
		SELECT (lnLastSelect)
		IF USED() AND lnRecNo>0
			GO lnRecNo
		ENDIF
		SET MESSAGE TO
		IF tlAllInstances
			FOR lnCount = _screen.FormCount TO 1 STEP -1
				oForm=_screen.Forms[lnCount]
				IF oForm.Name==this.Name OR NOT oForm.Class==this.Class OR ;
						NOT LOWER(LEFT(oForm.Name,12))=="classbrowser" OR ;
						TYPE("oForm.cBrowserTable")=="U"
					LOOP
				ENDIF
				IF oForm.UpdateReferences(tcOldClassLoc,tcOldClass,tcNewClassLoc,tcNewClass)
					llUpdated=.T.
					oForm.RefreshBrowser
				ENDIF
				oForm=.NULL.
			ENDFOR
			RETURN llUpdated
		ENDIF
		RETURN llUpdated
		
	ENDPROC


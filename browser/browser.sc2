			lcVersion=IIF(TYPE("Reserved1")=="M",UPPER(ALLTRIM(Reserved1)),"")
			lnVersion=VAL(SUBSTR(lcVersion,AT("=",lcVersion)+1))
			IF lnVersion>=3 OR lnVersion=0.029
				RETURN
			ENDIF
		ELSE
			this.vResult=.T.
			DO brwCheckGalleryTableStructure IN browser WITH (this)
			this.vResult=.T.
			IF FCOUNT()>=24 AND UPPER(FIELD(24))=="USER" AND TYPE("ID")=="C" AND ;
					TYPE("FileName")=="M" AND TYPE("Keywords")=="M"

	PROCEDURE cleanupfile
		LOCAL lcFilter,lnRecNo,lnRecCount,lcFileName,lnDeletedCount
		
		IF NOT this.lVCXSCXMode OR NOT USED(this.cAlias) OR this.lSCXMode
			RETURN .F.
		ENDIF
		this.RefreshRecNo
		lcFileName=this.cFileName
		SET MESSAGE TO M_CHECKING_FOR_DELETED_LOC+[ "]+lcFileName+[" ...]
		lcFilter=this.cFilter
		SELECT (this.cAlias)
		lnRecNo=RECNO()
		SET FILTER TO
		COUNT FOR DELETED() TO lnDeletedCount
		SET FILTER TO &lcFilter
		GO lnRecNo
		IF lnDeletedCount=0
			INKEY(.1,"HM")
			SELECT 0
			SET MESSAGE TO
			RETURN .F.
		ENDIF
		SET MESSAGE TO M_PACKING_FILE_LOC+[ "]+lcFileName+[" ...]
		this.ClearClass(.T.)
		lnRecCount=RECCOUNT()
		USE IN (this.cAlias)
		this.RefreshFileAttrib
		SELECT 0
		USE (lcFileName) EXCLUSIVE ALIAS (this.cAlias)
		IF USED(this.cAlias)
			SET FILTER TO NOT DELETED()
			lnLastRecCount=RECNO()
			IF USED(this.cAlias)
				CURSORSETPROP("Buffering",1)
				lnRecCount=RECCOUNT()
				PACK
				USE IN (this.cAlias)
			ENDIF
			this.RefreshFileAttrib
		ENDIF
		SELECT 0
		USE (lcFileName) AGAIN SHARED ALIAS (this.cAlias)
		CURSORSETPROP("Buffering",1)
		SET FILTER TO &lcFilter
		IF lnRecCount#RECCOUNT()
			this.RefreshBrowser
		ENDIF
		SELECT 0
		SET MESSAGE TO
		
	ENDPROC


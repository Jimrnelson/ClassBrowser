	PROCEDURE findclass
		LPARAMETERS tcFind
		LOCAL oFindForm,lcFind,lnCount,lcAlias,lcFileName,lcClass,llVCXSCXMode
		LOCAL lnAtLine,lnAtPos,lcMember,llMatch,llMethod
		LOCAL lnLastSelect,lnRecNo,lnFileNo
		
		IF NOT this.lBrowser OR this.nClassCount=0
			RETURN .F.
		ENDIF
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		IF TYPE("tcFind")=="C"
			lcFind=ALLTRIM(tcFind)
		ELSE
			lcFind=""
			this.lModalDialog=.T.
			DO FORM brwfind NAME oFindForm WITH (this),(this.cLastFindText) ;
					TO lcFind
			SET DATASESSION TO this.DataSessionID
			IF NOT ISNULL(lcFind)
				this.cLastFindText=lcFind
			ENDIF
		ENDIF
		IF ISNULL(lcFind) OR EMPTY(lcFind)
			RETURN .F.
		ENDIF
		lnLastSelect=SELECT()
		llMatch=.F.
		llMethod=.F.
		lnAtLine=0
		FOR lnFileNo = 1 TO this.nFileCount
			lcFileName=this.aFiles[lnFileNo]
			IF EMPTY(lcFileName)
				LOOP
			ENDIF
			lcAlias="metadata"+ALLTRIM(STR(lnFileNo))
			IF NOT USED(lcAlias)
				LOOP
			ENDIF
			llVCXSCXMode=(INLIST(RIGHT(lcFileName,4),".vcx",".scx",".prg"))
			SET MESSAGE TO M_SEARCHING_LOC+" ("+lcFileName+") ..."
			IF NOT llVCXSCXMode
				LOOP
			ENDIF
			SELECT (lcAlias)
			SCAN ALL
				IF EMPTY(Parent)
					lcClass=ObjName
					lcMember=""
				ELSE
					lcMember=ObjName
				ENDIF
				IF this.WildCardMatch(lcFind,ObjName)
					lnRecNo=RECNO()
					lnAtLine=0
					llMatch=.T.
					EXIT
				ENDIF
				lnAtLine=ATCLINE(lcFind,Reserved7)
				IF lnAtLine=0
					lnAtLine=ATCLINE(lcFind,Reserved3)
				ENDIF
				IF lnAtLine>0
					lnRecNo=RECNO()
					llMatch=.T.
					EXIT
				ENDIF
			ENDSCAN
			IF llMatch
				EXIT
			ENDIF
		ENDFOR
		IF NOT llMatch
			this.RefreshRecNo
			SELECT (lnLastSelect)
			SET MESSAGE TO
			WAIT CLEAR
			WAIT WINDOW W_MATCH_NOT_FOUND_LOC NOWAIT
			RETURN .F.
		ENDIF
		IF NOT this.SeekClass(lcClass,lcFileName)
			SELECT (lnLastSelect)
			RETURN .F.
		ENDIF
		IF lnAtLine=0 AND EMPTY(lcMember)
			SELECT (lnLastSelect)
			RETURN
		ENDIF
		IF EMPTY(lcMember)
			SELECT (lcAlias)
			GO lnRecNo
			lcMember=LOWER(ALLTRIM(CHRTRAN(MLINE(Reserved3,lnAtLine),"*?%","")))
			lnAtPos=AT(" ",lcMember)
			IF lnAtPos>0
				lcMember=ALLTRIM(LEFT(lcMember,lnAtPos-1))
			ENDIF
		ENDIF
		SELECT (lnLastSelect)
		RETURN this.SeekMember(lcMember)
		
	ENDPROC

	PROCEDURE formaddobject
		LPARAMETERS toObject,tnXCoord,tnYCoord,tlActivateForm,tlDesignMode,tlNoShow,tlNoErrors,tlBuilder,toForm
		LOCAL lcClass,lcObjName,lcMWindow,lnRow,tnRow,llPixelMode,llIgnoreErrors
		LOCAL laMembers,lnMemberCount,oNewObject,lcTargetBaseClass,lcBaseClass,lcBaseClass2
		LOCAL lnFormCount,lnRowOffset,lnColOffset,lcFileName,lcKeyBoard,llBuilder,llAutoAddForm
		LOCAL lnCount,lnCount2,lcFontName,lnFontSize,llAddToScreen,llAddToCommand,llAddToNew
		LOCAL oTemp,lcCode,lnLastSelect,lcParentObjName,lcParentObjProperty
		
		this.lFormAddObject=.T.
		IF this.lBrowser
			IF NOT this.lVCXSCXMode OR this.lFileMode OR this.lSCXMode
				RETURN .F.
			ENDIF
			lcClass=this.cClass
			lcBaseClass=this.cBaseClass
			lcFileName=this.cFileName
		ELSE
			IF ISNULL(this.oItem) OR NOT PEMSTATUS(this.oItem,"cBaseClass",5)
				RETURN .F.
			ENDIF
			lcClass=LOWER(this.oItem.cClass)
			lcBaseClass=LOWER(this.oItem.cBaseClass)
			lcFileName=LOWER(this.oItem.cFileName)
		ENDIF
		IF this.nAtPos=-.1
			this.nAtPos=-.2
		ENDIF
		llAddToScreen=.F.
		llAddToCommand=.F.
		llAddToNew=.F.
		IF VARTYPE(toObject)#"O"
			DO CASE
				CASE VARTYPE(toObject)#"C"
					RETURN .F.
				CASE LOWER(ALLTRIM(toObject))=="_screen"
					llAddToScreen=.T.
				CASE LOWER(ALLTRIM(toObject))=="command"
					llAddToCommand=.T.
				CASE LOWER(ALLTRIM(toObject))=="new"
					llAddToNew=.T.
				OTHERWISE
					RETURN .F.
			ENDCASE
			toObject=_screen
		ELSE
			IF toObject=_screen
				llAddToScreen=.T.
			ELSE
				DO WHILE .T.
					IF PEMSTATUS(toObject,"AddObject",5) OR VARTYPE(toObject.Parent)#"O" OR ;
							VARTYPE(toObject.Parent.BaseClass)#"C"
						EXIT
					ENDIF
					toObject=toObject.Parent
				ENDDO
			ENDIF
		ENDIF
		lcTargetBaseClass=LOWER(toObject.BaseClass)
		llAutoAddForm=(lcTargetBaseClass=="form" AND VARTYPE(tnXCoord)#"N")
		IF lcTargetBaseClass=="form" AND VARTYPE(toForm)#"O"
			toForm=toObject
		ENDIF
		IF lcTargetBaseClass=="pageframe" AND NOT lcTargetBaseClass=="page"
			FOR lnCount = 1 TO toObject.PageCount
				IF lnCount=toObject.ActivePage
					toObject=toObject.Pages[lnCount]
					lcTargetBaseClass=LOWER(toObject.BaseClass)
					EXIT
				ENDIF
			ENDFOR
		ENDIF
		llPixelMode=(VARTYPE(toForm)=="O" AND TYPE("toForm.ScaleMode")=="N" AND ;
				toForm.ScaleMode=3)
		lnScaleMode=3
		IF VARTYPE(tnXCoord)=="N" AND TYPE("toForm.ScaleMode")=="N" AND NOT llPixelMode
			lnScaleMode=toForm.ScaleMode
			IF NOT PEMSTATUS(toForm,"ScaleMode",0)
				lnScaleMode=-1
			ENDIF
			toForm.ScaleMode=3
			llPixelMode=.T.
			llAutoAddForm=.F.
		ENDIF
		IF llPixelMode
			IF TYPE("toForm.FontName")=="C"
				lcFontName=toForm.FontName
				lnFontSize=toForm.FontSize
			ELSE
				oTemp=CREATEOBJECT("Textbox")
				lcFontName=oTemp.FontName
				lnFontSize=oTemp.FontSize
				oTemp=.NULL.
			ENDIF
		ENDIF
		lcMWindow=" "
		lnRow=0
		lnCol=0
		lnRowOffset=0
		lnColOffset=0
		DO CASE
			CASE llAddToScreen OR llAddToCommand OR llAddToNew
				lcMWindow=""
			CASE llAutoAddForm AND tlDesignMode
				lcMWindow=LOWER(toObject.Caption)
			CASE PARAMETERS()<3 AND NOT llAutoAddForm AND NOT tlDesignMode
				lnCount=-1
				DO WHILE .T.
					lnCount=lnCount+1
					lcParentObjName="toObject"+REPLICATE(".parent",lnCount)
					IF TYPE(lcParentObjName)#"O"
						EXIT
					ENDIF
					lcBaseClass2=LOWER(EVALUATE(lcParentObjName+".BaseClass"))
					IF lcBaseClass2=="form"
						lcMWindow=EVALUATE(lcParentObjName+".Name")
						lcParentObjProperty=lcParentObjName+".ScaleMode"
						llPixelMode=(TYPE(lcParentObjProperty)#"N" OR ;
								EVALUATE(lcParentObjProperty)=3)
						EXIT
					ENDIF
					lcParentObjProperty=lcParentObjName+".Top"
					IF TYPE(lcParentObjProperty)#"N"
						LOOP
					ENDIF
					lnRowOffset=lnRowOffset-EVALUATE(lcParentObjProperty)
					lcParentObjProperty=lcParentObjName+".Left"
					lnColOffset=lnColOffset-EVALUATE(lcParentObjProperty)
					lcParentObjProperty=lcParentObjName+".BaseClass"
					IF LOWER(EVALUATE(lcParentObjProperty))=="pageframe"
						lnRowOffset=-OBJTOCLIENT(toObject,1)
						lnColOffset=-OBJTOCLIENT(toObject,2)
					ENDIF
				ENDDO
			CASE llAutoAddForm
				lcMWindow=LOWER(toObject.Name)
		ENDCASE
		IF VARTYPE(tnYCoord)=="N"
			lnRow=tnYCoord
		ELSE
			IF NOT lcMWindow==" " AND llAutoAddForm AND WEXIST(lcMWindow)
				lnRow=MROW(lcMWindow)
				IF lnRow<0
					RETURN .F.
				ENDIF
				IF llPixelMode
					lnRow=lnRow*FONTMETRIC(1,lcFontName,lnFontSize)
				ENDIF
				lnRow=lnRow+lnRowOffset
			ENDIF
		ENDIF
		IF VARTYPE(tnXCoord)=="N"
			lnCol=tnXCoord
		ELSE
			IF NOT lcMWindow==" " AND llAutoAddForm AND WEXIST(lcMWindow)
				lnCol=MCOL(lcMWindow)
				IF lnCol<0
					RETURN .F.
				ENDIF
				IF llPixelMode
					lnCol=lnCol*FONTMETRIC(6,lcFontName,lnFontSize)
				ENDIF
				lnCol=lnCol+lnColOffset
			ENDIF
		ENDIF
		DIMENSION laMembers[1]
		laMembers=""
		lnMemberCount=AMEMBERS(laMembers,toObject,2)
		FOR lnCount2 = 1 TO lnMemberCount
			laMembers[lnCount2]=LOWER(laMembers[lnCount2])+" "
		ENDFOR
		lnCount=0
		DO WHILE .T.
			lnCount=lnCount+1
			lcObjName=lcClass+ALLTRIM(STR(lnCount))
			IF ASCAN(laMembers,LOWER(lcObjName)+" ")=0
				EXIT
			ENDIF
		ENDDO
		DIMENSION laMembers[1]
		laMembersObj=""
		lnMemberCount=AMEMBERS(laMembers,toObject,1)
		FOR lnCount2 = 1 TO (2*lnMemberCount)
			laMembers[lnCount2]=LOWER(laMembers[lnCount2])+" "
		ENDFOR
		DO WHILE .T.
			IF ASCAN(laMembers,LOWER(lcObjName)+" ")=0
				EXIT
			ENDIF
			lnCount=lnCount+1
			lcObjName=lcClass+ALLTRIM(STR(lnCount))
		ENDDO
		llIgnoreErrors=this.lIgnoreErrors
		this.lIgnoreErrors=tlNoErrors
		lcObjName=PROPER(lcObjName)
		lnFormCount=IIF(lcTargetBaseClass=="formset",toObject.FormCount,0)
		IF llAddToCommand OR llAddToNew OR (llAddToScreen AND ;
				(lcBaseClass=="formset" OR lcBaseClass=="form" OR ;
				lcBaseClass=="toolbar" OR lcBaseClass=="custom"))
			lcObjName="o"+PROPER(lcObjName)
			DO WHILE .T.
				IF TYPE(lcObjName)=="U"
					EXIT
				ENDIF
				lnCount=lnCount+1
				lcObjName="o"+PROPER(lcClass+ALLTRIM(STR(lnCount)))
			ENDDO
			IF llAddToCommand
				DO CASE
					CASE lnScaleMode=-1
						toForm.ResetToDefault("ScaleMode")
					CASE lnScaleMode=0
						toForm.ScaleMode=0
				ENDCASE
				lcKeyboard=[{Ctrl+F2}{Ctrl+END}]+lcObjName+ ;
						[=NEWOBJECT("]+lcClass+[","]+lcFileName+["){ENTER}]
				KEYBOARD (lcKeyboard) PLAIN
				this.lIgnoreErrors=llIgnoreErrors
				RETURN
			ENDIF
			oNewObject=NEWOBJECT(lcClass,lcFileName)
			IF VARTYPE(oNewObject)#"O" OR ISNULL(oNewObject)
				DO CASE
					CASE lnScaleMode=-1
						toForm.ResetToDefault("ScaleMode")
					CASE lnScaleMode=0
						toForm.ScaleMode=0
				ENDCASE
				oNewObject=.NULL.
				this.lError=.F.
				this.lIgnoreErrors=llIgnoreErrors
				RETURN .F.
			ENDIF
			IF llAddToNew
				DO CASE
					CASE lnScaleMode=-1
						toForm.ResetToDefault("ScaleMode")
					CASE lnScaleMode=0
						toForm.ScaleMode=0
				ENDCASE
				this.lError=.F.
				this.lIgnoreErrors=llIgnoreErrors
				RETURN oNewObject
			ENDIF
			PUBLIC (lcObjName)
			lcCode=lcObjname+[=oNewObject]
			&lcCode
			IF NOT lcBaseClass=="formset" AND TYPE("oNewObject.ScaleMode")=="N"
				IF oNewObject.ScaleMode=0
					oTemp=IIF(lcBaseClass=="form",oNewObject,_screen)
					lnRow=lnRow/FONTMETRIC(1,oTemp.FontName,oTemp.FontSize)
					lnCol=lnCol/FONTMETRIC(6,oTemp.FontName,oTemp.FontSize)
					oTemp=.NULL.
				ENDIF
				oNewObject.Top=lnRow
				oNewObject.Left=lnCol
			ENDIF
			lnLastSelect=SELECT()
			IF this.lBrowser
				this.RefreshRecNo
				SELECT (this.cAlias)
				IF NOT llAddToNew AND NOT tlNoShow AND ;
						ATC(CR_LF+"Visible = .F.",CR_LF+Properties)=0 AND ;
						PEMSTATUS(oNewObject,"Show",5) AND ;
						PEMSTATUS(oNewObject,"Show",3)=="Method"
					oNewObject.Show
				ENDIF
			ELSE
				IF PEMSTATUS(oNewObject,"Show",5) AND ;
						PEMSTATUS(oNewObject,"Show",3)=="Method"
					oNewObject.Show
				ENDIF
			ENDIF
			SELECT (lnLastSelect)
			this.oSource=oNewObject
			this.lError=.F.
			this.lIgnoreErrors=llIgnoreErrors
			IF this.AddInMethod(PROGRAM())
				this.oSource=.NULL.
				this.AutoRefresh
				DO CASE
					CASE lnScaleMode=-1
						toForm.ResetToDefault("ScaleMode")
					CASE lnScaleMode=0
						toForm.ScaleMode=0
				ENDCASE
				RETURN
			ENDIF
			this.oSource=.NULL.
			this.AutoRefresh
			DO CASE
				CASE lnScaleMode=-1
					toForm.ResetToDefault("ScaleMode")
				CASE lnScaleMode=0
					toForm.ScaleMode=0
			ENDCASE
			RETURN
		ELSE
			toObject.NewObject(lcObjName,lcClass,lcFileName)
		ENDIF
		oNewObject=.NULL.
		IF TYPE("toObject."+lcObjName)#"O"
			DO CASE
				CASE lnScaleMode=-1
					toForm.ResetToDefault("ScaleMode")
				CASE lnScaleMode=0
					toForm.ScaleMode=0
			ENDCASE
			this.lError=.F.
			this.lIgnoreErrors=llIgnoreErrors
			RETURN .F.
		ENDIF
		oNewObject=toObject.&lcObjName
		oNewObject.Name=lcObjName
		
		*!* ** { JRN -- 12/30/2022 07:02:44 AM - Begin
		*!* IF PEMSTATUS(oNewObject, "Top", 5)
		IF PEMSTATUS(oNewObject, "Top", 5) and not PEMSTATUS(oNewObject, "Top", 1)
		*!* ** } JRN -- 12/30/2022 07:02:44 AM - End
		
			oNewObject.Top=lnRow
		EndIf
		
		*!* ** { JRN -- 12/30/2022 07:03:23 AM - Begin
		*!* IF PEMSTATUS(oNewObject, "Left", 5)
		IF PEMSTATUS(oNewObject, "Left", 5) and not PEMSTATUS(oNewObject, "Left", 1)
		*!* ** } JRN -- 12/30/2022 07:03:23 AM - End
		
			oNewObject.Left=lnCol
		ENDIF
		IF tlDesignMode
			toObject.Refresh
		ELSE
			IF NOT tlNoShow AND TYPE("oNewObject.Visible")=="L"
				lnLastSelect=SELECT()
				IF this.lBrowser
					this.RefreshRecNo
					SELECT (this.cAlias)
					IF ATC(CR_LF+"Visible = .F.",CR_LF+Properties)=0 AND ;
							GETPEM(oNewObject.Class,"Visible")
						oNewObject.Visible=.T.
					ENDIF
				ELSE
					oNewObject.Visible=.T.
				ENDIF
				SELECT (lnLastSelect)
			ENDIF
			IF llAddToScreen
				_screen.Refresh
			ENDIF
		ENDIF
		this.lIgnoreErrors=llIgnoreErrors
		this.lError=.F.
		this.oSource=oNewObject
		IF this.AddInMethod(PROGRAM())
			DO CASE
				CASE lnScaleMode=-1
					toForm.ResetToDefault("ScaleMode")
				CASE lnScaleMode=0
					toForm.ScaleMode=0
			ENDCASE
			this.oSource=.F.
			this.oSource=.NULL.
			RETURN
		ENDIF
		this.oSource=.NULL.
		llBuilder=(tlBuilder AND ;
				((TYPE("oNewObject.BuilderX")=="C" AND NOT EMPTY(oNewObject.BuilderX)) OR ;
					(TYPE("oNewObject.Builder")=="C" AND NOT EMPTY(oNewObject.Builder))) AND ;
					NOT EMPTY(_builder) AND FILE(_builder))
		IF NOT tlNoShow AND tlActivateForm
			IF this.lBrowser AND this.Visible
				this.cmdClassIcon.SetFocus
			ENDIF
			oTemp=toObject
			DO WHILE NOT LOWER(oTemp.BaseClass)=="form"
				IF TYPE("oTemp.parent")#"O"
					EXIT
				ENDIF
				oTemp=oTemp.parent
			ENDDO
			IF LOWER(oTemp.BaseClass)=="form"
				oTemp.Show
			ENDIF
			oTemp=.NULL.
		ENDIF
		IF NOT tlDesignMode
			DO CASE
				CASE lnScaleMode=-1
					toForm.ResetToDefault("ScaleMode")
				CASE lnScaleMode=0
					toForm.ScaleMode=0
			ENDCASE
			IF llBuilder
				DOEVENTS
				DO (_builder) WITH (oNewObject)
			ENDIF
			RETURN oNewObject
		ENDIF
		IF lcTargetBaseClass=="formset"
			DO CASE
				CASE lnScaleMode=-1
					toForm.ResetToDefault("ScaleMode")
				CASE lnScaleMode=0
					toForm.ScaleMode=0
			ENDCASE
			IF lnFormCount=1
				DOEVENTS
				CLEAR TYPEAHEAD
				KEYBOARD "{Alt+M}C" PLAIN
			ENDIF
			IF llBuilder
				DOEVENTS
				DO (_builder) WITH (oNewObject)
			ENDIF
			RETURN oNewObject
		ENDIF
		lcMWindow=toObject.Name
		IF NOT WEXIST(lcMWindow)
			IF TYPE("toObject.Caption")#"C"
				DO CASE
					CASE lnScaleMode=-1
						toForm.ResetToDefault("ScaleMode")
					CASE lnScaleMode=0
						toForm.ScaleMode=0
				ENDCASE
				IF llBuilder
					DOEVENTS
					DO (_builder) WITH (oNewObject)
				ENDIF
				RETURN oNewObject
			ENDIF
			lcMWindow=toObject.Caption
			IF NOT WEXIST(lcMWindow)
				DO CASE
					CASE lnScaleMode=-1
						toForm.ResetToDefault("ScaleMode")
					CASE lnScaleMode=0
						toForm.ScaleMode=0
				ENDCASE
				IF llBuilder
					DOEVENTS
					DO (_builder) WITH (oNewObject)
				ENDIF
				RETURN oNewObject
			ENDIF
		ENDIF
		CLEAR TYPEAHEAD
		IF NOT llBuilder AND NOT EMPTY(lcMWindow)
			DOEVENTS
			IF llPixelMode
				MOUSE CLICK ;
						AT lnRow+4,lnCol+2 PIXELS ;
						WINDOW (lcMWindow)
			ELSE
				MOUSE CLICK ;
						AT lnRow+.25,lnCol+.5 ;
						WINDOW (lcMWindow)
			ENDIF
			DOEVENTS
		ENDIF
		DO CASE
			CASE lnScaleMode=-1
				toForm.ResetToDefault("ScaleMode")
			CASE lnScaleMode=0
				toForm.ScaleMode=0
		ENDCASE
		IF llBuilder
			DOEVENTS
			DO (_builder) WITH (oNewObject)
		ENDIF
		RETURN oNewObject
		
	ENDPROC

	PROCEDURE formatmethods
		LPARAMETERS tcMethods
		LOCAL lcMethods
		
		IF EMPTY(tcMethods)
			RETURN ""
		ENDIF
		lcMethods=MARKER+tcMethods
		DO WHILE CR_LF+CR_LF+"ENDPROC"$lcMethods
			lcMethods=STRTRAN(lcMethods,CR_LF+CR_LF+"ENDPROC",CR_LF+"ENDPROC")
		ENDDO
		lcMethods=STRTRAN(STRTRAN(STRTRAN(STRTRAN(STRTRAN(STRTRAN(STRTRAN(SUBSTR(this.IndentText( ;
				CR_LF+STRTRAN(lcMethods,CR_LF+"ENDPROC"+CR_LF+"PROCEDURE ",CR_LF+"ENDPROC"+ ;
				CR_LF+CR_LF+CR_LF+"PROCEDURE "),2),2),CR_LF+TAB+TAB+"PROCEDURE ",CR_LF+ ;
				TAB+"PROCEDURE "),CR_LF+TAB+TAB+"ENDPROC",CR_LF+TAB+"ENDPROC"),TAB+MARKER,""), ;
				CR+TAB,CR_LF+TAB),CR_LF+TAB+TAB+CR_LF,CR_LF+CR_LF),TAB+"ENDPROC"+CR_LF+TAB+"PROTECTED ", ;
				TAB+"ENDPROC"+CR_LF+CR_LF+CR_LF+TAB+"PROTECTED "),TAB+"ENDPROC"+CR_LF+TAB+"HIDDEN ", ;
				TAB+"ENDPROC"+CR_LF+CR_LF+CR_LF+TAB+"HIDDEN ")
		RETURN lcMethods
		
	ENDPROC

	PROCEDURE formatproperties
		LPARAMETERS tcProperties,tlAddObjectMode
		LOCAL lcProperties,lcProperties2,lcProperty,lcBadChars,lcNewChars
		LOCAL lcStrData,lnStrLen,lcSubStr,lcNewStr,lnCount,lnAtPos,lcLeftChar
		LOCAL nLineCnt
		LOCAL cProperty
		LOCAL nLen
		LOCAL nEqualPos
		LOCAL nPos
		LOCAL i
		LOCAL nCnt
		LOCAL cDelimStart
		LOCAL cDelimEnd
		LOCAL nPos1
		LOCAL nPos2
		LOCAL cPropertyValue
		LOCAL ARRAY aPropList[1]
		LOCAL ARRAY aMultiLine[1]
		
		IF EMPTY(tcProperties)
			RETURN ""
		ENDIF
		
		lcProperties=tcProperties
		IF NOT RIGHT(lcProperties,1)==CR
			lcProperties=lcProperties+CR_LF
		ENDIF
		lcProperties2=""
		lcBadChars=[='/\,:;{}]+'[]!@#$%^&*<>+|"'
		lcNewChars=SPACE(LEN(lcBadChars))
		
		* RMK - 05/11/2004 - added support for multi-line properties
		m.nLineCnt = 0
		* m.cProperty = RTRIM(GETWORDNUM(m.lcProperties, 1, CHR(10)), 0, CHR(13))
		*!*	m.cProperty = GETWORDNUM(m.lcProperties, 1, CHR(10))
		*!*	DO WHILE !EMPTY(m.cProperty)
		*!*		m.nEqualPos = AT_C(' = ', m.cProperty)
		*!*		IF m.nEqualPos > 3
		*!*			IF SUBSTR(m.cProperty, m.nEqualPos + 3, 517) == REPLICATE(CHR(1), 517)
		*!*				* we have an extended propererty
		*!*				m.cPropertyName = LEFTC(m.cProperty, m.nEqualPos - 1)
		
		*!*				m.nLen = VAL(SUBSTR(m.cProperty, m.nEqualPos + 3 + 517, 8))
		
		*!*				m.cProperty = m.cPropertyName + " = " + SUBSTR(m.cProperty, m.nEqualPos + 3 + 517 + 8, nLen)
		*!*				m.lcProperties = SUBSTR(m.lcProperties, m.nEqualPos + 5 + 517 + 8 + nLen)
		*!*			ELSE
		*!*				m.cProperty = RTRIM(m.cProperty, 0, CHR(13))
		
		*!*				m.nPos = AT(CHR(10), m.lcProperties)
		*!*				IF m.nPos = 0 
		*!*					m.nPos = AT(CHR(13), m.lcProperties)
		*!*				ENDIF
		*!*				IF m.nPos > 0
		*!*					m.lcProperties = SUBSTR(m.lcProperties, m.nPos + 1)
		*!*				ELSE
		*!*					m.lcProperties = ''
		*!*				ENDIF
		*!*			ENDIF
		*!*			
		*!*			m.nLineCnt = m.nLineCnt + 1
		*!*			DIMENSION aPropList[m.nLineCnt]
		*!*			aPropList[m.nLineCnt] = m.cProperty
		*!*			
		*!*			* m.cProperty = RTRIM(GETWORDNUM(m.lcProperties, 1, CHR(10)), 0, CHR(13))
		*!*			m.cProperty = GETWORDNUM(m.lcProperties, 1, CHR(10))
		*!*		ELSE
		*!*			m.cProperty = ''
		*!*		ENDIF
		*!*	ENDDO
		
		m.cProperty = m.lcProperties
		DO WHILE !EMPTY(m.cProperty)
			m.nEqualPos = AT_C(' = ', m.cProperty)
			IF m.nEqualPos > 3
				m.cPropertyName = LEFTC(m.cProperty, m.nEqualPos - 1)
				IF SUBSTR(m.cProperty, m.nEqualPos + 3, 517) == REPLICATE(CHR(1), 517)
					* we have an extended property
					m.nLen = VAL(SUBSTR(m.cProperty, m.nEqualPos + 3 + 517, 8))
		
					m.cPropertyValue = SUBSTR(m.cProperty, m.nEqualPos + 3 + 517 + 8, nLen)
		
					m.cProperty = SUBSTR(m.cProperty, m.nEqualPos + 3 + 518 + 8 + nLen + 1)
		
				ELSE
					nPos1 = AT(CHR(13), m.cProperty)
					nPos2 = AT(CHR(10), m.cProperty)
					DO CASE
					CASE nPos1 == 0 AND nPos2 == 0
						m.nPos = 0
					CASE nPos1 == 0
						m.nPos = m.nPos1 + 1
					CASE nPos2 == 0
						m.nPos = m.nPos2 + 1
					OTHERWISE
						m.nPos = MIN(m.nPos1, m.nPos2) + 1
					ENDCASE
					IF m.nPos > 0
						m.cPropertyValue = RTRIM(RTRIM(SUBSTR(m.cProperty, m.nEqualPos + 3, m.nPos - (m.nEqualPos + 2)), 0, CHR(10)), 0, CHR(13))
						m.cProperty = RTRIM(RTRIM(SUBSTR(m.cProperty, m.nPos), 0, CHR(10)), 0, CHR(13))
					ELSE
						m.cPropertyValue = SUBSTR(m.cProperty, m.nEqualPos + 3)
						m.cProperty = ''
					ENDIF
				ENDIF
				m.cProperty = LTRIM(LTRIM(m.cProperty, 0, CHR(13)), 0, CHR(10))
				m.nLineCnt = m.nLineCnt + 1
				DIMENSION aPropList[m.nLineCnt]
				aPropList[m.nLineCnt] = m.cPropertyName + " = " + m.cPropertyValue
		
			ELSE
				m.cProperty = ''
			ENDIF
		ENDDO
		
		
		*!*	_mline=0
		*!*	FOR lnCount = 1 TO MEMLINE(lcProperties)
		*!*		lcProperty=ALLTRIM(MLINE(lcProperties,1,_mline))
		FOR lnCount = 1 TO m.nLineCnt
			lcProperty = aPropList[lnCount]
			
			IF LEN(lcProperty) == 0
				lcProperties2=lcProperties2+CR_LF
				LOOP
			ENDIF
			IF RIGHT(lcProperty,1)=="="
				lcProperties2=lcProperties2+lcProperty+" "+CHR(34)+CHR(34)+CR_LF
				LOOP
			ENDIF
			lcStrData="color = "
			lnStrLen=LEN(lcStrData)
			lnAtPos=ATC(lcStrData,lcProperty)
			IF lnAtPos>0 AND OCCURS(",",lcProperty)=2
				IF SUBSTR(lcProperty,lnAtPos+lnStrLen,1)=="("
					lcProperties2=lcProperties2+lcProperty+CR_LF
					LOOP
				ENDIF
				lcProperties2=lcProperties2+LEFT(lcProperty,lnAtPos+lnStrLen-1)+ ;
						"RGB("+SUBSTR(lcProperty,lnAtPos+lnStrLen, ;
						LEN(lcProperty)-lnAtPos)+")"+CR_LF
				LOOP
			ENDIF
			lcStrData="picture = "
			lnStrLen=LEN(lcStrData)
			lnAtPos=ATC(lcStrData,lcProperty)
			IF lnAtPos>0
				IF SUBSTR(lcProperty,lnAtPos+lnStrLen,1)=="("
					lcProperties2=lcProperties2+lcProperty+CR_LF
					LOOP
				ENDIF
				lcProperties2=lcProperties2+LEFT(lcProperty,lnAtPos+lnStrLen-1)+ ;
						CHR(34)+SUBSTR(lcProperty,lnAtPos+lnStrLen, ;
						LEN(lcProperty)-lnAtPos)+CHR(34)+CR_LF
				LOOP
			ENDIF
			lcStrData="icon = "
			lnStrLen=LEN(lcStrData)
			lnAtPos=ATC(lcStrData,lcProperty)
			IF lnAtPos>0
				IF SUBSTR(lcProperty,lnAtPos+lnStrLen,1)=="("
					lcProperties2=lcProperties2+lcProperty+CR_LF
					LOOP
				ENDIF
				lcProperties2=lcProperties2+LEFT(lcProperty,lnAtPos+lnStrLen-1)+ ;
						CHR(34)+SUBSTR(lcProperty,lnAtPos+lnStrLen, ;
						LEN(lcProperty)-lnAtPos)+CHR(34)+CR_LF
				LOOP
			ENDIF
			
			lcStrData="database = "
			lnStrLen=LEN(lcStrData)
			lnAtPos=ATC(lcStrData,lcProperty)
			IF lnAtPos>0
				IF SUBSTR(lcProperty,lnAtPos+lnStrLen,1)=="("
					lcProperties2=lcProperties2+lcProperty+CR_LF
					LOOP
				ENDIF
				lcProperties2=lcProperties2+LEFT(lcProperty,lnAtPos+lnStrLen-1)+ ;
						CHR(34)+SUBSTR(lcProperty,lnAtPos+lnStrLen, ;
						LEN(lcProperty)-lnAtPos)+CHR(34)+CR_LF
				LOOP
			ENDIF
			
			
			
			lcStrData="= "
			lnStrLen=LEN(lcStrData)
			lnAtPos=ATC(lcStrData,lcProperty)
			lcStrData=ALLTRIM(SUBSTR(lcProperty,lnAtPos+lnStrLen))
			lcLeftChar=LEFT(lcStrData,1)
		
			IF lnAtPos>0 AND NOT lcLeftChar==CHR(34) AND ;
					(NOT lcLeftChar=="{" OR NOT RIGHT(lcStrData,1)=="}") AND ;
					NOT lcLeftChar=="." AND NOT lcLeftChar=="(" AND ;
					(EMPTY(lcLeftChar) OR CHR(13) $ lcStrData OR BETWEEN(LOWER(lcLeftChar),"a","z") OR ;
					NOT UPPER(lcStrData)==LOWER(CHRTRAN(lcStrData,lcBadChars,lcNewChars)))
				lcSubStr=ALLTRIM(SUBSTR(lcProperty,lnAtPos+lnStrLen, ;
								LEN(lcProperty)-lnAtPos))
		
				DO CASE
					CASE LEFT(lcSubStr,1)=CHR(34) AND RIGHT(lcSubStr,1)==CHR(34)
						cDelimStart = '['
						cDelimEnd   = ']'
					CASE LEFT(lcSubStr,1)="[" AND RIGHT(lcSubStr,1)=="]"
						cDelimStart = CHR(34)
						cDelimEnd   = CHR(13)
					CASE CHR(34)$lcStrData
						cDelimStart = '['
						cDelimEnd   = ']'
					OTHERWISE
						cDelimStart = CHR(34)
						cDelimEnd   = CHR(34)
				ENDCASE
		
				nCnt = ALINES(aMultiLine, lcSubStr)
				IF m.nCnt = 1
					lcNewStr = cDelimStart + aMultiLine[1] + cDelimEnd
				ELSE
					lcNewStr = ''
					FOR m.i = 1 TO m.nCnt
						lcNewStr = lcNewStr + ;
						 IIF(LEN(aMultiLine[m.i]) == 0, IIF(m.i == 1, "CHR(13)", ''), IIF(m.i == 1, '', " + ") + cDelimStart + aMultiLine[m.i] + cDelimEnd) +;
						 IIF(m.i == m.nCnt, '', " + CHR(13)")
					ENDFOR
				ENDIF
		*!*				DO CASE
		*!*					CASE LEFT(lcSubStr,1)=CHR(34) AND RIGHT(lcSubStr,1)==CHR(34)
		*!*						lcNewStr="["+lcSubStr+"]"
		*!*					CASE LEFT(lcSubStr,1)="[" AND RIGHT(lcSubStr,1)=="]"
		*!*						lcNewStr=CHR(34)+lcSubStr+CHR(34)
		*!*					CASE CHR(34)$lcStrData
		*!*						lcNewStr="["+lcSubStr+"]"
		*!*					OTHERWISE
		*!*						lcNewStr=CHR(34)+lcSubStr+CHR(34)
		*!*				ENDCASE
		
		
				lcProperties2=lcProperties2+LEFT(lcProperty,lnAtPos+lnStrLen-1)+ ;
						lcNewStr+CR_LF
				LOOP
			ENDIF
			lcProperties2=lcProperties2+lcProperty+CR_LF
		ENDFOR
		
		lcProperties=lcProperties2
		IF tlAddObjectMode
			lcProperties=STRTRAN(lcProperties,CR_LF,", ;"+CR_LF)
			DO WHILE RIGHT(lcProperties,5)==(", ;"+CR_LF)
				lcProperties=TRIM(LEFT(lcProperties,LEN(lcProperties)-5))
			ENDDO
		ENDIF
		RETURN this.IndentText(lcProperties,1)
		
	ENDPROC

	PROCEDURE getfile
		LPARAMETERS tcFileExt,tlAutoExtDialog
		LOCAL lcFileExt
		LOCAL laGetClass[1]
		
		IF EMPTY(tcFileExt) OR TYPE("tcFileExt")#"C"
			IF this.lBrowser
				lcFileExt=this.cGetFileExt
				IF NOT tlAutoExtDialog AND NOT EMPTY(this.cLastGetFileExt)
					lcFileExt=this.cLastGetFileExt+STRTRAN(";"+lcFileExt,";"+this.cLastGetFileExt,"")
				ENDIF
			ELSE
				lcFileExt=this.cCatalogFileExt
			ENDIF
		ELSE
			lcFileExt=LOWER(ALLTRIM(tcFileExt))
		ENDIF
		IF lcFileExt=="*"
			lcFileExt=""
		ENDIF
		DO CASE
			CASE NOT tlAutoExtDialog
				lcFileName=LOWER(GETFILE(lcFileExt))
				IF this.lBrowser
					this.cLastGetFileExt=JUSTEXT(lcFileName)
				ENDIF
				RETURN lcFileName
			CASE LEFT(lcFileExt,3)=="vcx"
				IF NOT AGETCLASS(laGetClass)
					RETURN ""
				ENDIF
				RETURN laGetClass[1]+","+laGetClass[2]
			CASE AT("bmp",lcFileExt)>0 OR AT("ico",lcFileExt)>0 OR ;
					AT("cur",lcFileExt)>0 OR AT("ani",lcFileExt)>0 OR ;
					AT("jpg",lcFileExt)>0  OR AT("gif",lcFileExt)>0 OR ;
					AT("dib",lcFileExt)>0
				RETURN LOWER(GETPICT(lcFileExt))
		ENDCASE
		RETURN LOWER(GETFILE(lcFileExt))
		
	ENDPROC

	PROCEDURE getfileaddress
		LPARAMETERS tlNoShellExecute,tcFileExt
		LOCAL lvResult
		
		this.vResult=.T.
		DO brwGetFileAddress IN browser WITH (this),(tlNoShellExecute),(tcFileExt)
		lvResult=this.vResult
		this.vResult=.T.
		RETURN lvResult
		
	ENDPROC

	PROCEDURE getfolder
		LPARAMETERS tcID,tlGetFolderOnly,tlGetObjectOnly,tlGetViewOnly
		LOCAL lcID,lcID2,oFolder,lcText,lnCount
		
		IF this.nFolderCount<1
			RETURN .NULL.
		ENDIF
		IF TYPE("tcID")=="O" AND NOT ISNULL(tcID) AND NOT tlGetObjectOnly AND NOT tlGetViewOnly
			IF TYPE("tcID.Parent")#"O"
				RETURN this.GetItem(tcID)
			ENDIF
			IF TYPE("tcID.Index")#"N" OR NOT BETWEEN(tcID.Index,1,this.nFolderCount)
				RETURN .NULL.
			ENDIF
			RETURN this.aFolderList[tcID.Index]
		ENDIF
		IF TYPE("tcID")#"C" OR EMPTY(tcID)
			RETURN .NULL.
		ENDIF
		SET DATASESSION TO (this.DataSessionID)
		oFolder=.NULL.
		lcID=LOWER(ALLTRIM(tcID))
		FOR lnCount = 1 TO this.nFolderCount
			WITH this.aFolderList[lnCount]
				lcID2=lcID
				IF NOT "!"$lcID2
					lcID2=.cID2+"!"+lcID
				ENDIF
				lcText=LOWER(ALLTRIM(.cText))
				IF .cID==lcID2 OR (.lView AND (lcText==lcID OR ;
						STRTRAN(lcText," ","_")==STRTRAN(lcID," ","_")))
					IF (tlGetFolderOnly AND NOT .lFolder) OR ;
							(tlGetObjectOnly AND NOT .lObject) OR ;
							(tlGetViewOnly AND NOT .lView)
						LOOP
					ENDIF
					oFolder=this.aFolderList[lnCount]
					IF oFolder.lDeleted
						oFolder=.NULL.
					ENDIF
					EXIT
				ENDIF
			ENDWITH
		ENDFOR
		IF ISNULL(oFolder) AND "!"$lcID
			lcID=ALLTRIM(SUBSTR(lcID,AT("!",lcID)+1))
			oFolder=this.GetFolder(lcID,tlGetFolderOnly,tlGetObjectOnly,tlGetViewOnly)
		ENDIF
		RETURN oFolder
		
	ENDPROC

	PROCEDURE getimageindex
		LPARAMETERS tlTreeView,tcKey
		LOCAL lcKey,lcFileName,oImageList,lnIndex,lnImageCount,llError,llIgnoreErrors
		
		IF EMPTY(tcKey)
			RETURN 0
		ENDIF
		oImageList=IIF(tlTreeView,this.oleImageList,this.oleSmallImages)
		lcKey=LOWER(ALLTRIM(tcKey))
		lcFileName=lcKey
		IF tlTreeView
			lnIndex=ASCAN(this.aImageList,lcFileName+" ")
			IF lnIndex>0
				RETURN lnIndex
			ENDIF
			lnIndex=ASCAN(this.aImageList,JUSTFNAME(lcFileName)+" ")
			IF lnIndex>0
				RETURN lnIndex
			ENDIF
			lcFileName=LOWER(FULLPATH(lcFileName))
			lnIndex=ASCAN(this.aImageList,lcFileName+" ")
			IF lnIndex>0
				RETURN lnIndex
			ENDIF
		ELSE
			lnIndex=ASCAN(this.aSmallImages,lcFileName+" ")
			IF lnIndex>0
				RETURN lnIndex
			ENDIF
			lnIndex=ASCAN(this.aSmallImages,JUSTFNAME(lcFileName)+" ")
			IF lnIndex>0
				RETURN lnIndex
			ENDIF
			lcFileName=LOWER(FULLPATH(lcFileName))
			lnIndex=ASCAN(this.aSmallImages,lcFileName+" ")
			IF lnIndex>0
				RETURN lnIndex
			ENDIF
		ENDIF
		IF NOT FILE(lcFileName)
			RETURN 0
		ENDIF
		llError=this.lError
		this.lError=.F.
		llIgnoreErrors=this.lIgnoreErrors
		this.lIgnoreErrors=.T.
		oImageList.object.ListImages.Add(,lcFileName,LoadPicture(lcFileName))
		IF NOT tlTreeView
			this.oleNormalImages.object.ListImages.Add(,lcFileName,LoadPicture(lcFileName))
		ENDIF
		this.lIgnoreErrors=llIgnoreErrors
		IF this.lError
			this.lError=llError
			RETURN 0
		ENDIF
		this.lError=llError
		lnImageCount=oImageList.object.ListImages.Count
		IF tlTreeView
			DIMENSION this.aImageList[lnImageCount]
			this.aImageList[lnImageCount]=lcFileName+" "
		ELSE
			DIMENSION this.aSmallImages[lnImageCount]
			this.aSmallImages[lnImageCount]=lcFileName+" "
		ENDIF
		RETURN lnImageCount
		
	ENDPROC

	PROCEDURE getitem
		LPARAMETERS tcID
		LOCAL lcID,lcID2,oItem,oItem2,oFolder,lnCount,lnLastSelect,lnRecNo,lnIndex
		
		IF this.nFolderCount<1
			RETURN .NULL.
		ENDIF
		IF TYPE("tcID")=="O" AND NOT ISNULL(tcID)
			IF TYPE("tcID.Parent")=="O"
				RETURN this.GetFolder(tcID)
			ENDIF
			IF TYPE("tcID.Index")#"N" OR NOT BETWEEN(tcID.Index,1,this.oFolder.nItemCount)
				RETURN .NULL.
			ENDIF
			lnIndex=this.oFolder.aItemIndexes[tcID.Index]
			IF NOT BETWEEN(lnIndex,1,this.nItemCount)
				RETURN .NULL.
			ENDIF
			RETURN this.aItemList[lnIndex]
		ENDIF
		IF TYPE("tcID")#"C" OR EMPTY(tcID)
			RETURN .NULL.
		ENDIF
		SET DATASESSION TO (this.DataSessionID)
		lnLastSelect=SELECT()
		lnRecNo=IIF(EOF() OR RECNO()>RECCOUNT(),1,RECNO())
		oItem=.NULL.
		lcID=LOWER(ALLTRIM(tcID))
		FOR lnCount = 1 TO this.nItemCount
			oItem2=this.aItemList[lnCount]
			IF VARTYPE(oItem2)#"O"
				LOOP
			ENDIF
			lcID2=lcID
			IF NOT "!"$lcID2
				lcID2=oItem2.cID2+"!"+lcID
			ENDIF
			IF oItem2.cID==lcID2
				oItem=oItem2
				IF oItem.lDeleted OR oItem.lFolderItem
					oItem=.NULL.
				ENDIF
				EXIT
			ENDIF
		ENDFOR
		IF ISNULL(oItem)
			FOR lnCount = 1 TO this.nItemCount
				oItem2=this.aItemList[lnCount]
				IF VARTYPE(oItem2)#"O"
					LOOP
				ENDIF
				IF LOWER(ALLTRIM(oItem2.cText))==lcID
					oItem=oItem2
					IF oItem.lDeleted OR oItem.lFolderItem
						oItem=.NULL.
					ENDIF
					EXIT
				ENDIF
			ENDFOR
		ENDIF
		IF NOT ISNULL(oItem)
			RETURN oItem
		ENDIF
		IF NOT USED(this.cGallery)
			RETURN .NULL.
		ENDIF
		SELECT (this.cGallery)
		LOCATE FOR LOWER(ALLTRIM(ID))==lcID
		IF NOT EOF() AND UPPER(ALLTRIM(Type))=="ITEM"
			oFolder=this.GetFolder(Parent,.T.)
			IF NOT ISNULL(oFolder) AND oFolder.nItemCount=-1
				this.RefreshObjects(oFolder)
				IF oFolder.nItemCount>0
					FOR lnCount = 1 TO this.nItemCount
						oItem2=this.aItemList[lnCount]
						IF VARTYPE(oItem2)#"O"
							LOOP
						ENDIF
						lcID2=lcID
						IF NOT "!"$lcID2
							lcID2=oItem2.cID2+"!"+lcID
						ENDIF
						IF this.aItemList[lnCount].cID==lcID2
							oItem=oItem2
							IF oItem.lDeleted
								oItem=.NULL.
							ENDIF
							EXIT
						ENDIF
					ENDFOR
				ENDIF
			ENDIF
			oFolder=.NULL.
		ENDIF
		IF ISNULL(oItem)
			oItem=this.GetView(tcID)
			IF ISNULL(oItem)
				oItem=this.GetObject(tcID)
			ENDIF
		ENDIF
		SELECT (this.cGallery)
		IF lnRecNo=0
			LOCATE
			IF NOT EOF()
				GO BOTTOM
				SKIP
			ENDIF
		ELSE
			GO lnRecNo
		ENDIF
		SELECT (lnLastSelect)
		RETURN oItem
		
	ENDPROC

	PROCEDURE getlineno
		* Return the line no that a class or method occurs on
		* (PRG support)
		LPARAMETERS cPEMName
		LOCAL lnLastSelect
		LOCAL lnLineNo
		LOCAL i
		LOCAL nCnt
		
		lnLastSelect=SELECT()
		this.RefreshRecNo
		SELECT (this.cAlias)
		
		IF VARTYPE(cPEMName) <> 'C' OR EMPTY(cPEMName)
			lnLineNo = LineNo
		ELSE
			lnLineNo = 0
			cPEMName = LOWER(cPEMName)
		
			nCnt = ALINES(aLineNo, PEMLineNo, .T.)
			FOR i = 1 TO nCnt
				IF LOWER(GETWORDNUM(aLineNo[i], 1, '=')) == cPEMName
					lnLineNo = VAL(GETWORDNUM(aLineNo[i], 2, '='))
					EXIT
				ENDIF
			ENDFOR
		ENDIF
		
		SELECT (lnLastSelect)
		
		RETURN lnLineNo
	ENDPROC


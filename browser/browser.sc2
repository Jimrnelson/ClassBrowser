	PROCEDURE newclass
		LPARAMETERS tcClass,tcBaseClass
		LOCAL lcLastSetSafety,lcFileName
		
		IF NOT this.lVCXSCXMode OR NOT USED(this.cAlias) OR this.lSCXMode OR RECCOUNT((this.cAlias))=0
			RETURN .F.
		ENDIF
		lcLastSetSafety=SET("SAFETY")
		SET SAFETY ON
		lcFileName=this.cFileName
		IF EMPTY(tcClass) OR TYPE("tcClass")#"C"
			IF this.lFileMode
				CREATE CLASS ? OF (lcFileName) NOWAIT
			ELSE
				CREATE CLASS ? OF (lcFileName) AS (this.cClass) ;
						FROM (lcFileName) NOWAIT
			ENDIF
		ELSE
			DO CASE
				CASE NOT EMPTY(tcBaseClass) AND TYPE("tcBaseClass")=="C"
					CREATE CLASS (tcClass) OF (lcFileName) AS (tcBaseClass) NOWAIT
				CASE this.lFileMode
					CREATE CLASS (tcClass) OF (lcFileName) NOWAIT
				OTHERWISE
					CREATE CLASS (tcClass) OF (lcFileName) AS (this.cClass) ;
							FROM (lcFileName) NOWAIT
			ENDCASE
		ENDIF
		IF lcLastSetSafety=="ON"
			SET SAFETY ON
		ELSE
			SET SAFETY OFF
		ENDIF
		SELECT 0
		SET MESSAGE TO
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		
	ENDPROC

	PROCEDURE newfile
		LPARAMETERS tcFileName,tlOpenFile
		LOCAL lcFileName,lcLastSetSafety
		
		IF NOT this.lInitialized OR EMPTY(tcFileName) OR TYPE("tcFileName")#"C"
			RETURN .F.
		ENDIF
		lcFileName=LOWER(FULLPATH(ALLTRIM(tcFileName)))
		IF FILE(lcFileName) OR NOT RIGHT(lcFileName,4)==".vcx"
			RETURN .F.
		ENDIF
		lcLastSetSafety=SET("SAFETY")
		SET SAFETY ON
		CREATE CLASSLIB (lcFileName)
		IF lcLastSetSafety=="ON"
			SET SAFETY ON
		ELSE
			SET SAFETY OFF
		ENDIF
		SET DATASESSION TO this.DataSessionID
		IF NOT FILE(lcFileName)
			RETURN .F.
		ENDIF
		IF NOT tlOpenFile
			RETURN
		ENDIF
		SET MESSAGE TO
		this.OpenFile(lcFileName)
		SELECT 0
		
	ENDPROC


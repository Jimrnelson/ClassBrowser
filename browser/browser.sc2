	PROCEDURE refreshprefrecno
		LPARAMETERS tcName
		LOCAL lnRecNo,ltUpdated,laRecord,lcFontInfo,lcFileName
		
		SET DATASESSION TO this.DataSessionID
		IF NOT USED("browser")
			RETURN .F.
		ENDIF
		DO CASE
			CASE NOT EMPTY(tcName)
				lcFileName=LOWER(ALLTRIM(tcName))
			CASE this.lBrowser
				lcFileName=LOWER(this.cFileName)
			OTHERWISE
				lcFileName=LOWER(this.cCatalog)
		ENDCASE
		IF RIGHT(lcFileName,4)==".tmp"
			lcFileName=""
		ENDIF
		SELECT browser
		IF EMPTY(lcFileName)
			GO (this.nPrefRecNo)
		ELSE
			LOCATE FOR UPPER(ALLTRIM(Platform))==this.cPlatform AND ;
					UPPER(ALLTRIM(Type))=="PREFW" AND ;
					UPPER(ALLTRIM(ID))=="FORMINFO" AND ;
					LOWER(ALLTRIM(Name))==LOWER(lcFileName)
		ENDIF
		IF NOT EOF()
			SELECT 0
			RETURN
		ENDIF
		lnRecNo=0
		ltUpdated={ }
		lcFontInfo=""
		SET DATASESSION TO this.DataSessionID
		SCAN ALL FOR UPPER(ALLTRIM(Platform))==this.cPlatform AND ;
				UPPER(ALLTRIM(Type))=="PREFW" AND ;
				UPPER(ALLTRIM(ID))=="FORMINFO"
			IF Updated>=ltUpdated
				lnRecNo=RECNO()
				ltUpdated=Updated
				lcFontInfo=ALLTRIM(MLINE(FontInfo,1))
			ENDIF
		ENDSCAN
		IF lnRecNo=0
			INSERT INTO browser (Platform, Type, ID, Name, DescBoxes, AutoExpand, FontInfo) ;
					VALUES (this.cPlatform, "PREFW", "FORMINFO", ;
					lcFileName, this.lDescriptions, this.lAutoExpand, lcFontInfo)
		ELSE
			GO lnRecNo
			DIMENSION laRecord[1]
			SCATTER TO laRecord
			APPEND BLANK
			GATHER FROM laRecord
			REPLACE Name WITH lcFileName, ;
					Default WITH .F., ;
					Global WITH .F., ;
					WindowStat WITH 0, ;
					Updated WITH DATETIME()
		ENDIF
		this.SetFont(.NULL.)
		IF NOT this.lInitialized AND NOT EMPTY(lcFontInfo)
			this.SetFont(lcFontInfo)
		ENDIF
		SELECT 0
		
	ENDPROC

	PROCEDURE refreshrecno
		LOCAL lcClass,lnRecNo,lnLastRecNo,llRefresh,lnLastSelect,lcProgram,lnCount
		
		SET DATASESSION TO this.DataSessionID
		IF this.lRelease OR NOT this.lBrowser OR this.nClassListIndex<0
			RETURN .F.
		ENDIF
		lnLastSelect=SELECT()
		lcClass=LOWER(ALLTRIM(this.aClassList[this.nClassListIndex+1,1]))
		IF "."$lcClass
			IF USED(this.cAlias)
				SELECT (this.cAlias)
				GO TOP
				this.nClassTimeStamp=TimeStamp
			ELSE
				this.nClassTimeStamp=0
			ENDIF
			this.cClass=lcClass
			this.lFileMode=("."$lcClass)
			this.cParentClass=""
			this.cClassLibrary=""
			this.cBaseClass=""
			SELECT (lnLastSelect)
			RETURN
		ENDIF
		IF NOT USED(this.cAlias)
			SELECT (lnLastSelect)
			RETURN
		ENDIF
		SELECT (this.cAlias)
		lnLastRecNo=RECNO()
		lnRecNo=this.aClassList[this.nClassListIndex+1,2]
		llRefresh=(lnRecNo>RECCOUNT())
		IF llRefresh
			LOCATE FOR LOWER(ALLTRIM(ObjName))==lcClass
			IF NOT EOF()
				llRefresh=.F.
				lnRecNo=RECNO()
				this.aClassList[this.nClassListIndex+1,2]=lnRecNo
			ENDIF
		ENDIF
		IF NOT llRefresh
			GO lnRecNo
			IF DELETED() OR NOT LOWER(ALLTRIM(ObjName))==lcClass
				this.RefreshClassList(lcClass)
				this.RefreshMembers
				SELECT (lnLastSelect)
				RETURN .F.
			ENDIF
		ENDIF
		IF llRefresh
			SELECT 0
			lnCount=1
			DO WHILE .T.
				lcProgram=UPPER(PROGRAM(lnCount))
				IF EMPTY(lcProgram)
					EXIT
				ENDIF
				IF ".REFRESHMEMBERS"$lcProgram OR ".REFRESHCLASSLIST"$lcProgram
					SELECT (lnLastSelect)
					RETURN .F.
				ENDIF
				lnCount=lnCount+1
			ENDDO
			this.cClass=lcClass
			this.RefreshBrowser
			SELECT (lnLastSelect)
			RETURN .F.
		ENDIF
		IF DELETED()
			SELECT (this.cAlias)
			LOCATE FOR LOWER(ALLTRIM(ObjName))==lcClass
			IF EOF()
				SELECT 0
				this.cClass=lcClass
				this.RefreshBrowser
				SELECT (lnLastSelect)
				RETURN .F.
			ENDIF
		ENDIF
		this.nClassTimeStamp=TimeStamp
		this.cClass=lcClass
		this.lFileMode=("."$lcClass)
		this.cParentClass=LOWER(ALLTRIM(Class))
		this.cClassLibrary=LOWER(ALLTRIM(ClassLoc))
		IF NOT EMPTY(this.cClassLibrary)
			this.cClassLibrary=LOWER(FULLPATH(this.cClassLibrary,this.cFileName))
		ENDIF
		this.cBaseClass=LOWER(ALLTRIM(BaseClass))
		SELECT (lnLastSelect)
		
	ENDPROC


	PROCEDURE shpSplitterH.MouseDown
		LPARAMETERS nButton, nShift, nXCoord, nYCoord
		LOCAL lcWindowName,lnScaleMode,lnMinRow,lnMaxRow,lnMRow1,lnMRow2,lnRows,lnTop,lnOldTop
		LOCAL llBrowser,llVisible1,llVisible2,llLockScreen
		LOCAL oTopPane1,oTopPane2,oBottomPane
		
		thisform.lMouseDown=.T.
		thisform.nMouseButton=IIF(TYPE("nButton")=="N",nButton,0)
		thisform.nShift=nShift
		IF nButton#1
			RETURN
		ENDIF
		lcWindowName=thisform.Name
		lnScaleMode=thisform.ScaleMode
		lnMRow1=MROW(lcWindowName,lnScaleMode)
		IF TYPE("lnMRow1")#"N" OR lnMRow1<=0
			RETURN
		ENDIF
		llBrowser=thisform.lBrowser
		llLockScreen=thisform.LockScreen
		DO CASE
			CASE NOT thisform.lOutlineOCX
				oTopPane1=thisform.lstClassList
				oTopPane2=thisform.lstMembers
				oBottomPane=thisform.edtClassDesc
			CASE llBrowser
				oTopPane1=thisform.oleClassList
				oTopPane2=thisform.oleMembers
				oBottomPane=thisform.edtClassDesc
			OTHERWISE
				oTopPane1=thisform.oleFolderList
				oTopPane2=thisform.oleItems
				oBottomPane=thisform.edtItemDesc
		ENDCASE
		llVisible1=oTopPane1.Visible
		llVisible2=oTopPane2.Visible
		IF thisform.lOutlineOCX
			oTopPane1.Visible=.F.
			oTopPane2.Visible=.F.
		ENDIF
		IF thisform.lWebView
			thisform.oleWebBrowser.Visible=.F.
		ENDIF
		lnTop=this.Top
		lnOldTop=oBottomPane.Top
		lnMinRow=oTopPane1.Top+(this.Height-1)+32
		lnMaxRow=oBottomPane.Top+oBottomPane.Height-50
		lnMRow2=lnMRow1
		this.FillStyle=0
		this.ZOrder(0)
		DO WHILE MDOWN()
			DOEVENTS
			thisform.Draw
			lnMRow2=MROW(lcWindowName,lnScaleMode)
			IF TYPE("lnMRow2")#"N" OR lnMRow2=0
				LOOP
			ENDIF
			IF lnMRow2<=lnMinRow
				lnMRow2=MROW("",3)
				IF lnMRow2<=0
					LOOP
				ENDIF
				lnMRow2=IIF(lnMRow2>=thisform.Top+thisform.Height,lnMaxRow,lnMinRow)
			ENDIF
			lnMRow2=MIN(MAX(lnMRow2,lnMinRow),lnMaxRow)
			IF lnMRow2=lnMRow1 OR lnMRow2<0
				LOOP
			ENDIF
			this.Top=lnTop+(lnMRow2-lnMRow1)
		ENDDO
		this.FillStyle=1
		oTopPane1.Visible=llVisible1
		oTopPane2.Visible=llVisible2
		IF thisform.lWebView
			thisform.oleWebBrowser.Visible=.T.
		ENDIF
		IF lnMRow2=lnMRow1
			RETURN
		ENDIF
		IF lnMRow2<0
			lnMRow2=lnMRow1
		ENDIF
		lnRows=lnMRow2-lnMRow1+oBottomPane.Top
		lnRows=MIN(MAX(lnRows,lnMinRow),lnMaxRow)-oBottomPane.Top
		WITH thisform
			.LockScreen=.T.
			oTopPane1.Height=oTopPane1.Height+lnRows
			.txtClassList3D.Height=oTopPane1.Height
			oTopPane2.Height=oTopPane1.Height
			.txtMembers3D.Height=oTopPane2.Height
			.shpSplitterV.Height=.txtMembers3D.Height
			oBottomPane.Top=oBottomPane.Top+lnRows
			oBottomPane.Height=oBottomPane.Height-lnRows
			DO CASE
				CASE llBrowser
					.edtMemberDesc.Top=oBottomPane.Top
					.edtMemberDesc.Height=oBottomPane.Height
				CASE NOT llBrowser AND .lWebBrowser
					.oleWebBrowser.Top=oTopPane2.Top
					.oleWebBrowser.Height=oTopPane2.Height
			ENDCASE
			this.Top=lnTop+(oBottomPane.Top-lnOldTop)
			.MinHeight=IIF(.lBrowser,.edtClassDesc.Height,.edtItemDesc.Height)+.nMinHeightOffset
			.LockScreen=llLockScreen
			.SavePreferences
		ENDWITH
		
	ENDPROC


	PROCEDURE removeclass
		LPARAMETERS tlConfirm
		LOCAL lcFileName,lcClass,lnListIndex,lcDefaultClass,lnFileNo,lnCount,lnResult
		
		IF NOT this.lBrowser
			RETURN .F.
		ENDIF
		lcFileName=this.cFileName
		lcClass=this.cClass
		IF "."$lcClass
			IF tlConfirm
				SET MESSAGE TO
				IF this.lVCXSCXMode
					lnResult=this.MsgBox(M_DELETE_REMOVE_LOC,547)
				ELSE
					lnResult=this.MsgBox(M_REMOVE_TYPELIB_LOC,292)
					IF lnResult=6
						lnResult=7
					ELSE
						lnResult=2
					ENDIF
				ENDIF
				IF lnResult#6 AND lnResult#7
					RETURN .F.
				ENDIF
				IF lnResult=6 AND FILE(lcFileName) AND ;
						this.MsgBox(M_DELETE_FILE_LOC+[ "]+lcFileName+["?],292)=6
					USE IN (this.cAlias)
					ERASE (lcFileName) RECYCLE
					IF RIGHT(lcFileName,1)=="x"
						ERASE (LEFT(lcFileName,LEN(lcFileName)-1)+"t") RECYCLE
					ENDIF
				ENDIF
			ENDIF
			lnCount=0
			FOR lnFileNo = 1 TO this.nFileCount
				IF NOT EMPTY(this.aFiles[lnFileNo])
					lnCount=lnCount+1
				ENDIF
			ENDFOR
			lnFileNo=IIF(lnCount>=2,ASCAN(this.aFiles,lcFileName),0)
			IF lnFileNo=0 OR this.nFileCount<=1
				this.ClearBrowser
				RETURN
			ENDIF
			this.UpdateFormCount(-1,lcFileName)
			SELECT 0
			this.aFiles[lnFileNo]=""
			this.RefreshBrowser
			RETURN
		ENDIF
		IF tlConfirm
			SET MESSAGE TO
			IF this.MsgBox(M_REMOVE_CLASS_LOC+[ (]+lcClass+[) ]+M_FROM_LOC+[ "]+ ;
						lcFileName+["?],292)#6
				RETURN .F.
			ENDIF
		ENDIF
		IF this.lOutlineOCX
			lnListIndex=this.oleClassList.object.SelectedItem.Index
		ELSE
			lnListIndex=this.lstClassList.ListIndex
		ENDIF
		DO CASE
			CASE lnListIndex<this.nClassCount
				lcDefaultClass=this.aClassList[lnListIndex+1,1]
			CASE lnListIndex>1
				lcDefaultClass=this.aClassList[lnListIndex-1,1]
			OTHERWISE
				lcDefaultClass=""
		ENDCASE
		this.ClearClass
		REMOVE CLASS (lcClass) OF (lcFileName)
		this.cClass=lcDefaultClass
		this.RefreshBrowser
		
	ENDPROC

	PROCEDURE renameclass
		LPARAMETERS tcToClass
		LOCAL oRenameForm,lcToClass,lcClass,llError,lcClassNameStr
		
		IF NOT this.lBrowser OR NOT this.lVCXSCXMode OR this.lFileMode OR this.lSCXMode
			RETURN .F.
		ENDIF
		lcClass=this.cClass
		IF EMPTY(lcClass)
			RETURN .F.
		ENDIF
		IF TYPE("tcToClass")=="C"
			lcToClass=LOWER(ALLTRIM(tcToClass))
		ELSE
			lcToClass=""
			this.lModalDialog=.T.
			DO FORM brwrenam NAME oRenameForm WITH (this),(lcClass),(lcClass) ;
					TO lcToClass
			SET DATASESSION TO this.DataSessionID
		ENDIF
		IF ISNULL(lcToClass) OR EMPTY(lcToClass)
			RETURN .F.
		ENDIF
		this.RefreshRecNo
		SELECT (this.cAlias)
		IF LOWER(ALLTRIM(ObjName))==LOWER(ALLTRIM(lcToClass))
			SELECT 0
			SET MESSAGE TO
			RETURN .F.
		ENDIF
		SET MESSAGE TO M_RENAMING_CLASS_LOC+" ..."
		llError=this.lError
		this.lError=.F.
		lcToClass=LOWER(ALLTRIM(lcToClass))
		this.ClearClass(.T.)
		IF INLIST(this.cBaseClass+" ","olecontrol ","oleboundcontrol ") AND ;
			LOWER(MLINE(ObjName,1))==lcClass
			lcClassNameStr=CR_LF+[Name = "]+lcClass+["]+CR_LF
			REPLACE ObjName WITH lcToClass, ;
					Properties WITH SUBSTR(STRTRAN(CR_LF+Properties,lcClassNameStr, ;
							CR_LF+[Name = "]+lcToClass+["]+CR_LF),3)
		ELSE
			RENAME CLASS (lcClass) OF (this.cFileName) TO (lcToClass)
		ENDIF
		IF this.lError
			this.lError=llError
			SELECT 0
			SET MESSAGE TO
			RETURN .F.
		ENDIF
		this.lError=llError
		this.UpdateReferences(this.cFileName,lcClass,this.cFileName,lcToClass,.T.)
		SELECT 0
		SET MESSAGE TO
		this.cClass=lcToClass
		this.RefreshBrowser
		this.ClearClass
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		
	ENDPROC

	PROCEDURE renamemember
		LPARAMETERS tcToMember
		LOCAL oRenameForm,lcToMember,lcMember,lcMemberType
		LOCAL lcMethods,lcProperties,lcProtected,lcReserved3,lnAtPos,lnCount,lnAddPos
		LOCAL llArray,llProtected,llHidden,llMatch,lnASCII,lnLastSelect
		
		IF NOT this.lBrowser OR NOT this.lVCXSCXMode OR this.lFileMode OR this.lSCXMode OR ;
				this.nMemberListIndex<1
			RETURN .F.
		ENDIF
		lcMember=this.aMemberList[this.nMemberListIndex,1]
		lcMemberType=this.aMemberList[this.nMemberListIndex,2]
		IF ISNULL(lcMember) OR EMPTY(lcMember) OR NOT INLIST(lcMemberType,"m","p")
			RETURN .F.
		ENDIF
		lcToMember=IIF(TYPE("tcToMember")=="C",LOWER(ALLTRIM(tcToMember)),"")
		llArray=.F.
		llProtected=.F.
		llHidden=.F.
		IF lcMemberType=="p"
			lnAtPos=AT("[",lcMember)
			IF lnAtPos>0
				llArray=.T.
				lcMember=LEFT(lcMember,lnAtPos-1)
			ENDIF
		ENDIF
		this.ClearClass
		IF EMPTY(lcToMember)
			this.lModalDialog=.T.
			DO FORM brwrenam NAME oRenameForm ;
					WITH this,(lcMember),(lcMember) TO lcToMember
			SET DATASESSION TO this.DataSessionID
		ENDIF
		IF ISNULL(lcToMember) OR EMPTY(lcToMember)
			RETURN
		ENDIF
		lcToMember=LOWER(ALLTRIM(lcToMember))
		IF lcMember==lcToMember
			RETURN
		ENDIF
		lnLastSelect=SELECT()
		this.RefreshRecNo
		SELECT (this.cAlias)
		lcReserved3=STRTRAN(STRTRAN(STRTRAN(Reserved3,CR_LF,CR), ;
				CR+CR,CR),CR,CR_LF)
		IF NOT RIGHT(lcReserved3,2)==CR_LF
			lcReserved3=lcReserved3+CR_LF
		ENDIF
		lcProtected=STRTRAN(STRTRAN(STRTRAN(Protected,CR_LF,CR), ;
				CR+CR,CR),CR,CR_LF)
		IF NOT RIGHT(lcProtected,2)==CR_LF
			lcProtected=lcProtected+CR_LF
		ENDIF
		IF BETWEEN(LEFT(lcToMember,1),"0","9") OR ;
				NOT lcToMember==CHRTRAN(lcToMember, ;
				' /\,-=:;{}[]!@#$%^&*.<>()+|'+CHR(34)+CHR(39),"")
			this.MsgBox(M_MEMBER_LOC+" "+M_NAME_LOC+[ "]+ ;
					lcToMember+[" ]+M_IS_INVALID_LOC+[.],16)
			SELECT (lnLastSelect)
			this.vResult=.F.
			RETURN this.vResult
		ENDIF
		llMatch=PEMSTATUS(this.cBaseClass,lcToMember,5)
		IF NOT llMatch
			FOR lnCount = 1 TO 3
				DO CASE
					CASE lnCount=1
						lnAtPos=ATC(CR_LF+"*"+lcToMember,CR_LF+lcReserved3)
						lnAddPos=1
					CASE lnCount=2
						lnAtPos=ATC(CR_LF+"^"+lcToMember,CR_LF+lcReserved3)
						lnAddPos=1
					OTHERWISE
						lnAtPos=ATC(CR_LF+lcToMember,CR_LF+lcReserved3)
						lnAddPos=0
				ENDCASE
				IF lnAtPos=0
					LOOP
				ENDIF
				lnASCII=ASC(SUBSTR(lcReserved3,lnAtPos+lnAddPos+ ;
						LEN(lcToMember),1))
				IF lnASCII=0 OR lnASCII=32 OR lnASCII=13 OR ;
						lnASCII=10 OR lnASCII=91
					llMatch=.T.
					EXIT
				ENDIF
			ENDFOR
		ENDIF
		IF NOT llMatch
			SKIP
			SCAN REST FOR LOWER(Parent)==this.cClass ;
					WHILE NOT EMPTY(Parent)
				IF LOWER(ObjName)==lcToMember
					llMatch=.T.
					EXIT
				ENDIF
			ENDSCAN
			this.RefreshRecNo
		ENDIF
		IF llMatch
			this.MsgBox(M_MEMBER_LOC+" "+M_NAME_LOC+[ "]+ ;
					lcToMember+[" ]+M_ALREADY_IN_USE_LOC+[.],16)
			SELECT (lnLastSelect)
			this.vResult=.F.
			RETURN this.vResult
		ENDIF
		llMatch=.F.
		lnCount=0
		DO WHILE .T.
			lnCount=lnCount+1
			DO CASE
				CASE lcMemberType=="m"
					lnAtPos=ATC(CR_LF+"*"+lcMember,CR_LF+lcReserved3,lnCount)
					lnAddPos=1
				CASE llArray
					lnAtPos=ATC(CR_LF+"^"+lcMember,CR_LF+lcReserved3,lnCount)
					lnAddPos=1
				OTHERWISE
					lnAtPos=ATC(CR_LF+lcMember,CR_LF+lcReserved3,lnCount)
					lnAddPos=0
			ENDCASE
			IF lnAtPos=0
				EXIT
			ENDIF
			lnASCII=ASC(SUBSTR(lcReserved3,lnAtPos+lnAddPos+LEN(lcMember),1))
			IF lnASCII=0 OR lnASCII=32 OR lnASCII=13 OR lnASCII=10 OR lnASCII=91
				llMatch=.T.
				EXIT
			ENDIF
		ENDDO
		IF NOT llMatch
			SELECT (lnLastSelect)
			this.vResult=.F.
			RETURN this.vResult
		ENDIF
		lcReserved3=LEFT(lcReserved3,lnAtPos-1+lnAddPos)+ ;
				lcToMember+SUBSTR(lcReserved3, ;
				lnAtPos+LEN(lcMember)+lnAddPos)
		REPLACE Reserved3 WITH lcReserved3
		IF NOT Reserved3==lcReserved3
			SELECT (lnLastSelect)
			this.RefreshMembers
			RETURN
		ENDIF
		lnAtPos=ATC(CR_LF+lcMember+CR_LF,CR_LF+lcProtected)
		IF lnAtPos>0
			llProtected=.T.
		ELSE
			lnAtPos=ATC(CR_LF+lcMember+"^"+CR_LF,CR_LF+lcProtected)
			IF lnAtPos>0
				llHidden=.T.
			ENDIF
		ENDIF
		IF lnAtPos>0
			lcProtected=LEFT(lcProtected,lnAtPos-1)+ ;
					lcToMember+SUBSTR(lcProtected,lnAtPos+LEN(lcMember))
			REPLACE Protected WITH lcProtected
		ENDIF
		IF lcMemberType=="m"
			lcMethods=Methods
			llMatch=.F.
			lnCount=0
			DO WHILE .T.
				lnCount=lnCount+1
				lnAtPos=ATC(CR_LF+"PROCEDURE "+lcMember,CR_LF+lcMethods,lnCount)
				IF lnAtPos=0
					EXIT
				ENDIF
				IF SUBSTR(lcMethods,lnAtPos+LEN(lcMember)+10,1)=CR
					llMatch=.T.
					EXIT
				ENDIF
			ENDDO
			IF NOT llMatch
				SELECT (lnLastSelect)
				this.RefreshMembers(lcToMember)
				RETURN
			ENDIF
			lcMethods=LEFT(lcMethods,lnAtPos+9)+lcToMember+ ;
					SUBSTR(lcMethods,lnAtPos+LEN(lcMember)+10)
			REPLACE Methods WITH lcMethods
			IF NOT Methods==lcMethods
				SELECT (lnLastSelect)
				this.RefreshMembers(lcToMember)
				RETURN
			ENDIF
			COMPILE FORM (this.cFileName)
		ELSE
			IF llArray OR lnAddPos>0
				SELECT (lnLastSelect)
				this.RefreshMembers(lcToMember)
				RETURN
			ENDIF
			lcProperties=STRTRAN(STRTRAN(STRTRAN(Properties,CR_LF,CR), ;
					CR+CR,CR),CR,CR_LF)
			IF NOT RIGHT(lcProperties,2)==CR_LF
				lcProperties=lcProperties+CR_LF
			ENDIF
			llMatch=.F.
			lnCount=0
			DO WHILE .T.
				lnCount=lnCount+1
				lnAtPos=ATC(CR_LF+lcMember,CR_LF+lcProperties,lnCount)
				IF lnAtPos=0
					EXIT
				ENDIF
				IF SUBSTR(lcProperties,lnAtPos+LEN(lcMember),2)==" ="
					llMatch=.T.
					EXIT
				ENDIF
			ENDDO
			IF NOT llMatch
				SELECT (lnLastSelect)
				this.RefreshMembers(lcToMember)
				RETURN
			ENDIF
			lcProperties=LEFT(lcProperties,lnAtPos-1)+lcToMember+ ;
					SUBSTR(lcProperties,lnAtPos+LEN(lcMember))
			REPLACE Properties WITH lcProperties
		ENDIF
		SELECT (lnLastSelect)
		this.RefreshMembers(lcToMember)
		
	ENDPROC

	PROCEDURE resetdefaultfile
		LPARAMETERS tlIgnoreAll
		LOCAL lnLastSelect
		
		lnLastSelect=SELECT()
		IF NOT this.RefreshPrefRecNo()
			RETURN .F.
		ENDIF
		SELECT browser
		IF EOF()
			SELECT (lnLastSelect)
			RETURN .F.
		ENDIF
		IF tlIgnoreAll
			REPLACE Default WITH .F.
		ELSE
			REPLACE ALL Default WITH .F.
		ENDIF
		SELECT (lnLastSelect)
		
	ENDPROC

	PROCEDURE resetdefaults
		LOCAL llRefresh,llLockScreen
		
		llRefresh=.F.
		llLockScreen=this.LockScreen
		this.LockScreen=.T.
		this.SetFont(.NULL.)
		IF this.AlwaysOnTop
			this.AlwaysOnTop=.F.
		ENDIF
		IF NOT this.lDescriptions
			this.lDescriptions=.T.
			this.RefreshDescriptions
		ENDIF
		IF NOT this.lAutoExpand
			this.lAutoExpand=.T.
			llRefresh=.T.
		ENDIF
		IF this.lBrowser
			IF this.nDisplayMode#1
				this.DisplayMode(1)
			ENDIF
			IF this.lProtectedFilter
				this.lProtectedFilter=.F.
				llRefresh=.T.
			ENDIF
			IF this.lEmptyFilter
				this.lEmptyFilter=.F.
				llRefresh=.T.
			ENDIF
			IF this.lHiddenFilter
				this.lHiddenFilter=.F.
				llRefresh=.T.
			ENDIF
			IF NOT this.lParentClassBrowser
				this.lParentClassBrowser=.T.
				this.RefreshParentClassBrowser
			ENDIF
		ELSE
			IF this.nImageView#0
				this.SetImageView(0)
			ENDIF
		ENDIF
		IF llRefresh
			this.RefreshBrowser
		ENDIF
		this.SavePreferences
		this.LockScreen=llLockScreen
		
	ENDPROC

	PROCEDURE Resize
		IF this.lRelease
			RETURN .F.
		ENDIF
		IF this.lResizeMode
			RETURN .F.
		ENDIF
		this.lResizeMode=.T.
		IF this.Height<this.MinHeight
			this.Height=this.MinHeight
		ENDIF
		IF this.Width<this.MinWidth
			this.Width=this.MinWidth
		ENDIF
		IF this.AddInMethod(PROGRAM())
			this.lResizeMode=.F.
			RETURN
		ENDIF
		this.ScaleResize
		this.lResizeMode=.F.
		
		
	ENDPROC

	PROCEDURE restorepreferences
		LOCAL lcProperties,lcMemLine,lnCount,lnLastSelect
		
		IF NOT USED("browser")
			SET DATASESSION TO this.DataSessionID
			IF NOT USED("browser")
				RETURN .F.
			ENDIF
		ENDIF
		lnLastSelect=SELECT()
		SELECT browser
		GO (this.nPrefRecNo)
		lcProperties=ALLTRIM(Properties)
		_mline=0
		FOR lnCount = 1 TO MEMLINES(lcProperties)
			lcMemLine=ALLTRIM(MLINE(lcProperties,1,_mline))
			IF EMPTY(lcMemLine)
				LOOP
			ENDIF
			&lcMemLine
		ENDFOR
		SELECT (lnLastSelect)
		
	ENDPROC

	PROCEDURE RightClick
		this.Show
		this.DisplayMenu
		
	ENDPROC

	PROCEDURE runcode
		LPARAMETERS tcCode,tlFile,tvIgnoreErrors
		LOCAL lnDataSessionID,lnLastSelect
		
		IF VARTYPE(tcCode)#"C" OR EMPTY(tcCode)
			RETURN .F.
		ENDIF
		lnLastSelect=SELECT()
		lnDataSessionID=this.DataSessionID
		SET DATASESSION TO (lnDataSessionID)
		ACTIVATE SCREEN
		DO c:\dev\browser\RunCode WITH (tcCode),(tlFile),(tvIgnoreErrors)
		SET DATASESSION TO (lnDataSessionID)
		SELECT (lnLastSelect)
		
	ENDPROC

	PROCEDURE runcodetimer
		LPARAMETERS tcCode
		
		IF VARTYPE(tcCode)#"C" OR EMPTY(tcCode)
			RETURN .F.
		ENDIF
		IF TYPE("this.tmrRunCodeTimer")#"O"
			this.NewObject("tmrRunCodeTimer","_RunCodeTimer","browser.vcx")
		ENDIF
		RETURN this.tmrRunCodeTimer.RunCode(tcCode)
	ENDPROC

	PROCEDURE runscript
		LPARAMETERS tvItem
		LOCAL lcID,oItem,lcScript
		
		DO CASE
			CASE ISNULL(tvItem)
				RETURN .F.
			CASE TYPE("tvItem")=="C"
				IF NOT ISNULL(this.oItem) AND LOWER(this.oItem.cID)==ALLTRIM(LOWER(tvItem))
					oItem=this.oItem
				ELSE
					oItem=this.GetItem(tvItem)
				ENDIF
				IF ISNULL(oItem)
					oItem=this.GetFolder(tvItem)
					IF ISNULL(oItem)
						RETURN .F.
					ENDIF
				ENDIF
			CASE TYPE("tvItem")=="O"
				oItem=tvItem
				tvItem=.NULL.
			OTHERWISE
				oItem=this
		ENDCASE
		lcScript=oItem.cScript
		IF EMPTY(lcScript)
			RETURN .F.
		ENDIF
		RETURN oItem.RunCode(lcScript)
		
	ENDPROC

	PROCEDURE savepreferences
		LPARAMETERS tcName,tlSaveOptions
		LOCAL lnHeight1,lnWidth1,lnHeight2,lnWidth2,lcFileName,lcFileName2,lcPath
		LOCAL lcProperties,lcAlias,lnCount,oFolder,llIgnoreErrors,lnRecNo,oRecord
		
		this.lSavePreferences=.F.
		IF NOT this.lPreferences
			RETURN .F.
		ENDIF
		IF NOT USED("browser")
			SET DATASESSION TO this.DataSessionID
			IF NOT USED("browser")
				RETURN .F.
			ENDIF
		ENDIF
		SELECT browser
		IF tlSaveOptions
			GO (this.nPrefRecNo)
			lcProperties="this.lRunFileDefault = "+IIF(this.lRunFileDefault,".T.",".F.")+CR+ ;
					"this.lAddFileDefault = "+IIF(this.lAddFileDefault,".T.",".F.")+CR+ ;
					"this.lAutoLabelEdit = "+IIF(this.lAutoLabelEdit,".T.",".F.")+CR+ ;
					"this.lAdvancedEditing = "+IIF(this.lAdvancedEditing,".T.",".F.")+CR+ ;
					"this.lFFCBuilderLock = "+IIF(this.lFFCBuilderLock,".T.",".F.")+CR+ ;
					"this.lDragDropToDesktop = "+IIF(this.lDragDropToDesktop,".T.",".F.")
			REPLACE Properties WITH lcProperties
		ENDIF
		lcFileName=IIF(EMPTY(tcName),"",LOWER(ALLTRIM(tcName)))
		IF EMPTY(lcFileName) AND ((this.lBrowser AND this.nClassCount>0) OR ;
				(NOT this.lBrowser AND this.nFolderCount>0))
			IF this.lBrowser	
				FOR lnCount = 1 TO this.nFileCount
					lcFileName=this.aFiles[lnCount]
					IF EMPTY(lcFileName)
						LOOP
					ENDIF
					this.SavePreferences(lcFileName)
				ENDFOR
			ELSE
				lnCount=1
				DO WHILE .T.
					IF NOT USED(lnCount)
						EXIT
					ENDIF
					lcAlias=LOWER(ALIAS(lnCount))
					lnCount=lnCount+1
					IF NOT LEFT(lcAlias,7)=="catalog" OR NOT USED(lcAlias)
						LOOP
					ENDIF
					lcFileName=LOWER(DBF(lcAlias))
					this.SavePreferences(lcFileName)
				ENDDO
			ENDIF
			SELECT 0
			RETURN
		ENDIF
		IF EOF("browser") OR (NOT EMPTY(lcFileName) AND NOT this.RefreshPrefRecNo(lcFileName))
			SELECT 0
			RETURN .F.
		ENDIF
		SELECT browser
		IF EMPTY(Name)
			IF NOT this.lInitialized
				RETURN
			ENDIF
			GO (this.nPrefRecNo)
		ENDIF
		lnRecNo=RECNO()
		lnHeight1=47-this.edtClassDesc.Height
		IF this.lOutlineOCX
			lnWidth1=this.oleClassList.Width-219
			lnHeight2=47-this.edtItemDesc.Height
			lnWidth2=this.oleFolderList.Width-219
		ELSE
			lnWidth1=this.lstClassList.Width-219
			lnHeight2=0
			lnWidth2=0
		ENDIF
		IF this.WindowState=0
			REPLACE Top WITH this.Top, ;
					Left WITH this.Left, ;
					Height WITH this.Height, ;
					Width WITH this.Width, ;
					Height1 WITH lnHeight1, ;
					Width1 WITH lnWidth1, ;
					Height2 WITH lnHeight2, ;
					Width2 WITH lnWidth2
		ENDIF
		REPLACE WindowStat WITH this.WindowState, ;
				DispMode WITH this.nDisplayMode, ;
				Protected WITH this.lProtectedFilter, ;
				Empty WITH this.lEmptyFilter, ;
				Hidden WITH this.lHiddenFilter, ;
				DescBoxes WITH this.lDescriptions, ;
				AutoExpand WITH this.lAutoExpand, ;
				Pushpin WITH this.AlwaysOnTop, ;
				PCBrowser WITH this.lParentClassBrowser, ;
				ViewMode WITH this.nImageView, ;
				FontInfo WITH this.FontName+","+ALLTRIM(STR(this.FontSize))+","+ ;
						IIF(this.FontBold,"B","")+IIF(this.FontItalic,"I",""), ;
				Updated WITH DATETIME()
		IF lnRecNo#this.nPrefRecNo
			SCATTER MEMO NAME oRecord FIELDS Top,Left,Height,Width,Height1,Width1, ;
					Height2,Width2,WindowStat,DispMode,Protected,Empty,Hidden,DescBoxes, ;
					AutoExpand,Pushpin,PCBrowser,ViewMode,FontInfo,Updated
			GO (this.nPrefRecNo)
			GATHER MEMO NAME oRecord	
			GO lnRecNo
		ENDIF
		IF NOT EMPTY(lcFileName) AND NOT this.lBrowser AND this.nViewType=1
			FOR lnCount = 1 TO this.nFolderCount
				oFolder=this.aFolderList[lnCount]
				IF oFolder.lObject OR NOT oFolder.lCatalog OR NOT oFolder.cCatalog==lcFileName
					LOOP
				ENDIF
				REPLACE Desc WITH oFolder.cText
				EXIT
			ENDFOR
		ENDIF
		IF Backup AND FILE(lcFileName)
			lcPath=JUSTPATH(lcFileName)+"\backup\"
			lcFileName2=FORCEPATH(lcFileName,lcPath)
			llIgnoreErrors=this.lIgnoreErrors
			this.lIgnoreErrors=.T.
			this.nError=0
			IF NOT FILE(lcFileName2)
				MD (lcPath)
			ENDIF
			SELECT 0
			USE (lcFileName) AGAIN SHARED
			IF USED()
				COPY TO (lcFileName2)
				USE
			ELSE
				IF this.nError=15
					COPY FILE (lcFileName) TO (lcFileName2)
				ENDIF
			ENDIF
			SELECT browser
			IF FILE(lcFileName2)
				REPLACE Backup WITH .F.
			ENDIF
			this.lIgnoreErrors=llIgnoreErrors
		ENDIF
		SELECT 0
		
	ENDPROC

	PROCEDURE scaleresize
		LOCAL lvResult
		
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		this.vResult=.T.
		DO brwScaleResize IN browser WITH (this)
		lvResult=this.vResult
		this.vResult=.T.
		RETURN lvResult
		
	ENDPROC

	PROCEDURE seekclass
		LPARAMETERS tcClass,tcClassLibrary
		LOCAL lcClass,lcClassLibrary,lnCount,lnClassCount
		
		IF NOT this.lBrowser
			RETURN .F.
		ENDIF
		IF NOT USED(this.cAlias)
			RETURN .F.
		ENDIF
		IF TYPE("tcClass")=="N"
			lnCount=INT(tcClass)
			IF lnCount=0
				this.RefreshMembers
				RETURN
			ENDIF
			IF NOT BETWEEN(lnCount,1,this.nClassCount)
				RETURN .F.
			ENDIF
			lcClass=this.aClassList[lnCount,1]
		ELSE
			lcClass=IIF(TYPE("tcClass")#"C","",LOWER(ALLTRIM(tcClass)))
		ENDIF
		IF TYPE("tcClassLibrary")=="N"
			lnCount=INT(tcClassLibrary)
			IF NOT BETWEEN(lnCount,1,this.nFileCount)
				RETURN .F.
			ENDIF
			lcClassLibrary=this.aFiles[lnCount]
		ELSE
			lcClassLibrary=IIF(TYPE("tcClassLibrary")#"C","",LOWER(ALLTRIM(tcClassLibrary)))
			IF NOT EMPTY(lcClassLibrary)
				IF NOT "."$lcClassLibrary
					lcClassLibrary=lcClassLibrary+".vcx"
				ENDIF
				lcClassLibrary=LOWER(FULLPATH(lcClassLibrary))
				IF NOT FILE(lcClassLibrary)
					lcClassLibrary=JUSTFNAME(lcClassLibrary)
					FOR lnCount = 1 TO this.nFileCount
						IF JUSTFNAME(this.aFiles[lnCount])==lcClassLibrary
							lcClassLibrary=this.aFiles[lnCount]
							EXIT
						ENDIF
					ENDFOR
				ENDIF
			ENDIF
		ENDIF
		IF EMPTY(lcClass)
			IF this.lOutlineOCX
				this.oleClassList.object.Nodes[1].Selected=.T.
			ELSE
				this.lstClassList.ListIndex=1
				IF this.lstClassList.TopIndex#1
					this.lstClassList.TopIndex=1
				ENDIF
			ENDIF
			this.RefreshMembers
			RETURN
		ENDIF
		lnClassCount=this.nClassCount
		FOR lnCount = 1 TO lnClassCount
			IF NOT this.aClassList[lnCount,1]==lcClass OR ;
					(NOT EMPTY(lcClassLibrary) AND ;
					NOT this.aClassList[lnCount,6]==lcClassLibrary)
				LOOP
			ENDIF
			IF this.lOutlineOCX
				this.oleClassList.object.Nodes[lnCount].Selected=.T.
			ELSE
				this.lstClassList.ListIndex=lnCount
				IF this.lstClassList.TopIndex#this.lstClassList.ListIndex
					this.lstClassList.TopIndex=this.lstClassList.ListIndex
				ENDIF
			ENDIF
			this.RefreshMembers
			IF NOT this.lRelease AND this.lActive AND this.Visible
				IF this.lOutlineOCX
					this.oleClassList.SetFocus
				ELSE
					this.lstClassList.SetFocus
				ENDIF
			ENDIF
			RETURN
		ENDFOR
		RETURN .F.
		
	ENDPROC

	PROCEDURE seekmember
		LPARAMETERS tcMember
		LOCAL lcMember,lnCount,lnAtPos,lcMember,llMatch
		LOCAL lcItem,oMemberList,oNode
		
		IF EMPTY(tcMember)
			IF this.nMemberListIndex<1
				RETURN
			ENDIF
			this.nMemberListIndex=-1
			IF this.lOutlineOCX
				this.oleMembers.object.SelectedItem.Selected=.F.
			ELSE
				this.lstMembers.ListIndex=-1
				this.lstMembers.TopIndex=-1
			ENDIF
			this.RefreshCaption
			this.RefreshButtons
			this.edtMemberDesc.Refresh
			RETURN
		ENDIF
		lcMember=LOWER(ALLTRIM(tcMember))
		FOR lnCount = 1 TO this.nMemberCount
			IF NOT LOWER(this.aMemberList[lnCount,1])==lcMember
				LOOP
			ENDIF
			this.nMemberListIndex=lnCount
			IF this.lOutlineOCX
				this.oleMembers.Nodes[lnCount].Selected=.T.
			ELSE
				this.lstMembers.ListIndex=lnCount
				this.lstMembers.TopIndex=lnCOunt
			ENDIF
			this.RefreshCaption
			this.RefreshButtons
			this.edtMemberDesc.Refresh
			RETURN
		ENDFOR
		RETURN .F.
	ENDPROC

	PROCEDURE seekparentclass
		LOCAL lcClassLibrary,lcParentClass,lnFileCount,llActive
		
		IF NOT this.lBrowser OR NOT this.lVCXSCXMode 
			RETURN .F.
		ENDIF
		lcClassLibrary=this.cClassLibrary
		lcParentClass=this.cParentClass
		IF EMPTY(lcClassLibrary) OR EMPTY(lcParentClass)
			RETURN .F.
		ENDIF
		IF ASCAN(this.aFiles,lcClassLibrary)>0
			this.SeekClass(lcParentClass)
		ELSE
			IF ASCAN(this.aFiles,lcClassLibrary)=0
				lnFileCount=this.nFileCount
				llActive=this.lActive
				this.lActive=.T.
				this.AddFile(lcClassLibrary,.T.)
				this.lActive=llActive
				IF ASCAN(this.aFiles,lcClassLibrary)=0
					RETURN .F.
				ENDIF
			ENDIF
			this.cClass=lcParentClass
			this.RefreshBrowser
		ENDIF
		
	ENDPROC

	PROCEDURE selectfolder
		LPARAMETERS tvFolder,tlSetFocus
		LOCAL oFolder
		
		DO CASE
			CASE ISNULL(tvFolder)
				RETURN .F.
			CASE TYPE("tvFolder")=="C"
				oFolder=this.GetFolder(tvFolder)
				IF ISNULL(oFolder)
					RETURN .F.
				ENDIF
			CASE TYPE("tvFolder")=="O"
				oFolder=tvFolder
				tvFolder=.NULL.
				IF TYPE("oFolder.lFolder")#"L" OR NOT oFolder.lFolder
					RETURN .F.
				ENDIF
			OTHERWISE
				RETURN .F.
		ENDCASE
		RETURN this.SelectItem(oFolder,tlSetFocus)
		
	ENDPROC

	PROCEDURE selectitem
		LPARAMETERS tvItem,tlSetFocus
		LOCAL oItem,oFolder,lnIndex,lnFolder,lnLastFolderIndex,llFolder
		
		DO CASE
			CASE ISNULL(tvItem)
				RETURN .F.
			CASE TYPE("tvItem")=="C"
				oItem=this.GetItem(tvItem)
				IF ISNULL(oItem)
					oItem=this.GetFolder(tvItem,.T.)
					IF ISNULL(oItem)
						RETURN .F.
					ENDIF
				ENDIF
			CASE TYPE("tvItem")=="O"
				oItem=tvItem
				tvItem=.NULL.
				IF TYPE("oItem.lFolder")#"L"
					RETURN .F.
				ENDIF
			OTHERWISE
				RETURN .F.
		ENDCASE
		IF oItem.lObject OR NOT oItem.Visible OR NOT oItem.Enabled
			RETURN .F.
		ENDIF
		llFolder=oItem.lFolder
		IF llFolder
			oFolder=oItem
			lnIndex=oItem.nIndex
			IF lnIndex<1
				RETURN .F.
			ENDIF
		ELSE
			oFolder=oItem.oParent
			lnIndex=oItem.nNodeIndex
		ENDIF
		lnFolderIndex=oFolder.nIndex
		lnLastFolderIndex=this.nFolderListIndex
		IF lnFolderIndex#lnLastFolderIndex OR lnIndex<1 OR NOT this.lInitialized OR ;
				this.lRefreshBrowserMode
			this.nLastFolderListIndex=lnLastFolderIndex
			this.nFolderListIndex=lnFolderIndex
			IF lnLastFolderIndex>0
				this.oleFolderList.object.Nodes[lnLastFolderIndex].Selected=.F.
			ENDIF
			IF lnFolderIndex>0
				this.oleFolderList.object.Nodes[lnFolderIndex].Selected=.T.
			ENDIF
			this.RefreshItemList
			IF llFolder
				IF tlSetFocus AND this.Visible
					this.oleFolderList.SetFocus
				ENDIF
				RETURN
			ENDIF
			lnIndex=oItem.nNodeIndex
		ELSE
			IF NOT ISNULL(this.oItemSource) AND NOT this.oItemSource.lFolder
				IF NOT llFolder AND this.oItemSource.nNodeIndex=lnIndex
					IF tlSetFocus AND this.Visible
						this.oleItems.SetFocus
					ENDIF
					RETURN
				ENDIF
				this.oleItems.object.ListItems.Item[this.oItemSource.nNodeIndex].Selected=.F.
				IF llFolder
					this.nItemListIndex=-1
					this.RefreshItem
				ENDIF
			ENDIF
			IF llFolder
				IF tlSetFocus AND this.Visible
					this.oleFolderList.SetFocus
				ENDIF
				RETURN
			ENDIF
		ENDIF
		IF TYPE("this.oleItems.object.ListItems.Item[lnIndex]")=="O"
			this.nItemListIndex=lnIndex
			this.oleItems.object.ListItems.Item[lnIndex].Selected=.T.
		ENDIF
		this.RefreshItem
		IF tlSetFocus AND this.Visible
			IF llFolder
				this.oleFolderList.SetFocus
			ELSE
				this.oleItems.SetFocus
			ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE setbusystate
		LPARAMETERS tlBusyState,lnMousePointer
		
		IF this.lRelease OR NOT this.lInitialized
			RETURN .F.
		ENDIF
		lnMousePointer=IIF(tlBusyState,11,0)
		IF this.MousePointer=lnMousePointer
			RETURN
		ENDIF
		WITH this
			.MousePointer=lnMousePointer
			IF .lOutlineOCX
				IF .lBrowser
					.oleClassList.object.MousePointer=lnMousePointer
					.oleMembers.object.MousePointer=lnMousePointer
				ELSE
					.oleFolderList.object.MousePointer=lnMousePointer
					.oleItems.object.MousePointer=lnMousePointer
				ENDIF
			ELSE
				.lstClassList.MousePointer=lnMousePointer
				.lstMembers.MousePointer=lnMousePointer
			ENDIF
			.txtClassList3D.MousePointer=lnMousePointer
			.txtMembers3D.MousePointer=lnMousePointer
			.lBusyState=tlBusyState
		ENDWITH
		

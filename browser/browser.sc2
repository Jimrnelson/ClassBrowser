	PROCEDURE edtMemberDesc.LostFocus
		LOCAL lcValue,lcOldValue,lcReserved3,lcOldReserved3
		LOCAL lnAtPos,lnStrLen
		
		lnAtPos=thisform.nAtPos
		lnStrLen=thisform.nStrLen
		IF thisform.lReadOnly OR lnAtPos=0
			RETURN
		ENDIF
		lcLastValue=this.Value
		lcValue=this.Value
		IF RIGHT(lcValue,1)==" "
			lcValue=TRIM(lcValue)
		ENDIF
		IF NOT USED(thisform.cAlias)
			this.Value=""
			RETURN
		ENDIF
		SELECT (thisform.cAlias)
		GO thisform.nLastRecNo
		IF lnAtPos<0
			lcOldValue=Reserved7
			IF NOT lcOldValue==lcValue
				thisform.ClearClass
				REPLACE Reserved7 WITH lcValue
				IF NOT Reserved7==lcValue
					this.Value=thisform.cLastValue
				ENDIF
			ENDIF
			SELECT 0
			RETURN
		ENDIF
		lcOldReserved3=Reserved3
		lcOldValue=SUBSTR(lcOldReserved3,lnAtPos,lnStrLen)
		IF lcOldValue==lcValue
			SELECT 0
			RETURN
		ENDIF
		lcValue=TRIM(STRTRAN(STRTRAN(STRTRAN(lcValue,CR_LF," "),CR," "),LF," "))
		IF LEN(lcValue)>0
			lcValue=" "+lcValue
		ENDIF
		lcReserved3=TRIM(LEFT(lcOldReserved3,lnAtPos-1))+lcValue+ ;
						SUBSTR(lcOldReserved3,lnAtPos+lnStrLen)
		DO WHILE RIGHT(lcReserved3,1)==LF OR RIGHT(lcReserved3,1)==CR
			lcReserved3=LEFT(lcReserved3,LEN(lcReserved3)-1)
		ENDDO
		lcReserved3=lcReserved3+CR_LF
		thisform.ClearClass
		REPLACE Reserved3 WITH lcReserved3
		IF NOT Reserved3==lcReserved3
			this.Value=thisform.cLastValue
		ENDIF
		SELECT 0
		
	ENDPROC

	PROCEDURE edtMemberDesc.Refresh
		LOCAL lcValue,lcMemberChar,lcReserved3,lcParams,lcToolTipText
		LOCAL lcItem,lcItem2,lcItemMessage,lcFileName,lcSearchStr,lcTimeStamp
		LOCAL lcClassLibrary,lnAtPos,lnStrLen,lnRecNo,lnLastRecNo,lnCount,llLockScreen
		
		IF NOT thisform.lBrowser OR thisform.nClassCount<=0
			this.ReadOnly=.T.
			this.MaxLength=0
			this.SelLength=0
			this.SelStart=0
			this.StatusBarText=""
			this.ToolTipText=""
			this.Value=""
			this.ScrollBars=0
			RETURN
		ENDIF
		llLockScreen=thisform.LockScreen
		thisform.LockScreen=.T.
		this.Value=""
		this.ReadOnly=.T.
		this.MaxLength=0
		this.SelLength=0
		this.SelStart=0
		thisform.nAtPos=0
		thisform.nStrLen=0
		thisform.cLastValue=""
		WITH thisform
			IF .nMemberListIndex>0
				lcItem=.aMemberList[.nMemberListIndex,1]
				lcMemberChar=.aMemberList[.nMemberListIndex,2]
			ELSE
				lcMemberChar=""
				lcItem=""
			ENDIF
		ENDWITH
		lcToolTipText=LEFT(thisform.cClass,127)
		IF NOT thisform.lVCXSCXMode
			IF EMPTY(lcMemberChar)
				lcValue=M_TYPELIB_LOC+":  "+thisform.cFileName+ ;
						IIF(thisform.lReadOnly,"  ("+M_READ_ONLY_LOC+")","")
				lcTimeStamp=thisform.GetTimeStamp()
				IF NOT EMPTY(lcTimeStamp)
					lcValue=lcValue+CR+M_TIMESTAMP_LOC+lcTimeStamp
				ENDIF
				lcItemMessage=thisform.cClass
			ELSE
				lcValue=thisform.aMemberList[thisform.nMemberListIndex,3]
				IF EMPTY(lcValue)
					lcValue=lcItem
				ENDIF
				lcItemMessage="("+thisform.cFileName+"::"+lcItem+")"
			ENDIF
			this.Value=lcValue
			this.ScrollBars=IIF(EMPTY(lcValue),0,2)
			this.StatusBarText=M_TYPELIB_LOC+" "+lcItemMessage+" "+M_INFORMATION_LOC
			this.ToolTipText=lcToolTipText
			thisform.LockScreen=llLockScreen
			RETURN
		ENDIF
		IF thisform.lFileMode
			this.ReadOnly=.T.
			lcValue=IIF(thisform.lSCXMode,M_FORM_LOC,M_CLASS_LIBRARY_LOC)+ ;
					":  "+thisform.cClass+IIF(thisform.lReadOnly,"  ("+ ;
					M_READ_ONLY_LOC+")","")
			lcTimeStamp=thisform.GetTimeStamp()
			IF NOT EMPTY(lcTimeStamp)
				lcValue=lcValue+CR+"Time Stamp:  "+lcTimeStamp
			ENDIF
			this.Value=lcValue
			this.ScrollBars=IIF(EMPTY(lcValue),0,2)
			this.StatusBarText=M_FILE_LOC+" ("+thisform.cFileName+") "+M_INFORMATION_LOC
			this.ToolTipText=lcToolTipText
			thisform.LockScreen=llLockScreen
			RETURN
		ENDIF
		IF EMPTY(lcItem) OR EMPTY(lcMemberChar)
			this.ReadOnly=.T.
			lcValue="Class:  "+thisform.cClass
			IF NOT EMPTY(thisform.cClassLibrary)
				lcClassLibrary=LOWER(FULLPATH(thisform.cClassLibrary))
				lcValue=lcValue+CR+"ParentClass:  "+thisform.cParentClass+"  ("+lcClassLibrary+")"
			ENDIF
			IF NOT EMPTY(thisform.cBaseClass)
				lcValue=lcValue+CR+"BaseClass:  "+thisform.ProperBaseClass(thisform.cBaseClass)
			ENDIF
			lcTimeStamp=thisform.GetTimeStamp()
			IF NOT EMPTY(lcTimeStamp)
				lcValue=lcValue+CR+"Time Stamp:  "+lcTimeStamp
			ENDIF
			this.Value=lcValue
			this.ScrollBars=IIF(EMPTY(lcValue),0,2)
			this.StatusBarText=M_CLASS_LOC+" ("+thisform.cClass+") "+M_INFORMATION_LOC
			this.ToolTipText=lcToolTipText
			thisform.LockScreen=llLockScreen
			RETURN
		ENDIF
		IF lcMemberChar=="i"
			this.Value=""
			this.StatusBarText=M_INSTANCE_LOC+" ("+lcItem+") "+M_STATUS_LOC
			this.ToolTipText=LEFT(lcItem+" ("+thisform.cClass+")",127)
			lcFileName=LOWER(FULLPATH(SYS(2015)+".txt",HOME()))
			DISPLAY OBJECTS LIKE (lcItem) TO (lcFileName) NOCONSOLE
			CREATE CURSOR temp (Info M)
			APPEND BLANK
			APPEND MEMO Info FROM (lcFileName)
			lcValue=ALLTRIM(Info)
			USE
			SELECT 0
			DO WHILE LEFT(lcValue,1)==LF OR LEFT(lcValue,1)==CR
				lcValue=SUBSTR(lcValue,2)
			ENDDO
			DO WHILE RIGHT(lcValue,1)==LF OR RIGHT(lcValue,1)==CR
				lcValue=LEFT(lcValue,LEN(lcValue)-1)
			ENDDO
			this.Value=lcValue
			this.ScrollBars=IIF(EMPTY(lcValue),0,2)
			thisform.LockScreen=llLockScreen
			ERASE (lcFileName)
			RETURN
		ENDIF
		lcItemMessage="("+thisform.cClass+"::"+lcItem+")"
		IF lcMemberChar=="o"
			lcItem2=lcItem
			SELECT (thisform.cAlias)
			lnLastRecNo=RECNO()
			lnRecNo=thisform.aMemberList[thisform.nMemberListIndex,3]
			GO lnRecNo
			IF NOT EMPTY(ClassLoc)
				lcClassLibrary=LOWER(FULLPATH(ClassLoc,thisform.cFileName))
				lcItem2=lcItem2+CR+"Class:  "+Class+"  ("+lcClassLibrary+")"
				thisform.cLastValue=lcClassLibrary+","+Class
			ENDIF
			lcValue=thisform.cClass+"::"+lcItem2
			IF BETWEEN(lnLastRecNo,1,RECCOUNT())
				GO lnLastRecNo
			ENDIF
			SELECT 0
			this.Value=lcValue
			this.ScrollBars=IIF(EMPTY(lcValue),0,2)
			this.StatusBarText=M_OBJECT_MEMBER_LOC+" "+lcItemMessage+" "+M_INFORMATION_LOC
			this.ToolTipText=LEFT(thisform.cClass+"::"+lcItem,127)
			thisform.LockScreen=llLockScreen
			RETURN
		ENDIF
		this.MaxLength=255
		DO CASE
			CASE lcMemberChar=="m"
				lcParams=thisform.GetMethod(lcItem,.T.)
				lcItem2=lcItem+"("+lcParams+")"
				this.StatusBarText=M_METHOD_LOC+" "+lcItemMessage+" "+M_DESCRIPTION_LOC
				this.ToolTipText=LEFT(thisform.cClass+"::"+lcItem2,127)
			CASE lcMemberChar=="p"
				this.StatusBarText=M_PROPERTY_LOC+" "+lcItemMessage+" "+M_DESCRIPTION_LOC
				this.ToolTipText=LEFT(thisform.cClass+"::"+lcItem+ " = "+thisform.GetProperty(lcItem),127)
			OTHERWISE
				this.StatusBarText=M_OBJECT_MEMBER_LOC+" "+lcItemMessage+" "+ ;
						M_DESCRIPTION_LOC
				this.ToolTipText=LEFT(thisform.cClass+"::"+lcItem,127)
		ENDCASE
		thisform.cLastValue=this.Value
		thisform.RefreshRecNo
		SELECT (thisform.cAlias)
		IF lcMemberChar=="o"
			IF thisform.lOutlineOCX
				SKIP (thisform.oleMembers.object.SelectedItem.Index)
			ELSE
				SKIP (thisform.lstMembers.ListIndex)
			ENDIF
			thisform.nLastRecNo=RECNO()
			IF thisform.lOutlineOCX
				thisform.nAtPos=-thisform.oleMembers.object.SelectedItem.Index
			ELSE
				thisform.nAtPos=-thisform.lstMembers.ListIndex
			ENDIF
			lcValue=TRIM(Reserved7)
			lnStrLen=LEN(lcValue)
			thisform.nStrLen=lnStrLen
			this.ReadOnly=thisform.lReadOnly OR THISFORM.lPRGMode
			SELECT 0
			this.Value=lcValue
			this.ScrollBars=IIF(EMPTY(lcValue),0,2)
			thisform.LockScreen=llLockScreen
			RETURN
		ENDIF
		lcReserved3=Reserved3+CR_LF
		thisform.nLastRecNo=RECNO()
		lcValue=""
		lnStrLen=0
		lcSearchStr=""
		IF lcMemberChar=="m"
			lcSearchStr="*"
		ELSE
			WITH thisform
				lcItem2=ALLTRIM(IIF(.lOutlineOCX,.oleMembers.SelectedItem.Text,.lstMembers.Value))
			ENDWITH
			lnAtPos=AT("[",lcItem2)
			IF lnAtPos>0
				lcSearchStr="^"
				IF NOT ","$lcItem2
					lcItem2=STRTRAN(lcItem2,"]",",0]")
				ENDIF
				lcItem=lcItem+SUBSTR(lcItem2,lnAtPos)
			ENDIF
		ENDIF
		lcSearchStr=lcSearchStr+LOWER(lcItem)+" "
		lnAtPos=ATC(CR_LF+lcSearchStr,CR_LF+lcReserved3)
		IF lnAtPos=0
			lcSearchStr=TRIM(lcSearchStr)
			lnAtPos=ATC(CR_LF+lcSearchStr+CR_LF,CR_LF+lcReserved3)
			IF lnAtPos=0
				lcSearchStr=TRIM(lcSearchStr)+"["
				lnAtPos=ATC(CR_LF+lcSearchStr+CR_LF,CR_LF+lcReserved3)
			ENDIF
		ENDIF
		IF lnAtPos>0
			lnAtPos=LEN(lcSearchStr)+lnAtPos
			thisform.nAtPos=lnAtPos
			lcValue=TRIM(SUBSTR(lcReserved3,lnAtPos))
			lnAtPos=AT(CR_LF,lcValue)
			IF lnAtPos>0
				lcValue=TRIM(LEFT(lcValue,lnAtPos-1))
			ENDIF
			lnStrLen=LEN(lcValue)
			thisform.nStrLen=lnStrLen
			this.ReadOnly=thisform.lReadOnly OR THISFORM.lPRGMode
		ENDIF
		SELECT 0
		this.Value=lcValue
		this.ScrollBars=IIF(EMPTY(lcValue),0,2)
		thisform.LockScreen=llLockScreen
		
		
	ENDPROC


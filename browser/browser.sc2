	PROCEDURE Activate
		SET DATASESSION TO (this.DataSessionID)
		
		IF this.lRelease
			RETURN .F.
		ENDIF
		IF this.lModalDialog
			this.lModalDialog=.F.
			RETURN .F.
		ENDIF
		THIS.nlangopt=_VFP.LanguageOptions
		IF THIS.nlangopt=1
			_VFP.LanguageOptions=0
		ENDIF
		THIS.cSYS3054 = SYS(3054)
		SYS(3054,0)
		SET DATASESSION TO this.DataSessionID
		IF TYPE("_screen._oCGClipBoard")#"O"
			_screen.NewObject("_oCGClipBoard","_CGClipBoard","browser.vcx")
		ENDIF
		IF this.lInitialized AND NOT this.lActive
			this.cLastSetESC=SET("ESCAPE")
			this.cLastOnKeyF5=ON("KEY","F5")
			this.cLastSetComp=SET("COMPATIBLE")
			this.cLastSetUDFParms=SET("UDFPARMS")
			this.lLastAutoYield=_vfp.AutoYield
		ENDIF
		SET ESCAPE OFF
		ON KEY LABEL F5
		SET COMPATIBLE OFF
		SET UDFPARMS TO VALUE
		_vfp.AutoYield=.F.
		SELECT 0
		SET MESSAGE TO
		IF this.lRelease
			RETURN .F.
		ENDIF
		IF TYPE("_oBrowser")#"O" OR ISNULL(_oBrowser) OR _oBrowser#this
			RELEASE _oBrowser
			PUBLIC _oBrowser
			_oBrowser=this
		ENDIF
		SET DATASESSION TO this.DataSessionID
		this.lActive=.T.
		
		this.RefreshParentClassBrowser
		IF this.lInitialized
			this.RestorePreferences
		ELSE
			this.cboClassType.SelStart=0
			IF this.lBrowser AND NOT this.lOutlineOCX AND ;
					this.lstClassList.TopIndex#this.lstClassList.ListIndex
				this.lstClassList.TopIndex=this.lstClassList.ListIndex
			ENDIF
			this.lInitialized=.T.
			this.SavePreferences
			IF NOT this.lRelease AND this.Visible
				this.cmdOpen.SetFocus
			ENDIF
			SELECT 0
			SET MESSAGE TO
			IF NOT USED("browser")
				this.Release
				RETURN .F.
			ENDIF
			IF this.lBrowser
				IF EMPTY(this.cFileName) OR NOT USED(this.cAlias)
					IF this.AddInMethod(PROGRAM())
						RETURN
					ENDIF
					RETURN
				ENDIF
				IF this.lOutlineOCX
					IF TYPE("this.oleClassList.object.SelectedItem.Text")=="C" AND ;
							(this.oleClassList.object.SelectedItem.Index-1)#this.nClassListIndex
		
						this.RefreshMembers
					ENDIF
				ELSE
					IF this.lstClassList.ListIndex>=1
						IF (this.lstClassList.ListIndex-1)#this.nClassListIndex
							this.RefreshMembers
						ENDIF
						IF this.lstClassList.TopIndex#this.lstClassList.ListIndex
							this.lstClassList.TopIndex=this.lstClassList.ListIndex
						ENDIF
					ENDIF
				ENDIF
			ENDIF
			IF this.AddInMethod(PROGRAM())
				RETURN
			ENDIF
			IF this.lBrowser
				IF this.lOutlineOCX
					IF (this.oleClassList.object.SelectedItem.Index-1)#this.nClassListIndex
						this.RefreshMembers
					ENDIF
				ELSE
					IF (this.lstClassList.ListIndex-1)#this.nClassListIndex
						this.RefreshMembers
					ENDIF
				ENDIF
			ENDIF
			RETURN
		ENDIF
		IF this.lBrowser#this.lLastBrowser
			this.lBrowser=this.lLastBrowser
			this.SwitchBrowser
		ENDIF
		IF this.lBrowser
			IF this.lOutlineOCX
				IF TYPE("this.oleClassList.object.SelectedItem.Text")=="C" AND ;
						(this.oleClassList.object.SelectedItem.Index-1)#this.nClassListIndex
					this.RefreshMembers
				ENDIF
			ELSE
				IF this.lstClassList.ListIndex>=1 AND ;
						(this.lstClassList.ListIndex-1)#this.nClassListIndex
					this.RefreshMembers
				ENDIF
			ENDIF
		ENDIF
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		this.AutoRefresh
		this.SavePreferences
		SELECT 0
		SET MESSAGE TO
		IF this.lBrowser
			IF this.lOutlineOCX
				IF TYPE("this.oleClassList.object.SelectedItem.Text")=="C" AND ;
						(this.oleClassList.object.SelectedItem.Index-1)#this.nClassListIndex
					this.RefreshMembers
				ENDIF
			ELSE
				IF this.lstClassList.ListIndex>=1 AND ;
						(this.lstClassList.ListIndex-1)#this.nClassListIndex
					this.RefreshMembers
				ENDIF
			ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE addclass
		LOCAL lcFileName,lcFileName2,lcClass,lnCount,lnClassCount,llActive
		
		IF NOT this.lBrowser OR this.lReadOnly
			RETURN .F.
		ENDIF
		this.oSource.parent.lDragDrop=.T.
		llActive=this.lActive
		this.lActive=.T.
		lcFileName=this.oSource.parent.cFileName
		lcClass=this.oSource.parent.cClass
		IF this.oSource.parent.lFileMode
			lnClassCount=0
			FOR lnCount = 1 TO this.oSource.parent.nClassCount
				lcClass=this.oSource.parent.aClassList[lnCount,1]
				lcFileName2=this.oSource.parent.aClassList[lnCount,6]
				IF EMPTY(lcClass) OR "."$lcClass OR NOT lcFileName==lcFileName2
					LOOP
				ENDIF
				lnClassCount=lnClassCount+1
				SET MESSAGE TO M_COPYING_CLASS_LOC+[ (]+lcClass+[) ]+M_TO_LOC+[ "]+ ;
						this.cFileName+[" ...]
				this.ClearClass(.T.)
				this.oSource.parent.ClearClass(.T.)
				this.lError=.F.
				ADD CLASS (lcClass) OF (lcFileName) TO (this.cFileName)
				IF NOT this.lError AND this.oSource.parent.nShift#2
					this.UpdateReferences(lcFileName,lcClass,this.cFileName,lcClass,.T.)
					this.oSource.parent.SeekClass(lcClass)
					this.oSource.parent.RemoveClass
				ENDIF
			ENDFOR
			IF lnClassCount>0
				this.RefreshClassList
				this.RefreshMembers
			ENDIF
		ELSE
			SET MESSAGE TO M_COPYING_CLASS_LOC+[ (]+lcClass+[) ]+M_TO_LOC+[ "]+ ;
					this.cFileName+[" ...]
			this.ClearClass(.T.)
			this.oSource.parent.ClearClass(.T.)
			this.lError=.F.
			ADD CLASS (lcClass) OF (lcFileName) TO (this.cFileName)
			IF NOT this.lError AND this.oSource.parent.nShift#2
				this.UpdateReferences(lcFileName,lcClass,this.cFileName,lcClass,.T.)
				this.oSource.parent.RemoveClass
			ENDIF
			IF NOT this.lError
				this.cClass=lcClass
				this.RefreshBrowser
			ENDIF
		ENDIF
		this.lActive=llActive
		SELECT 0
		SET MESSAGE TO
		
	ENDPROC


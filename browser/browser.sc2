					M_DESC_NOT_AVAIL_LOC
			this.ToolTipText=lcToolTipText
			this.Value=""
			this.ScrollBars=0
			RETURN
		ENDIF
		this.ReadOnly=thisform.lReadOnly OR THISFORM.lPRGMode
		IF thisform.lFileMode
			this.MaxLength=0
			this.StatusBarText=M_FILE_LOC+" ("+thisform.cFileName+") "+M_DESCRIPTION_LOC
			this.ToolTipText=lcToolTipText
		ELSE
			this.MaxLength=255
			this.StatusBarText=M_CLASS_LOC+" ("+thisform.cClass+") "+M_DESCRIPTION_LOC
			this.ToolTipText=lcToolTipText
		ENDIF
		thisform.cLastValue=this.Value
		IF USED(thisform.cAlias)
			thisform.RefreshRecNo
			SELECT (thisform.cAlias)
			thisform.nLastRecNo=RECNO()
			this.Value=TRIM(Reserved7)
		ELSE
			this.Value=""
		ENDIF
		
		
		this.ScrollBars=IIF(EMPTY(this.Value),0,2)
		SELECT 0
		
	ENDPROC


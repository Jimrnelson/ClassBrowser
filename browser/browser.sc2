	PROCEDURE refreshmembers
		LPARAMETERS tcDefaultMember
		LOCAL lcDefaultMember,lcFileName,llLockScreen
		
		SET DATASESSION TO this.DataSessionID
		IF this.lRelease OR NOT this.lBrowser
			RETURN .F.
		ENDIF
		
		this.lRefreshMode=.T.
		lcDefaultMember=IIF(EMPTY(tcDefaultMember) OR ;
				TYPE("tcDefaultMember")#"C","",LOWER(tcDefaultMember))
		IF this.nClassCount>0
			IF this.lOutlineOCX
				this.nClassListIndex=this.oleClassList.object.SelectedItem.Index-1
			ELSE
				this.nClassListIndex=this.lstClassList.ListIndex-1
			ENDIF
		ENDIF
		IF this.nClassListIndex<0
			this.RefreshViewCode
			this.lRefreshMode=.F.
			RETURN .F.
		ENDIF
		SET MESSAGE TO M_REFRESH_MEMBER_LIST_LOC+" ..."
		llLockScreen=this.LockScreen
		this.LockScreen=.T.
		this.imgClassIcon.Visible=.F.
		this.cmdClassIcon.Visible=.F.
		IF NOT this.lRefreshBrowserMode
			WITH this
				.edtClassDesc.Value=""
				.edtMemberDesc.Value=""
				IF .lOutlineOCX
					.ClearNodes(.oleMembers)
				ENDIF
				.SetBusyState(.T.)
				IF .lOutlineOCX
					.oleMembers.Enabled=.F.
					.oleMembers.Visible=.F.
				ENDIF
				.LockScreen=.F.
				.Draw
				.LockScreen=.T.
			ENDWITH
		ENDIF
		IF this.lOutlineOCX
			lcFileName=LOWER(this.aClassList[this.oleClassList.object.SelectedItem.Index,6])
		ELSE
			lcFileName=LOWER(this.aClassList[this.lstClassList.ListIndex,6])
		ENDIF
		IF NOT lcFileName==this.cFileName
			this.cFileName=lcFileName
			this.cFileNamePath=JUSTPATH(lcFileName)+"\"
			this.lSCXMode=(RIGHT(this.cFileName,4)==".scx")
			this.lPRGMode=(RIGHT(this.cFileName,4)==".prg")
			this.lVCXSCXMode=(this.lSCXMode OR RIGHT(this.cFileName,4)==".vcx")
			this.cAlias="metadata"+ALLTRIM(STR(ASCAN(this.aFiles,lcFileName)))
			IF this.lVCXSCXMode AND NOT USED(this.cAlias)
				IF this.lOutlineOCX
					this.oleClassList.object.Nodes[1].Selected=.T.
				ELSE
					this.lstClassList.ListIndex=1
				ENDIF
				lcFileName=this.aClassList[1,6]
				this.cFileName=lcFileName
				this.cFileNamePath=JUSTPATH(lcFileName)+"\"
				this.cAlias="metadata"+ALLTRIM(STR(ASCAN(this.aFiles,lcFileName)))
			ENDIF
		
		
			this.RefreshFileAttrib
			this.lSCXMode=(RIGHT(this.cFileName,4)==".scx")
			this.lVCXSCXMode=(this.lSCXMode OR THIS.lPRGMode OR RIGHT(this.cFileName,4)==".vcx")
			IF NOT this.lVCXSCXMode
				this.cClass="("+this.cFileName+")"
				this.cParentClass=""
				this.cClassLibrary=""
				this.cBaseClass=""
			ENDIF
		ELSE
			IF THIS.lPRGMode
				LOCAL oPRGToVCX
				* for PRG mode we need to convert the PRG file to a VCX cursor
				oPRGToVcx = NEWOBJECT("VCXWrapper", "VCXWrapper.prg")
				IF oPRGToVcx.ParsePRG(lcFileName)
					oPRGToVcx.CreateVCXCursor(this.cAlias, .T.)
				ENDIF
			ENDIF
		ENDIF
		
		WITH this
			.RefreshRecNo
			IF .lOutlineOCX
				.oleMembers.Refresh
			ELSE
				.lstMembers.Refresh
			ENDIF
			.RefreshRecNo
			.RefreshCaption
			.cmdClassIcon.Refresh
			.RefreshButtons
			.edtClassDesc.Refresh
			.edtMemberDesc.Refresh
			IF NOT EMPTY(lcDefaultMember)
				.SeekMember(lcDefaultMember)
			ENDIF
			.lRefreshMode=.F.
			.LockScreen=llLockScreen
			IF NOT .lRefreshBrowserMode
				.SetBusyState(.F.)
				IF .lOutlineOCX
					.oleMembers.Enabled=.T.
					.oleMembers.Visible=.lBrowser
				ENDIF
			ENDIF
		ENDWITH
		
		
		
		
		this.RefreshViewCode
		SELECT 0
		SET MESSAGE TO
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		
	ENDPROC


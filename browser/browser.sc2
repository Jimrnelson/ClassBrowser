			ENDIF
		ENDIF
		IF tlShowErrorMsg
			this.MsgBox(M_FILE_LOC+[ "]+lcFileName+[" ]+M_IS_INVALID_LOC+[.],16)
		ENDIF
		RETURN .F.
		
	ENDPROC


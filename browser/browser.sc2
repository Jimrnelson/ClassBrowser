	PROCEDURE modifyclass
		LPARAMETERS tcMethod
		LOCAL lcMethod,lcDesignerName
		LOCAL lnLineNo
		
		IF NOT this.lVCXSCXMode 
			RETURN .F.
		ENDIF
		lcDesignerName=IIF(this.lSCXMode,M_FORM_DESIGNER_LOC,M_CLASS_DESIGNER_LOC)
		lcWindowName=STRTRAN(this.Caption,M_CLASS_BROWSER_LOC+" - ",lcDesignerName+" - ")
		lcMethod=IIF(EMPTY(tcMethod),"",ALLTRIM(LOWER(tcMethod)))
		SET MESSAGE TO M_LOADING_LOC+" "+lcDesignerName+" ..."
		
		DO CASE
		CASE this.lSCXMode
			this.MsgBox(M_SCX_FILES_EXCLUSIVE_LOC,16)
			RETURN .F.
		
		*** Changed by RMK 
		CASE this.lPRGMode
			IF this.lFileMode
				EDITSOURCE(THIS.cFilename)
			ELSE
				this.ClearClass(.T.)
				DOEVENTS
				CLEAR TYPEAHEAD
		
				* get line number where this occurs
				IF NOT EMPTY(lcMethod)
					lnLineNo = THIS.GetLineNo(lcMethod)
					EDITSOURCE(THIS.cFilename, lnLineNo)
				ELSE
					lnLineNo = THIS.GetLineNo()
		
					EDITSOURCE(THIS.cFilename, lnLineNo)
					IF WEXIST(lcWindowName)
						ACTIVATE WINDOW (lcWindowName)
					ENDIF
				ENDIF
			EndIf
			*** JRN 2022-12-30 : Add to MRU list if Thor active
			this.AddMRU(THIS.cFilename)
				
		OTHERWISE
			IF this.lFileMode
				MODIFY CLASS ? OF (this.cFileName) NOWAIT
				*** JRN 2022-12-30 : Add to MRU list if Thor active
				this.AddMRU(THIS.cFilename)
			ELSE
				this.ClearClass(.T.)
				DOEVENTS
				CLEAR TYPEAHEAD
				IF NOT EMPTY(lcMethod)
					MODIFY CLASS (this.cClass) OF (this.cFileName) NOWAIT METHOD &lcMethod
				ELSE
					MODIFY CLASS (this.cClass) OF (this.cFileName) NOWAIT
					IF WEXIST(lcWindowName)
						ACTIVATE WINDOW (lcWindowName)
					ENDIF
				ENDIF
				*** JRN 2022-12-30 : Add to MRU list if Thor active
				this.AddMRU(THIS.cFilename, this.cClass)
			ENDIF
		ENDCASE
		SET DATASESSION TO this.DataSessionID
		SELECT 0
		SET MESSAGE TO
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		
	ENDPROC


	PROCEDURE doaddin
		LPARAMETERS tcAddIn
		LOCAL lcName,lcProgram,lcClass,lcClassLib,llClassMode
		LOCAL llAddInMode,lcLastOnKeyF5,lnLastSelect
		
		SET DATASESSION TO this.DataSessionID
		IF NOT USED("browser")
			RETURN .F.
		ENDIF
		lnLastSelect=SELECT()
		DO CASE
			CASE TYPE("tcAddIn")=="L"
				SELECT browser
			CASE TYPE("tcAddIn")=="N"
				SELECT browser
				GO tcAddIn
			CASE TYPE("tcAddIn")=="C"
				IF EMPTY("tcAddIn")
					RETURN .F.
				ENDIF
				SELECT browser
				LOCATE FOR UPPER(ALLTRIM(Type))=="ADDIN" AND ;
						LOWER(ALLTRIM(MLINE(Name,1)))==LOWER(ALLTRIM(tcAddIn))
				IF EOF()
					SELECT (lnLastSelect)
					RETURN .F.
				ENDIF
			OTHERWISE
				SELECT (lnLastSelect)
				RETURN .F.
		ENDCASE
		llClassMode=.F.
		lcName=LOWER(ALLTRIM(MLINE(Name,1)))
		IF this.lAddInTrace
			ACTIVATE SCREEN
			? "("+this.Name+") Add-in name:    "+lcName
		ENDIF
		lcProgram=LOWER(ALLTRIM(Program))
		IF EMPTY(lcProgram)
			lcClass=LOWER(ALLTRIM(ClassName))
			lcClassLib=LOWER(ALLTRIM(ClassLib))
			IF EMPTY(lcClass) OR EMPTY(lcClassLib)
				RETURN .F.
			ENDIF
			llClassMode=.T.
			lcProgram=lcClassLib
		ENDIF
		IF NOT "."$lcProgram
			lcProgram=lcProgram+IIF(llClassMode,".vcx",".prg")
		ENDIF
		lcProgram=LOWER(FULLPATH(lcProgram))
		IF NOT FILE(lcProgram)
			this.MsgBox(M_ADD_IN_LOC+[:  ]+lcName+CR+CR+ ;
					M_FILE_LOC+[ "]+lcProgram+[" ]+M_DOES_NOT_EXIST_LOC+[.],16)
			SELECT (lnLastSelect)
			RETURN .F.
		ENDIF
		llAddInMode=this.lAddInMode
		this.lAddInMode=.T.
		this.cAddIn=TRIM(Name)
		IF this.cLastSetUDFParms=="REFERENCE"
			SET UDFPARMS REFERENCE
		ELSE
			SET UDFPARMS VALUE
		ENDIF
		IF this.cLastSetComp=="ON"
			SET COMPATIBLE ON
		ELSE
			SET COMPATIBLE OFF
		ENDIF
		IF NOT EMPTY(this.cLastOnKeyF5)
			lcLastOnKeyF5=this.cLastOnKeyF5
			ON KEY LABEL F5 &lcLastOnKeyF5
		ENDIF
		IF this.cLastSetESC=="OFF"
			SET ESCAPE OFF
		ELSE
			SET ESCAPE ON
		ENDIF
		_vfp.AutoYield=this.lLastAutoYield
		DO CASE
			CASE llClassMode
				IF this.lAddInTrace
					ACTIVATE SCREEN
					? "("+this.Name+") Add-in class:   "+lcProgram+" ("+lcClass+")"
				ENDIF
				SET MESSAGE TO
				NEWOBJECT(lcClass,this,lcProgram)
			CASE LOWER(RIGHT(lcProgram,4))==".scx"
				IF this.lAddInTrace
					ACTIVATE SCREEN
					? "("+this.Name+") Add-in form:    "+lcProgram
				ENDIF
				SET MESSAGE TO
				DO FORM (lcProgram) NAME oAddInForm WITH this
			OTHERWISE
				IF this.lAddInTrace
					ACTIVATE SCREEN
					? "("+this.Name+") Add-in program: "+lcProgram
				ENDIF
				SET MESSAGE TO
				DO (lcProgram) WITH this
		ENDCASE
		SET ESCAPE OFF
		ON KEY LABEL F5
		SET COMPATIBLE OFF
		_vfp.AutoYield=.F.
		this.lAddInMode=llAddInMode
		this.cAddIn=""
		SET DATASESSION TO this.DataSessionID
		SELECT (lnLastSelect)
		SET MESSAGE TO
		
	ENDPROC

	PROCEDURE DragDrop
		LPARAMETERS oSource, nXCoord, nYCoord
		LOCAL lnResult,lnLastShift
		
		IF NOT this.lBrowser
			IF ISNULL(this.oItem.oAction)
				RETURN
			ENDIF
			this.oItem.oAction.DragDrop(oSource,nXCoord,nYCoord)
			RETURN
		ENDIF
		IF TYPE("oSource")#"O" OR this.lReadOnly OR oSource.parent.Name==this.Name OR ;
				NOT LEFT(oSource.parent.Name,12)=="classbrowser" OR ;
				oSource.parent.cFileName==this.cFileName OR ;
				NOT RIGHT(oSource.parent.cFileName,4)==".vcx"
			this.oSource=.NULL.
			RETURN .F.
		ENDIF
		oSource.DragIcon=""
		this.oSource=oSource
		IF this.AddInMethod(PROGRAM())
			this.oSource=.NULL.
			RETURN
		ENDIF
		this.AddClass
		this.oSource=.NULL.
		SET MESSAGE TO
		
		
	ENDPROC

	PROCEDURE DragOver
		LPARAMETERS oSource, nXCoord, nYCoord, nState
		LOCAL lcDragIcon
		
		IF TYPE("nState")#"N" OR nState=2 OR TYPE("oSource.parent")#"O" OR ;
				NOT LEFT(oSource.parent.Name,12)=="classbrowser"
			RETURN .F.
		ENDIF
		DO CASE
			CASE nState=1
				oSource.DragIcon=oSource.parent.cDragIcon
				SET MESSAGE TO
			CASE this.lBrowser AND (this.lReadOnly OR oSource.parent.Name==this.Name OR ;
					oSource.parent.cFileName==this.cFileName OR ;
					NOT RIGHT(oSource.parent.cFileName,4)==".vcx")
				oSource.DragIcon="c:\dev\browser\nodrop.cur"
			CASE NOT oSource.parent.lBrowser
				lcDragIcon="c:\dev\browser\dragmove.cur"
			OTHERWISE
				lcDragIcon=IIF(oSource.parent.nShift=2,"dragcopy.cur","dragmove.cur")
				oSource.DragIcon="c:\dev\browser\"+lcDragIcon
				SET MESSAGE TO IIF(oSource.parent.nShift=2,M_DROP_TO_COPY_LOC,M_DROP_TO_MOVE_LOC)+ ;
						[ (]+oSource.parent.cClass+[) ]+M_FROM_LOC+[ "]+oSource.parent.cFileName+ ;
						[" ]+M_TO_LOC+[ "]+this.cFileName+["]
		ENDCASE
		oSource.Refresh
		
		
	ENDPROC

	PROCEDURE Error
		LPARAMETERS nError, cMethod, nLine, oObject
		LOCAL lcErrorMsg,lcCodeLineMsg,llRuntime,oError,llIgnoreErrors
		
		this.nError=nError
		IF this.lRelease
			RETURN .F.
		ENDIF
		llRuntime=(RIGHT(this.cProgramName,4)==".app")
		IF this.lIgnoreErrors OR INLIST(nError,1113,1426,2012) OR ;
				(llRuntime AND nError=1429)
			RETURN
		ENDIF
		llIgnoreErrors=this.lIgnoreErrors
		this.lIgnoreErrors=.T.
		IF NOT this.lBrowser
			oError=this.GetObject("Error")
			IF NOT ISNULL(oError)
				IF NOT oError.Error(nError,cMethod,nLine)
					this.lIgnoreErrors=llIgnoreErrors
					RETURN
				ENDIF
			ENDIF
		ENDIF
		this.lError=.T.
		WAIT CLEAR
		IF TYPE("oObject")#"O"
			oObject=this
		ENDIF
		lcErrorMsg=MESSAGE()+CR+CR+M_BROWSER_LOC+":   "+this.Name+CR
		IF this.lAddInMode
			lcErrorMsg=lcErrorMsg+M_ADD_IN_LOC+":      "+this.cAddIn+CR
		ELSE
			lcErrorMsg=lcErrorMsg+M_OBJECT_LOC+":      "+oObject.Name+CR
		ENDIF
		lcErrorMsg=lcErrorMsg+M_ERROR_LOC+":         "+ALLTRIM(STR(nError))+CR
		lcErrorMsg=lcErrorMsg+M_METHOD_LOC+":    "+cMethod
		lcCodeLineMsg=MESSAGE(1)
		IF BETWEEN(nLine,1,10000) AND NOT lcCodeLineMsg="..."
			lcErrorMsg=lcErrorMsg+CR+M_LINE_LOC+":         "+ALLTRIM(STR(nLine))
			IF NOT EMPTY(lcCodeLineMsg)
				lcErrorMsg=lcErrorMsg+CR+CR+lcCodeLineMsg
			ENDIF
		ENDIF
		lcErrorMsg=lcErrorMsg+CR+REPLICATE("_",30)+CR+CR+M_OK_CANCEL_DESC_LOC+" "+ ;
				IIF(this.lBrowser,T_CLASS_BROWSER_LOC,T_COMPONENT_GALLERY_LOC)+"."
		IF this.MsgBox(lcErrorMsg,17)#1
			IF NOT this.lInitialized OR NOT this.lActive
				this.lPreferences=.F.
			ENDIF
			this.Release
			this.lIgnoreErrors=llIgnoreErrors
			RETURN .F.
		ENDIF
		this.lIgnoreErrors=llIgnoreErrors
		
		
	ENDPROC


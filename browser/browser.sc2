	PROCEDURE cmdClassIcon.Refresh
		LOCAL lcClass,lcBaseClass,lcClassIconBMP,lcToolTipText,lcStatusText
		LOCAL lnClassIconHeight,lnClassIconWidth,oItem,lcFileName
		
		lnClassIconHeight=this.Height
		lnClassIconWidth=this.Width
		IF thisform.lBrowser
			thisform.RefreshRecNo
		ENDIF
		lcClass=thisform.cClass
		lcBaseClass=thisform.cBaseClass
		DO CASE
			CASE NOT thisform.lBrowser
				IF thisform.nFolderListIndex<1
					thisform.imgClassIcon.Visible=.F.
					this.Visible=.F.
					RETURN .F.
				ENDIF
				lcToolTipText=""
			CASE thisform.nClassCount<=0
				thisform.imgClassIcon.Visible=.F.
				this.Visible=.F.
				RETURN .F.
			CASE thisform.lFileMode
				lcToolTipText=JUSTFNAME(thisform.cFileName)
			OTHERWISE
				lcToolTipText=thisform.ProperBaseClass(lcBaseClass)
		ENDCASE
		DO CASE
			CASE NOT thisform.lBrowser AND ISNULL(thisform.oItem)
				* lcClassIconBMP="c:\dev\browser\toolbox.ico"
				lcClassIconBMP = "toolbox.ico"
			CASE NOT thisform.lBrowser
				oItem=thisform.oItem
				IF oItem.cType=="FOLDER"
					lcClassIconBMP=LOWER(oItem.cFolderPicture)
					IF EMPTY(lcClassIconBMP)
						oParent=oItem
						DO WHILE NOT oParent.cID==oParent.oFolder.oLink.cID
							oParent=oParent.oFolder.oLink
							IF oParent.lCatalog
								EXIT
							ENDIF
							IF NOT EMPTY(oParent.cFolderPicture)
								lcClassIconBMP=oParent.cFolderPicture
								EXIT
							ENDIF
						ENDDO
						oParent=.NULL.
					ENDIF
				ELSE
					lcClassIconBMP=LOWER(oItem.cPicture)
					IF EMPTY(lcClassIconBMP)
						oParent=oItem
						DO WHILE NOT oParent.cID==oParent.oFolder.oLink.cID
							oParent=oParent.oFolder.oLink
							IF oParent.lCatalog
								EXIT
							ENDIF
							IF NOT EMPTY(oParent.cPicture)
								lcClassIconBMP=oParent.cPicture
								EXIT
							ENDIF
						ENDDO
						oParent=.NULL.
					ENDIF
				ENDIF
				DO CASE
					CASE lcClassIconBMP=="catalog.ico"
						lcClassIconBMP="openctlg.ico"
					CASE lcClassIconBMP=="folder.ico"
						lcClassIconBMP="openfldr.ico"
					CASE lcClassIconBMP=="cfldr.ico"
						lcClassIconBMP="cofldr.ico"
					CASE lcClassIconBMP=="dfldr.ico"
						lcClassIconBMP="dofldr.ico"
					CASE lcClassIconBMP=="ffldr.ico"
						lcClassIconBMP="fofldr.ico"
					CASE lcClassIconBMP=="pfldr.ico"
						lcClassIconBMP="pofldr.ico"
				ENDCASE
				IF EMPTY(lcClassIconBMP)
					DO CASE
						CASE oItem.cType=="ITEM"
							IF oItem.lWebView
								lcClassIconBMP=thisform.GetWebIcon(oItem.cFileName)
							ELSE
								lcClassIconBMP="item.ico"
							ENDIF
						CASE oItem.cType=="FOLDER"
							DO CASE
								CASE oItem.lCatalog
									lcClassIconBMP="catalog.ico"
								CASE oItem.lFolderItem
									IF oItem.lWebView
										lcClassIconBMP=thisform.GetWebIcon(oItem.cFileName)
									ELSE
										lcClassIconBMP="folder.ico"
									ENDIF
								OTHERWISE
									IF oItem.lWebView
										lcClassIconBMP=thisform.GetWebIcon(oItem.cFileName)
									ELSE
										lcClassIconBMP="openfldr.ico"
									ENDIF
							ENDCASE
						OTHERWISE
							lcClassIconBMP="item.ico"
					ENDCASE
					lcFileName=LOWER(lcClassIconBMP)
				ELSE
					IF NOT ":"$lcClassIconBMP AND NOT "\\"$lcClassIconBMP
						IF "\"$lcClassIconBMP
							lcClassIconBMP=LOWER(ALLTRIM(FULLPATH(oItem.cCatalogPath+lcClassIconBMP, ;
									thisform.cFileName)))
						ELSE
							lcClassIconBMP=oItem.cCatalogPath+lcClassIconBMP
						ENDIF
					ENDIF
				ENDIF
				lcFileName=LOWER(JUSTFNAME(lcClassIconBMP))
				lcToolTipText=oItem.cToolTipText
				lcStatusText=oItem.cStatusBarText
				IF NOT FILE(lcClassIconBMP)
					lcClassIconBMP=FORCEPATH(lcClassIconBMP,"c:\dev\browser\")
					IF NOT FILE(lcClassIconBMP)
		*				lcClassIconBMP="c:\dev\browser\toolbox.ico"
						lcClassIconBMP="toolbox.ico"
					ENDIF
				ENDIF
				oItem=.NULL.
			CASE thisform.lVCXSCXMode
				IF USED(thisform.cAlias)
					SELECT (thisform.cAlias)
					lcClassIconBMP=ALLTRIM(Reserved5)
					IF RIGHT(lcClassIconBMP,1)=="\" OR RIGHT(lcClassIconBMP,1)=="."
						lcClassIconBMP=""
					ENDIF
				ELSE
		*			lcClassIconBMP="c:\dev\browser\toolbox.ico"
					lcClassIconBMP="toolbox.ico"
				ENDIF
				IF EMPTY(lcClassIconBMP)
					IF "."$lcClass
		*				lcClassIconBMP="c:\dev\browser\"+thisform.ClassIcon(lcClass)
						lcClassIconBMP=thisform.ClassIcon(lcClass)
					ELSE
		*				lcClassIconBMP="c:\dev\browser\"+thisform.ClassIcon(lcBaseClass)
						lcClassIconBMP=thisform.ClassIcon(lcBaseClass)
					ENDIF
				ELSE
					IF NOT ":"$lcClassIconBMP AND NOT "\\"$lcClassIconBMP
						IF "\"$lcClassIconBMP
							lcClassIconBMP=LOWER(ALLTRIM(FULLPATH(lcClassIconBMP,thisform.cFileName)))
						ELSE
							lcClassIconBMP=thisform.cFileNamePath+lcClassIconBMP
						ENDIF
					ENDIF
				ENDIF
				IF NOT FILE(lcClassIconBMP)
					lcClassIconBMP=FORCEPATH(lcClassIconBMP,"c:\dev\browser\")
					IF NOT FILE(lcClassIconBMP)
		*				lcClassIconBMP="c:\dev\browser\toolbox.ico"
						lcClassIconBMP="toolbox.ico"
					ENDIF
				ENDIF
			OTHERWISE
		*	 	lcClassIconBMP="c:\dev\browser\ole.ico"
			 	lcClassIconBMP="ole.ico"
		ENDCASE
		IF thisform.lBrowser
			IF thisform.lFileMode
				IF thisform.lVCXSCXMode
					lcStatusText=IIF(thisform.lSCXMode,M_FORM_LOC,M_CLASS_LIBRARY_LOC)
				ELSE
					lcStatusText=M_TYPELIB_LOC
				ENDIF
				lcToolTipText=lcToolTipText+" ("+lcStatusText+")"
			ELSE
				lcToolTipText=lcClass+" ("+lcToolTipText+")"
			ENDIF
		ENDIF
		thisform.imgClassIcon.Picture=lcClassIconBMP
		thisform.imgClassIcon.Height=lnClassIconHeight
		thisform.imgClassIcon.Width=lnClassIconWidth
		this.ToolTipText=LEFT(lcToolTipText,127)
		this.StatusBarText=""
		IF NOT thisform.imgClassIcon.Visible
			thisform.imgClassIcon.Visible=.T.
		ENDIF
		IF NOT this.Visible
			this.Visible=.T.
		ENDIF
		SELECT 0
		
	ENDPROC

	PROCEDURE cmdClassIcon.RightClick
		LOCAL lcOldClassIconBMP,lcNewClassIconBMP,llIconMatch,lcPicture,lcClass,llRefreshMode
		LOCAL lcFileNameCaption,lcSetDefault,lcLastSetDefault,lnIndex,lnLastSelect
		
		SET DATASESSION TO (thisform.DataSessionID)
		DO CASE
			CASE thisform.nShift=1
				RETURN thisform.FormAddObject(_screen,0,0)
			CASE thisform.nShift=2
				RETURN thisform.FormAddObject("command")
		ENDCASE
		lnLastSelect=SELECT()
		WITH thisform
			IF .lBrowser
				IF .lSCXMode OR NOT .lVCXSCXMode OR .lReadOnly OR NOT USED(.cAlias)
					RETURN .F.
				ENDIF
				.RefreshRecNo
				SELECT (.cAlias)
				lcOldClassIconBMP=ALLTRIM(MLINE(Reserved5,1))
				lcFileNameCaption=JUSTFNAME(lcOldClassIconBMP)
				lcOldClassIconBMP=IIF(EMPTY(lcOldClassIconBMP),"", ;
						LOWER(ALLTRIM(FULLPATH(lcOldClassIconBMP,.cFileName))))
			ELSE
				IF ISNULL(.oItem) OR .oItem.lDynamic
					RETURN .F.
				ENDIF
				lcOldClassIconBMP=IIF(.oItem.lFolder,.oItem.cFolderPicture,.oItem.cPicture)
				lcFileNameCaption=JUSTFNAME(lcOldClassIconBMP)
			ENDIF
		ENDWITH
		IF EMPTY(lcFileNameCaption)
			lcFileNameCaption=M_FILE_NAME_LOC
		ENDIF
		SELECT 0
		lcSetDefault=JUSTPATH(lcOldClassIconBMP)
		IF NOT EMPTY(lcSetDefault) AND DIRECTORY(lcSetDefault)
			lcLastSetDefault=SET("DEFAULT")+SYS(2003)
			CD (lcSetDefault)
		ELSE
			lcLastSetDefault=""
		ENDIF
		lcNewClassIconBMP=LOWER(GETPICT("ico;bmp",lcFileNameCaption+":"))
		IF NOT EMPTY(lcLastSetDefault)
			CD (lcLastSetDefault)
		ENDIF
		IF NOT thisform.lBrowser
			IF EMPTY(lcNewClassIconBMP)
				IF EMPTY(lcOldClassIconBMP)
					SELECT (lnLastSelect)
					RETURN .F.
				ENDIF
				IF thisform.MsgBox(M_RESET_LOC+" ("+thisform.oItem.cText+") "+ ;
						M_ICON_TO_DEFAULT_LOC+"?",292)#6
					SELECT (lnLastSelect)
					RETURN .F.
				ENDIF
			ENDIF
			WITH thisform.oItem
				IF .lFolder
					.cFolderPicture=lcNewClassIconBMP
				ELSE
					.cPicture=lcNewClassIconBMP
				ENDIF
				.WriteProperties(.T.)
				.Refresh(.T.)
			ENDWITH
			SELECT (lnLastSelect)
			RETURN
		ENDIF
		IF EMPTY(lcNewClassIconBMP)
			IF EMPTY(lcOldClassIconBMP)
				SELECT (lnLastSelect)
				RETURN .F.
			ENDIF
			lcClass=thisform.cClass
			IF thisform.lFileMode
				lcClass=IIF(thisform.lSCXMode,M_FORM_LOC,M_CLASS_LIBRARY_LOC)+" "+lcClass
			ELSE
				lcClass=LOWER(M_CLASS_LOC)+" ("+lcClass+")"
			ENDIF
			IF thisform.MsgBox(M_RESET_LOC+" "+lcClass+" "+ ;
					M_ICON_TO_DEFAULT_LOC+"?",292)#6
				SELECT (lnLastSelect)
				RETURN .F.
			ENDIF
		ENDIF
		thisform.RefreshRecNo
		SELECT (thisform.cAlias)
		llIconMatch=(Reserved4==Reserved5)
		lcPicture=IIF(EMPTY(lcNewClassIconBMP),"", ;
				LOWER(SYS(2014,lcNewClassIconBMP,thisform.cFileName)))
		IF NOT Reserved5==lcPicture
			thisform.ClearClass
			REPLACE Reserved5 WITH lcPicture
			lnIndex=thisform.nClassListIndex+1
			thisform.aClassList[lnIndex,7]=lcPicture
			thisform.RefreshClassIcon(lnIndex)
			DOEVENTS
		ENDIF
		SELECT (thisform.cAlias)
		IF NOT Reserved5==lcPicture
			SELECT (lnLastSelect)
			RETURN .F.
		ENDIF
		IF llIconMatch AND NOT Reserved4==lcPicture
			thisform.ClearClass
			REPLACE Reserved4 WITH lcPicture
		ENDIF
		llRefreshMode=thisform.lRefreshMode
		thisform.lRefreshMode=.T.
		this.Refresh
		thisform.lRefreshMode=llRefreshMode
		SELECT (lnLastSelect)
		
	ENDPROC


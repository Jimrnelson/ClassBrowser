				this.QueryUnload
				this.Destroy
				RETURN .F.
			ENDIF
			ERASE (lcBrowserDBF)
			ERASE (lcBrowserFPT)
			CREATE (lcBrowserDBF) FROM brwstruc
			APPEND FROM (lcBrowser2DBF)
			USE
			ERASE (lcBrowser2DBF)
			ERASE (lcBrowser2FPT)
			USE (lcBrowserDBF) AGAIN SHARED ALIAS browser
			REPLACE ALL PCBrowser WITH .T. FOR UPPER(ALLTRIM(Platform))==this.cPlatform AND ;
					UPPER(ALLTRIM(Type))=="PREFW" AND ;
					UPPER(ALLTRIM(ID))=="FORMINFO"
			IF RECCOUNT()>0 AND FILE(this.cDefaultBrowserTable)
				IF USED("brwdflt")
					USE IN brwdflt
				ENDIF
				SELECT 0
				USE (this.cDefaultBrowserTable) AGAIN SHARED ALIAS brwdflt
				SCAN ALL FOR NOT DELETED() AND Backup AND NOT EMPTY(Name)
					lcFileName=JUSTFNAME(LOWER(ALLTRIM(MLINE(Name,1))))
					SELECT browser
					LOCATE FOR NOT DELETED() AND JUSTFNAME(LOWER(ALLTRIM(MLINE(Name,1))))==lcFileName
					IF NOT EOF()
						REPLACE Backup WITH .T.
					ENDIF
					SELECT brwdflt
				ENDSCAN
				USE IN brwdflt
				SELECT browser
			ENDIF
			LOCATE
		ENDIF
		CURSORSETPROP("Buffering",1)

	PROCEDURE oleMembers.KeyDown
		*** OLE Control Event ***
		LPARAMETERS keycode, shift
		
		IF thisform.lRelease OR TYPE("this.object.SelectedItem.Expanded")#"L"
			RETURN
		ENDIF
		thisform.nMouseButton=-1
		thisform.nShift=0
		thisform.lExpanded=this.object.SelectedItem.Expanded
		DO CASE
			CASE keycode=116
				NODEFAULT
				thisform.RefreshBrowser
				RETURN
			CASE keycode=121
				NODEFAULT
				IF shift=1
					thisform.DisplayMenu(2)
				ENDIF
				RETURN
			CASE keycode=112
				NODEFAULT
				HELP ID (thisform.HelpContextID) NOWAIT
				RETURN
		ENDCASE
		
	ENDPROC

	PROCEDURE oleMembers.KeyPress
		*** OLE Control Event ***
		LPARAMETERS keyascii
		LOCAL lnIndex
		
		WAIT CLEAR
		IF thisform.lRelease OR TYPE("this.object.SelectedItem.Expanded")#"L"
			RETURN
		ENDIF
		thisform.nKeyASCII=keyascii
		IF keyascii=27
			NODEFAULT
			thisform.cmdOpen.SetFocus
			thisform.NewObject("tmrReleaseBrowser","_ReleaseBrowserTimer","browser.vcx")
			RETURN
		ENDIF
		lnIndex=this.object.SelectedItem.Index
		DO CASE
			CASE keyascii=32
				NODEFAULT
				thisform.nMouseButton=-2
				this.DblClick
				thisform.nMouseButton=-3
				IF TYPE("this.object.Nodes[lnIndex].Selected")=="L"
					thisform.nShift=lnIndex
					this.object.Nodes[lnIndex].Selected=.T.
				ELSE
					thisform.nShift=-1
				ENDIF
				RETURN
		ENDCASE
		
	ENDPROC

	PROCEDURE oleMembers.MouseDown
		*** OLE Control Event ***
		LPARAMETERS button, shift, x, y
		
		IF thisform.lRelease OR TYPE("this.object.SelectedItem.Expanded")#"L"
			RETURN
		ENDIF
		thisform.lMouseDown=.T.
		thisform.nMouseButton=IIF(TYPE("button")=="N",button,0)
		thisform.nShift=shift
		thisform.lExpanded=this.object.SelectedItem.Expanded
		
	ENDPROC

	PROCEDURE oleMembers.MouseUp
		*** OLE Control Event ***
		LPARAMETERS button, shift, x, y
		
		thisform.lMouseDown=.F.
		
	ENDPROC

	PROCEDURE oleMembers.NodeClick
		*** OLE Control Event ***
		LPARAMETERS node
		LOCAL lnIndex,lnListIndex,lcMember,lcViewCodeFileName,lcViewCodeWindow,lcSearch,lnRange
		
		IF thisform.lRelease
			RETURN
		ENDIF
		ACTIVATE SCREEN
		lnIndex=Node.Index
		IF TYPE("this.object.Nodes[lnIndex].Selected")#"L"
			RETURN
		ENDIF
		thisform.nMemberListIndex=lnIndex
		IF thisform.nMouseButton=-3
			this.object.Nodes[thisform.nShift].Selected=.T.
			thisform.nMouseButton=-1
			RETURN .F.
		ENDIF
		IF thisform.lMouseDown
			this.object.Nodes[lnIndex].Selected=.T.
		ELSE
			thisform.nMouseButton=-1
		ENDIF
		WITH thisform
			.RefreshCaption
			.RefreshButtons
			.edtMemberDesc.Refresh
			IF .nMemberListIndex>=1 AND .aMemberList[.nMemberListIndex,2]=="m"
				lcMember=LOWER(.aMemberList[.nMemberListIndex,1])
				lcViewCodeFileName=LOWER(.cViewCodeFileName)
				IF NOT EMPTY(lcViewCodeFileName) AND LOWER(JUSTEXT(lcViewCodeFileName))=="prg"
					lcViewCodeWindow=JUSTFNAME(lcViewCodeFileName)
					IF WEXIST(lcViewCodeWindow)
						RELEASE WINDOW (lcViewCodeWindow)
						lcSearch="PROCEDURE "+lcMember+CR_LF
						lnRange=ATC(lcSearch,FILETOSTR(lcViewCodeFileName))
						IF lnRange>0
							lnRange=lnRange+LEN(lcSearch)
						ENDIF
						WAIT CLEAR
						DOEVENTS
						MODIFY COMM (lcViewCodeFileName) RANGE lnRange,lnRange IN SCREEN NOWAIT
					ENDIF
				ENDIF
			ENDIF
		ENDWITH
		
	ENDPROC

	PROCEDURE oleMembers.Refresh
		*** OLE Control Method ***
		LOCAL lnChildCount,laObjects,laObjects2,laObjectRecNo,laBaseClasses,lnObjectCount
		LOCAL lcSeparator,lcProtectedSeparator,lcEmptySeparator,lcHiddenSeparator
		LOCAL lcItem,lcItem2,laMethods,lnMethodCount,laProperties,lnPropertyCount
		LOCAL laProtected,lnProtectedCount,laHidden,lnHiddenCount
		LOCAL laInstances,lnInstanceCount,lnCount,lnImageIndex,llAccessAssignMethod
		LOCAL llShowProtected,llProtectedMatch,llShowEmpty,llShowHidden,llHiddenMatch
		LOCAL llEnabled,llVisible,lnRecNo,lnAtPos,llMethod,lcObjName
		
		IF thisform.lRelease
			RETURN .F.
		ENDIF
		IF NOT thisform.lBrowser
			RETURN
		ENDIF
		IF NOT thisform.lRefreshMode
			thisform.lRefreshMode=.T.
			RETURN .F.
		ENDIF
		WITH thisform
			DIMENSION .aMemberList[1,3]
			.aMemberList=""
			.nMemberCount=0
			.nMemberListIndex=-1
			DIMENSION .aInstances[1]
			.aInstances=""
			.nInstances=0
			.nTimeStamp=-1
			.RefreshRecNo
		ENDWITH
		IF NOT thisform.lVCXSCXMode
			RETURN
		ENDIF
		IF EMPTY(thisform.cClass) OR thisform.lFileMode OR NOT USED(thisform.cAlias)
			thisform.ClearNodes(this)
			IF EMPTY(thisform.cClass)
				RETURN
			ENDIF
		ENDIF
		llShowProtected=thisform.lProtectedFilter
		llShowHidden=thisform.lHiddenFilter
		llShowEmpty=thisform.lEmptyFilter
		lcSeparator=SPACE(6)
		lcProtectedSeparator="*"+SPACE(5)
		DIMENSION laProtected[1]
		laProtected=""
		lnProtectedCount=0
		lcHiddenSeparator="^"+SPACE(5)
		DIMENSION laHidden[1]
		laHidden=""
		lnHiddenCount=0
		DIMENSION laObjects[1]
		laObjects=""
		DIMENSION laObjects2[1]
		laObjects2=""
		DIMENSION laObjectRecNo[1]
		laObjectRecNo=0
		DIMENSION laBaseClasses[1]
		laBaseClasses=""
		lnObjectCount=0
		DIMENSION laMethods[1]
		laMethods=""
		lnMethodCount=0
		DIMENSION laProperties[1]
		laProperties=""
		lnPropertyCount=0
		DIMENSION laInstances[1]
		laInstances=""
		lnInstanctCount=0
		IF thisform.lFileMode
			RETURN
		ENDIF
		llVisible=this.Visible
		this.Visible=.F.
		llEnabled=this.Enabled
		this.Enabled=.F.
		SELECT (thisform.cAlias)
		thisform.nTimeStamp=IIF(TYPE("Timestamp")=="N",Timestamp,0)
		IF NOT EMPTY(thisform.cClass) AND NOT thisform.lFileMode
			thisform.nInstances=AINSTANCE(thisform.aInstances,thisform.cClass)
			IF thisform.nInstances=0
				DIMENSION thisform.aInstances[1]
				thisform.aInstances=""
			ENDIF
		ENDIF
		lcObjName=LOWER(ALLTRIM(ObjName))
		IF thisform.lSCXMode
			lnRecNo=RECNO()
			SKIP
			COUNT REST FOR ALLTRIM(LOWER(Parent))==thisform.cClass ;
					TO lnChildCount
			GO lnRecNo
			FOR lnCount = 1 TO lnChildCount
				SKIP
				IF NOT ALLTRIM(LOWER(Parent))==thisform.cClass
					lnCount=lnCount-1
					LOOP
				ENDIF
				lcItem=LOWER(ALLTRIM(ObjName))
				lcItem=lcSeparator+lcItem
				lnObjectCount=lnObjectCount+1
				DIMENSION laObjects[lnObjectCount],laObjects2[lnObjectCount]
				DIMENSION laObjectRecNo[lnObjectCount],laBaseClasses[lnObjectCount]
				laObjects[lnObjectCount]=lcItem
				laObjects2[lnObjectCount]="  ("+thisform.ProperBaseClass(BaseClass)+")"
				laObjectRecno[lnObjectCount]=RECNO()
				laBaseClasses[lnObjectCount]=BaseClass
			ENDFOR
		ELSE
			lnChildCount=VAL(Reserved2)-1
			FOR lnCount = 1 TO lnChildCount
				SKIP
				IF NOT llShowProtected AND NOT EMPTY(Protected) OR ;
						NOT LOWER(ALLTRIM(Parent))==lcObjName
					LOOP
				ENDIF
				lcItem=LOWER(ALLTRIM(ObjName))
				lcItem=IIF(EMPTY(Protected),lcSeparator,lcProtectedSeparator)+lcItem
				lnObjectCount=lnObjectCount+1
				DIMENSION laObjects[lnObjectCount],laObjects2[lnObjectCount]
				DIMENSION laObjectRecNo[lnObjectCount],laBaseClasses[lnObjectCount]
				laObjects[lnObjectCount]=lcItem
				laObjects2[lnObjectCount]="  ("+thisform.ProperBaseClass(BaseClass)+")"+ ;
						IIF(EMPTY(Reserved8),"","  (NOINIT)")
				laObjectRecNo[lnObjectCount]=RECNO()
				laBaseClasses[lnObjectCount]=BaseClass
			ENDFOR
		ENDIF
		thisform.RefreshRecNo
		SELECT (thisform.cAlias)
		_mline=0
		FOR lnCount = 1 TO MEMLINE(Protected)
			lcItem=LOWER(ALLTRIM(MLINE(Protected,1,_mline)))
			IF EMPTY(lcItem)
				LOOP
			ENDIF
			IF RIGHT(lcItem,1)=="^"
				lcItem=ALLTRIM(LEFT(lcItem,LEN(lcItem)-1))
				lnHiddenCount=lnHiddenCount+1
				DIMENSION laHidden[lnHiddenCount]
				laHidden[lnHiddenCount]=lcItem+" "
			ELSE
				lnProtectedCount=lnProtectedCount+1
				DIMENSION laProtected[lnProtectedCount]
				laProtected[lnProtectedCount]=lcItem+" "
			ENDIF
		ENDFOR
		_mline=0
		FOR lnCount = 1 TO MEMLINE(Reserved3)
			lcItem=LOWER(ALLTRIM(MLINE(Reserved3,1,_mline)))
			lnAtPos=AT(" ",lcItem)
			IF lnAtPos>0
				lcItem=ALLTRIM(LEFT(lcItem,lnAtPos-1))
			ENDIF
			IF EMPTY(lcItem)
				LOOP
			ENDIF
			llMethod=(LEFT(lcItem,1)=="*")
			IF LEFT(lcItem,1)=="^"
				lcItem=STRTRAN(STRTRAN(STRTRAN(ALLTRIM(SUBSTR(lcItem,2)), ;
						"(","["),")","]"),",0]","]")
			ENDIF
			lcItem=STRTRAN(STRTRAN(LOWER(ALLTRIM(IIF(llMethod,SUBSTR(lcItem,2),lcItem))),"(","["),")","]")
			lcItem2=lcItem
			lnAtPos=AT("[",lcItem2)
			IF lnAtPos>0
				lcItem2=ALLTRIM(LEFT(lcItem2,lnAtPos-1))
			ENDIF
			llProtectedMatch=(ASCAN(laProtected,lcItem2+" ")>0)
			IF NOT llShowProtected AND llProtectedMatch
				LOOP
			ENDIF
			llHiddenMatch=(ASCAN(laHidden,lcItem2+" ")>0)
			llAccessAssignMethod=.F.
			IF llMethod AND NOT llHiddenMatch AND NOT llProtectedMatch
				IF INLIST(RIGHT(lcItem2,7),"_access","_assign")
					llAccessAssignMethod=.T.
					llProtectedMatch=.T.
				ENDIF
			ENDIF
			IF NOT llShowHidden AND llHiddenMatch
				LOOP
			ENDIF
			lcEmptySeparator=""
		
			IF llMethod AND ATC(CR+"PROCEDURE "+lcItem+CR,CR+Methods+CR)=0 AND ;
					ATC(LF+"PROCEDURE "+lcItem+CR,LF+Methods+CR)=0
				lcEmptySeparator="~"
				IF NOT llShowEmpty
					LOOP
				ENDIF
			ENDIF
			
			
			DO CASE
				CASE (llShowProtected OR llAccessAssignMethod) AND llProtectedMatch
					lcItem=lcProtectedSeparator+lcItem
				CASE llShowHidden AND llHiddenMatch
					lcItem=lcHiddenSeparator+lcItem
				OTHERWISE
					lcItem=lcSeparator+lcItem
			ENDCASE
			IF NOT EMPTY(lcEmptySeparator)
				lcItem=STUFF(lcItem,3,1,lcEmptySeparator)
			ENDIF
			IF llMethod
				lnMethodCount=lnMethodCount+1
				DIMENSION laMethods[lnMethodCount]
				laMethods[lnMethodCount]=lcItem
			ELSE
				lnPropertyCount=lnPropertyCount+1
				DIMENSION laProperties[lnPropertyCount]
				laProperties[lnPropertyCount]=lcItem
			ENDIF
		ENDFOR
		lnInstanceCount=AINSTANCE(laInstances,thisform.cClass)
		IF lnMethodCount>0
			=ASORT(laMethods)
		ENDIF
		IF lnPropertyCount>0
			=ASORT(laProperties)
		ENDIF
		thisform.ClearNodes(this)
		IF NOT USED(thisform.cAlias)
			RETURN
		ENDIF
		IF lnObjectCount>0
			oNode=this.object.Nodes.Add(,4,"Objects","Objects")
			IF thisform.lAutoExpand
				oNode.Expanded=.T.
			ENDIF
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]="Objects"
				.aMemberList[.nMemberCount,2]=""
				.aMemberList[.nMemberCount,3]=""
			ENDWITH
		ENDIF
		FOR lnCount = 1 TO lnObjectCount
			lcItem=laObjects[lnCount]
			oNode=this.object.Nodes.Add("Objects",4,lcItem,lcItem+laObjects2[lnCount],,)
			oNode.Image=thisform.ClassIcon(laBaseClasses[lnCount])
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]=ALLTRIM(CHRTRAN(lcItem,"*~^",""))
				.aMemberList[.nMemberCount,2]="o"
				.aMemberList[.nMemberCount,3]=laObjectRecNo[lnCount]
			ENDWITH
		ENDFOR
		IF lnMethodCount>0
			oNode=this.object.Nodes.Add(,4,"Methods","Methods")
			IF thisform.lAutoExpand
				oNode.Expanded=.T.
			ENDIF
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]="Methods"
				.aMemberList[.nMemberCount,2]=""
				.aMemberList[.nMemberCount,3]=""
			ENDWITH
		ENDIF
		FOR lnCount = 1 TO lnMethodCount
			lcItem=laMethods[lnCount]
			oNode=this.object.Nodes.Add("Methods",4,lcItem,lcItem,,)
			DO CASE
				CASE ALLTRIM(lcProtectedSeparator)$lcItem
					lnImageIndex=thisform.GetImageIndex(.T.,"methodc.ico")
				CASE ALLTRIM(lcHiddenSeparator)$lcItem
					lnImageIndex=thisform.GetImageIndex(.T.,"methodh.ico")
				OTHERWISE
					lnImageIndex=thisform.GetImageIndex(.T.,"method.ico")
			ENDCASE
			IF lnImageIndex>0
				oNode.Image=ALLTRIM(thisform.aImageList[lnImageIndex])
			ENDIF
			lnAtPos=AT(" ",lcItem)
			IF lnAtPos>0
				lcItem=ALLTRIM(SUBSTR(lcItem,lnAtPos+1))
			ENDIF
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]=ALLTRIM(CHRTRAN(lcItem,"*~^",""))
				.aMemberList[.nMemberCount,2]="m"
				.aMemberList[.nMemberCount,3]=""
			ENDWITH
		ENDFOR
		IF lnPropertyCount>0
			oNode=this.object.Nodes.Add(,4,"Properties","Properties")
			IF thisform.lAutoExpand
				oNode.Expanded=.T.
			ENDIF
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]="Properties"
				.aMemberList[.nMemberCount,2]=""
				.aMemberList[.nMemberCount,3]=""
			ENDWITH
		ENDIF
		FOR lnCount = 1 TO lnPropertyCount
			lcItem=laProperties[lnCount]
			lcItem2=""
			oNode=this.object.Nodes.Add("Properties",4,lcItem,lcItem,,)
			DO CASE
				CASE ALLTRIM(lcProtectedSeparator)$lcItem
					lnImageIndex=thisform.GetImageIndex(.T.,"propc.ico")
				CASE ALLTRIM(lcHiddenSeparator)$lcItem
					lnImageIndex=thisform.GetImageIndex(.T.,"proph.ico")
				OTHERWISE
					lnImageIndex=thisform.GetImageIndex(.T.,"prop.ico")
			ENDCASE
			IF lnImageIndex>0
				oNode.Image=ALLTRIM(thisform.aImageList[lnImageIndex])
			ENDIF
			lnAtPos=AT("[",lcItem)
			IF lnAtPos>0
				lcItem2=ALLTRIM(SUBSTR(lcItem,lnAtPos))
				lcItem=ALLTRIM(LEFT(lcItem,lnAtPos-1))
			ENDIF
			lnAtPos=AT(" ",lcItem)
			IF lnAtPos>0
				lcItem=ALLTRIM(SUBSTR(lcItem,lnAtPos+1))
			ENDIF
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]=ALLTRIM(CHRTRAN(lcItem,"*~^",""))
				.aMemberList[.nMemberCount,2]="p"
				.aMemberList[.nMemberCount,3]=lcItem2
			ENDWITH
		ENDFOR
		IF lnInstanceCount>0
			oNode=this.object.Nodes.Add(,4,"Instances","Instances")
			IF thisform.lAutoExpand
				oNode.Expanded=.T.
			ENDIF
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]="Instances"
				.aMemberList[.nMemberCount,2]=""
				.aMemberList[.nMemberCount,3]=""
			ENDWITH
		ENDIF
		FOR lnCount = 1 TO lnInstanceCount
			lcItem=laInstances[lnCount]
			oNode=this.object.Nodes.Add("Instances",4,lcItem,lcItem,,)
			lnImageIndex=thisform.GetImageIndex(.T.,"instance.ico")
			IF lnImageIndex>0
				oNode.Image=ALLTRIM(thisform.aImageList[lnImageIndex])
			ENDIF
			lnAtPos=AT(" ",lcItem)
			IF lnAtPos>0
				lcItem=ALLTRIM(SUBSTR(lcItem,lnAtPos+1))
			ENDIF
			WITH thisform
				.nMemberCount=.nMemberCount+1
				DIMENSION .aMemberList[.nMemberCount,3]
				.aMemberList[.nMemberCount,1]=ALLTRIM(CHRTRAN(lcItem,"*~^",""))
				.aMemberList[.nMemberCount,2]="i"
				.aMemberList[.nMemberCount,3]=""
			ENDWITH
		ENDFOR
		IF this.Enabled#llEnabled
			this.Enabled=llEnabled
		ENDIF
		IF this.Visible#llVisible
			this.Visible=llVisible
		ENDIF
		thisform.RefreshRecNo
		SELECT 0
		
	ENDPROC


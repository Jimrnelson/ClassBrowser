	PROCEDURE getmethod
		LPARAMETERS tcMethod,tlParamsOnly
		LOCAL lcMethod,lcCode,lcMethods,lcSearchStr,lnAtPos,lnLastSelect
		
		IF VARTYPE(tcMethod)#"C" OR EMPTY(tcMethod)
			RETURN ""
		ENDIF
		lcMethod=LOWER(ALLTRIM(tcMethod))
		lnLastSelect=SELECT()
		this.RefreshRecNo
		SELECT (this.cAlias)
		lcMethods=CR_LF+ALLTRIM(Methods)+CR_LF
		SELECT (lnLastSelect)
		lcSearchStr=CR_LF+"PROCEDURE "+lcMethod+CR
		lnAtPos=ATC(lcSearchStr,lcMethods)
		IF lnAtPos=0
			RETURN ""
		ENDIF
		lcCode=SUBSTR(lcMethods,lnAtPos+LEN(lcSearchStr)+1)
		lcSearchStr=CR_LF+"ENDPROC"+CR_LF
		lnAtPos=AT(lcSearchStr,lcCode)
		IF lnAtPos>0
			lcCode=LEFT(lcCode,lnAtPos-1)
		ENDIF
		IF tlParamsOnly
			lcCode=this.GetParams(lcCode)
		ENDIF
		RETURN lcCode
		
	ENDPROC


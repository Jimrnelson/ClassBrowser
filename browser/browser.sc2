	ENDPROC

	PROCEDURE addhistoryitem
		LPARAMETERS tcPrompt,tcItem,tcFileName
		LOCAL lcPrompt,lcItem,lcFileName,lnIndex
		
		IF this.lRelease OR NOT this.lInitialized OR this.lRefreshBrowserMode OR ;
				EMPTY(tcPrompt) OR EMPTY(tcItem)
			RETURN .F.
		ENDIF
		IF this.nHistoryMaxLength<1
			DIMENSION this.aHistory[1,4]
			this.aHistory=""
			this.nHistoryCount=0
		ENDIF
		lcPrompt=ALLTRIM(tcPrompt)
		lcItem=ALLTRIM(tcItem)
		lcFileName=IIF(EMPTY(tcFileName),"",ALLTRIM(tcFileName))
		lnIndex=INT((ASCAN(this.aHistory,lcItem+" ")+3)/4)
		IF lnIndex>this.nHistoryMaxLength
			lnIndex=0
		ENDIF
		IF lnIndex=0
			this.nHistoryCount=MIN(this.nHistoryCount+1,this.nHistoryMaxLength)
			DIMENSION this.aHistory[this.nHistoryCount,4]
		ELSE
			ADEL(this.aHistory,lnIndex)
		ENDIF
		AINS(this.aHistory,1)
		this.aHistory[1,1]=lcPrompt
		this.aHistory[1,2]=lcItem+" "
		this.aHistory[1,3]=lcFileName
		this.aHistory[1,4]=this.lBrowser
		
	ENDPROC

	PROCEDURE addin
		LPARAMETERS tcName,tcProgram,tcMethod,tcFileFilter,tcPlatform,tcComment
		LOCAL lcName,lcFileFilter,lcFileFilter2
		LOCAL lnSelect,lnRecNo,lnAtPos,lnCount,lcFileName
		
		IF this.lRelease OR EMPTY(tcName) OR TYPE("tcName")#"C"
			RETURN .F.
		ENDIF
		lcName=TRIM(tcName)
		SET DATASESSION TO this.DataSessionID
		IF NOT USED("browser")
			SET DATASESSION TO this.DataSessionID
			IF NOT USED("browser")
				RETURN .F.
			ENDIF
		ENDIF
		lnSelect=SELECT()
		SELECT browser
		IF EOF()
			LOCATE
		ENDIF
		lnRecNo=IIF(EOF() OR RECNO()>RECCOUNT(),0,RECNO())
		LOCATE FOR UPPER(ALLTRIM(Type))=="ADDIN" AND UPPER(TRIM(Name))==UPPER(lcName)
		IF ISNULL(tcProgram)
			IF EOF()
				IF lnRecNo>0
					GO lnRecNo
				ELSE
					GO BOTTOM
					IF NOT EOF()
						SKIP
					ENDIF
				ENDIF
				SELECT (lnSelect)
				RETURN .F.
			ENDIF
			DELETE
			IF lnRecNo>0
				GO lnRecNo
			ELSE
				GO BOTTOM
				IF NOT EOF()
					SKIP
				ENDIF
			ENDIF
			SELECT (lnSelect)
			RETURN
		ENDIF
		IF EOF()
			APPEND BLANK
			REPLACE Type WITH "ADDIN", ;
					Name WITH lcName, ;
					ID WITH "MENU"
		ENDIF
		IF TYPE("tcMethod")=="C"
			REPLACE ID WITH IIF(EMPTY(tcMethod),"MENU","METHOD"), ;
					Method WITH UPPER(tcMethod)
		ENDIF
		IF TYPE("tcProgram")=="C"
			lnAtPos=AT(",",tcProgram)
			IF lnAtPos=0
				REPLACE Program WITH LOWER(tcProgram), ;
						ClassLib WITH "", ;
						ClassName WITH ""
			ELSE
				REPLACE Program WITH "", ;
						ClassLib WITH LOWER(ALLTRIM(LEFT(tcProgram,lnAtPos-1))), ;
						ClassName WITH LOWER(ALLTRIM(SUBSTR(tcProgram,lnAtPos+1)))
			ENDIF
		ENDIF
		IF TYPE("tcFileFilter")=="C"
			IF EMPTY(tcFileFilter)
				lcFileFilter=""
			ELSE
				lcFileFilter2=LOWER(STRTRAN(tcFileFilter,",",CR))+CR
				lcFileFilter=""
				FOR lnCount = 1 TO MEMLINE(lcFileFilter2)
					lcFileName=MLINE(lcFileFilter2,lnCount)
					IF EMPTY(lcFileName)
						LOOP
					ENDIF
					IF NOT "."$lcFileName
						lcFileName=lcFileName+".vcx"
					ENDIF
					lcFileFilter=lcFileFilter+lcFileName+CR
				ENDFOR
			ENDIF
			REPLACE FileFilter WITH lcFileFilter
		ENDIF
		IF TYPE("tcPlatform")=="C" AND TYPE("Platform")=="C"
			REPLACE Platform WITH tcPlatform
		ENDIF
		IF TYPE("tcComment")=="C" AND TYPE("Comment")=="M"
			REPLACE Comment WITH tcComment
		ENDIF
		IF TYPE("Updated")=="T"
			REPLACE Updated WITH DATETIME()
		ENDIF
		IF lnRecNo>0
			GO lnRecNo
		ELSE
			GO BOTTOM
			IF NOT EOF()
				SKIP
			ENDIF
		ENDIF
		SELECT (lnSelect)
		RETURN
		
	ENDPROC

	PROCEDURE addinmenu
		LOCAL laMenu,lnMenuCount,lcProgram,lnSelect,lnRecNo,lcFileName
		
		IF this.lRelease
			RETURN .F.
		ENDIF
		this.cAddIn=""
		this.cAddInMethod=""
		DIMENSION laMenu[1]
		SET DATASESSION TO this.DataSessionID
		IF NOT USED("browser")
			SET DATASESSION TO this.DataSessionID
			IF NOT USED("browser")
				RETURN .F.
			ENDIF
		ENDIF
		IF this.lBrowser
			lcFileName=this.cFileName
		ELSE
			lcFileName=IIF(ISNULL(this.oCatalog),"",this.oCatalog.cSourceCatalog)
		ENDIF
		lnMenuCount=0
		lnSelect=SELECT()
		SELECT browser
		IF EOF()
			LOCATE
		ENDIF
		lnRecNo=IIF(EOF() OR RECNO()>RECCOUNT(),0,RECNO())
		SCAN ALL FOR UPPER(ALLTRIM(Type))=="ADDIN" AND UPPER(ALLTRIM(ID))=="MENU" AND ;
				NOT EMPTY(Name) AND this.FileMatch(lcFileName,FileFilter)
			lnMenuCount=lnMenuCount+1
			DIMENSION laMenu[lnMenuCount,2]
			laMenu[lnMenuCount,1]=TRIM(Name)
			laMenu[lnMenuCount,2]="_screen.ActiveForm.DoAddIn("+ALLTRIM(STR(RECNO()))+")"
		ENDSCAN
		IF lnRecNo>0
			GO lnRecNo
		ELSE
			GO BOTTOM
			IF NOT EOF()
				SKIP
			ENDIF
		ENDIF
		SELECT (lnSelect)
		IF lnMenuCount=0
			SET MESSAGE TO
			this.MsgBox(M_THERE_NO_REG_ADDINS_LOC+[ "]+lcFileName+[".],0)
			RETURN .F.
		ENDIF
		ASORT(laMenu)
		SELECT 0
		SET MESSAGE TO
		this.ShowMenu(@laMenu)
		SELECT (lnSelect)
		SET MESSAGE TO
		RETURN
		
	ENDPROC

	PROCEDURE addinmethod
		LPARAMETERS tcObjMethod
		LOCAL lcAddInMethod,llHook,lnSelect,lnRecNo,lnAtPos,lcFileName,lnRecNo2
		LOCAL oAddInForm 
		
		IF this.lRelease OR this.lAddInMode OR EMPTY(tcObjMethod)
			RETURN .F.
		ENDIF
		SET DATASESSION TO this.DataSessionID
		IF NOT USED("browser")
			SET DATASESSION TO this.DataSessionID
			IF NOT USED("browser")
				RETURN .F.
			ENDIF
		ENDIF
		lcAddInMethod=UPPER(ALLTRIM(tcObjMethod))
		lnAtPos=AT(".",lcAddInMethod)
		IF lnAtPos>0
			lcAddInMethod=ALLTRIM(SUBSTR(lcAddInMethod,lnAtPos+1))
		ENDIF
		IF this.lAddInTrace
			ACTIVATE SCREEN
			? "("+this.Name+") Add-in method:  "+lcAddInMethod
		ENDIF
		lcFileName=JUSTFNAME(this.cFileName)
		this.cAddInMethod=lcAddInMethod
		this.lNoDefault=.F.
		this.lAddInMode=.T.
		llHook=.F.
		lnSelect=SELECT()
		SELECT browser
		IF EOF()
			LOCATE
		ENDIF
		lnRecNo=IIF(EOF() OR RECNO()>RECCOUNT(),0,RECNO())
		SCAN ALL FOR UPPER(ALLTRIM(Type))=="ADDIN" AND ;
				UPPER(ALLTRIM(ID))=="METHOD" AND ;
				(ALLTRIM(Method)=="*" OR UPPER(ALLTRIM(MLINE(Method,1)))==lcAddInMethod) AND ;
				this.FileMatch(lcFileName,FileFilter)
			lnRecNo2=RECNO()
			IF this.DoAddIn() AND this.lNoDefault
				llHook=.T.
			ENDIF
			SELECT browser
			GO lnRecNo2
		ENDSCAN
		this.cAddInMethod=""
		this.lNoDefault=.F.
		IF lnRecNo>0
			GO lnRecNo
		ELSE
			GO BOTTOM
			IF NOT EOF()
				SKIP
			ENDIF
		ENDIF
		SELECT (lnSelect)
		this.lAddInMode=.F.
		RETURN llHook
		
	ENDPROC


		ENDIF
		lcAlias="view_"+STRTRAN(LOWER(lcViewType)," ","_")
		IF NOT USED(lcAlias) AND this.ListIndex>1
			thisform.MsgBox([(]+lcViewType+[) ]+M_VIEW_LOC+[ ]+M_DOES_NOT_EXIST_LOC+[.],16)
			this.ListIndex=thisform.nViewType
			RETURN .F.
		ENDIF
		thisform.nViewType=this.ListIndex
		thisform.cGallery=lcAlias
		IF NOT thisform.lInitialized OR thisform.lRefreshBrowserMode OR NOT USED(lcAlias)
			RETURN
		ENDIF
		WITH thisform
			.lSwitchViewMode=.T.
			IF .nViewType>1
				.oView=.GetView(.cViewType)
			ENDIF
			.RefreshBrowser
			.lSwitchViewMode=.F.
			.oView=.GetView(.cViewType)
		ENDWITH
		
	ENDPROC


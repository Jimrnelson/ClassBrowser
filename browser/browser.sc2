		IF this.lRelease
			RETURN .F.
		ENDIF
		IF THIS.nlangopt=1
			_VFP.LanguageOptions=1
		ENDIF
		SYS(3054,INT(VAL(THIS.cSYS3054)))
		IF this.lModalDialog
			RETURN
		ENDIF
		IF TYPE("this.cboClassType")=="O" AND this.cboClassType.Enabled
			this.cboClassType.Enabled=.F.
			this.cboClassType.Enabled=.T.
		ENDIF
		this.SavePreferences
		SELECT 0
		ACTIVATE SCREEN
		SET MESSAGE TO
		IF this.lActive
			IF this.lWebBrowser AND this.lWebView
				this.oleWebBrowser.RefreshDeactivate
			ENDIF
			IF NOT EMPTY(this.cLastSetUDFParms)
				IF this.cLastSetUDFParms=="REFERENCE"
					SET UDFPARMS REFERENCE
				ELSE
					SET UDFPARMS VALUE
				ENDIF
			ENDIF
			IF NOT EMPTY(this.cLastSetComp)
				IF this.cLastSetComp=="ON"
					SET COMPATIBLE ON
				ELSE
					SET COMPATIBLE OFF
				ENDIF
			ENDIF
			IF NOT EMPTY(this.cLastOnKeyF5)
				lcLastOnKeyF5=this.cLastOnKeyF5
				ON KEY LABEL F5 &lcLastOnKeyF5
			ENDIF
			IF NOT EMPTY(this.cLastSetESC)
				IF this.cLastSetESC=="OFF"
					SET ESCAPE OFF
				ELSE
					SET ESCAPE ON
				ENDIF
			ENDIF
			_vfp.AutoYield=this.lLastAutoYield
		ENDIF
		this.lActive=.F.
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		
	ENDPROC


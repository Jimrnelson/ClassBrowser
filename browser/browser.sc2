	PROCEDURE Destroy
		LOCAL lcAlias,lcDBF,lnCount,oForm,lcLastOnKeyF5
		LOCAL laMembers[1]
		
		SET DATASESSION TO (this.DataSessionID)
		this.lRelease=.T.
		IF this.AddInMethod(PROGRAM())
			RETURN
		ENDIF
		this.SavePreferences(,.T.)
		IF VARTYPE(this.oViewCode)=="O"
			this.oViewCode.Release
			tihs.oViewCode=.NULL.
		ENDIF
		this.Visible=.F.
		SET MESSAGE TO M_RELEASING_LOC+" "+IIF(this.lBrowser,M_CLASS_BROWSER_LOC, ;
				M_COMPONENT_GALLERY_LOC)+" ..."
		IF TYPE("_screen._oCGClipBoard")=="O"
			this.ClearClipBoard
			_screen._oCGClipBoard.ReleaseObjects
			_screen.RemoveObject("_oCGClipBoard","_CGClipBoard","browser.vcx")
		ENDIF
		IF VARTYPE(this.oShortCutMenu)=="O"
			this.oShortCutMenu.Release
		ENDIF
		this.oShortCutMenu=.NULL.
		IF VARTYPE(this.oItemShortCutMenu)=="O"
			this.oItemShortCutMenu.Release
		ENDIF
		this.oItemShortCutMenu=.NULL.
		IF VARTYPE(this.oNullRecord)=="O"
			this.oNullRecord.Release
		ENDIF
		this.oNullRecord=.NULL.
		IF VARTYPE(this.oEdit)=="O"
			this.oEdit.Release
		ENDIF
		this.oEdit=.NULL.
		IF VARTYPE(this.oView)=="O"
			this.oView.Release
		ENDIF
		this.oView=.NULL.
		IF VARTYPE(this.oNodeObject)=="O"
			this.oNodeObject.Release
		ENDIF
		this.oNodeObject=.NULL.
		FOR lnCount = 1 TO AMEMBERS(laMembers,this.oNodes,2)
			this.oNodes.RemoveObject(laMembers[lnCount])
		ENDFOR
		this.oNodes=.NULL.
		this.UpdateFormCount(-1,"")
		this.ClearProperties
		this.CloseViews(.T.)
		FOR lnCount = 1 TO 1024
			IF NOT USED(lnCount)
				LOOP
			ENDIF
			lcAlias=LOWER(ALIAS(lnCount))
			IF lcAlias=="browser"
				LOOP
			ENDIF
			IF LOWER(LEFT(lcAlias,7))=="catalog"
				lcDBF=LOWER(DBF(lcAlias))
				IF JUSTEXT(lcDBF)=="dbf"
					this.UpdateFormCount(-1,lcDBF)
				ENDIF
			ENDIF
			USE IN (lnCount)
		ENDFOR
		IF USED("browser")
			USE IN browser
		ENDIF
		SELECT 0
		RELEASE _oBrowser
		PUBLIC _oBrowser
		_oBrowser=.NULL.
		FOR lnCount = _screen.FormCount TO 1 STEP -1
			oForm=_screen.Forms[lnCount]
			IF NOT oForm.Name==this.Name AND oForm.Class==this.Class AND ;
					LOWER(LEFT(oForm.Name,12))=="classbrowser" AND ;
					TYPE("oForm.cBrowserTable")#"U"
				_oBrowser=oForm
				EXIT
			ENDIF
		ENDFOR
		oForm=.NULL.
		this.vResult=.T.
		DO brwReleaseClassLibraries IN browser WITH (this)
		this.vResult=.T.
		IF NOT EMPTY(this.cLastSetUDFParms)
			IF this.cLastSetUDFParms=="REFERENCE"
				SET UDFPARMS REFERENCE
			ELSE
				SET UDFPARMS VALUE
			ENDIF
		ENDIF
		IF NOT EMPTY(this.cLastSetComp)
			IF this.cLastSetComp=="ON"
				SET COMPATIBLE ON
			ELSE
				SET COMPATIBLE OFF
			ENDIF
		ENDIF
		IF NOT EMPTY(this.cLastOnKeyF5)
			lcLastOnKeyF5=this.cLastOnKeyF5
			ON KEY LABEL F5 &lcLastOnKeyF5
		ENDIF
		IF NOT EMPTY(this.cLastSetESC)
			IF this.cLastSetESC=="OFF"
				SET ESCAPE OFF
			ELSE
				SET ESCAPE ON
			ENDIF
		ENDIF
		_vfp.AutoYield=this.lLastAutoYield
		
	ENDPROC

